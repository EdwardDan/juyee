<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 6.2.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_6_2_0.dtd">
<service-builder package-path="com.justonetech.cp.contract">
	<author>fanqi</author>
	<namespace>cp</namespace>

	<entity name="Contract" local-service="true" remote-service="false">

		<!-- PK fields -->

		<column name="id" type="long" primary="true" />

		<!-- Group instance -->

		<column name="groupId" type="long" />

		<!-- Audit fields -->

		<column name="companyId" type="long" />
		<column name="userId" type="long" />
		<column name="userName" type="String" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />

		<!-- business fields -->
		<column name="xmqHtId" type="long" /><!-- 小木桥合同ID -->
		<column name="bjbh" type="String" /><!-- 报建编号 -->
		<column name="zbjg" type="Double" /><!-- 中标价格（万元） -->
		<column name="htgq" type="Double" /><!-- 合同工期（日历天 -->

		<!-- Order -->
		<order by="asc">
			<order-column name="bjbh" />
		</order>

		<!-- Finder methods -->
		<finder name="Bjbh" return-type="Collection">
			<finder-column name="bjbh" />
		</finder>
	</entity>
</service-builder>