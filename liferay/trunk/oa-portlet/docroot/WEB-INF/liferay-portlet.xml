<?xml version="1.0"?>
<!DOCTYPE liferay-portlet-app PUBLIC "-//Liferay//DTD Portlet Application 6.2.0//EN" "http://www.liferay.com/dtd/liferay-portlet-app_6_2_0.dtd">

<liferay-portlet-app>
	<portlet>
		<portlet-name>office-supply</portlet-name>
		<icon>/icons/icon.png</icon>
		<header-portlet-css>/css/main.css</header-portlet-css>
		<footer-portlet-javascript>
			/js/main.js
		</footer-portlet-javascript>
		<css-class-wrapper>office-supply-portlet</css-class-wrapper>
	</portlet>
	<portlet>
		<portlet-name>leader-weekly-work</portlet-name>
		<icon>/icons/icon.png</icon>
		<header-portlet-css>/css/main.css</header-portlet-css>
		<footer-portlet-javascript>
			/js/main.js
		</footer-portlet-javascript>
		<css-class-wrapper>
			leader-weekly-work-portlet
		</css-class-wrapper>
	</portlet>
	<portlet>
		<portlet-name>dept-weekly-work</portlet-name>
		<icon>/icons/icon.png</icon>
		<header-portlet-css>/css/main.css</header-portlet-css>
		<footer-portlet-javascript>
			/js/main.js
		</footer-portlet-javascript>
		<css-class-wrapper>dept-weekly-work-portlet</css-class-wrapper>
	</portlet>
	<portlet>
		<portlet-name>send-dispatch</portlet-name>
		<icon>/icons/icon.png</icon>
