		<footer-portlet-javascript>
			/js/main.js
		</footer-portlet-javascript>
		<css-class-wrapper>send-dispatch-portlet</css-class-wrapper>
	</portlet>
	<portlet>
		<portlet-name>vehicle-application</portlet-name>
		<icon>/icons/icon.png</icon>
		<user-notification-definitions>warn-notification-definitions.xml</user-notification-definitions>
        <user-notification-handler-class>com.justonetech.oa.notification.VehicleApplicationNotificationHandler</user-notification-handler-class>
		<asset-renderer-factory>com.justonetech.oa.asset.VehicleApplicationAssetRenderFactory</asset-renderer-factory>
	    <workflow-handler>com.justonetech.oa.workflow.VehicleApplicationWorkflowHandler</workflow-handler>
		<header-portlet-css>/css/main.css</header-portlet-css>
		<footer-portlet-javascript>
			/js/main.js
		</footer-portlet-javascript>
		<css-class-wrapper>
			vehicle-application-portlet
		</css-class-wrapper>
	</portlet>
	<portlet>
		<portlet-name>office-supply-application</portlet-name>
		<icon>/icon.png</icon>
		<user-notification-definitions>warn-notification-definitions.xml</user-notification-definitions>
        <user-notification-handler-class>com.justonetech.oa.notification.OfficeSupplyApplicationNotificationHandler</user-notification-handler-class>
		<asset-renderer-factory>com.justonetech.oa.asset.OfficeSupplyApplicationAssetRenderFactory</asset-renderer-factory>
	    <workflow-handler>com.justonetech.oa.workflow.OfficeSupplyApplicationWorkflowHandler</workflow-handler>
		<header-portlet-css>/css/main.css</header-portlet-css>
		<footer-portlet-javascript>
			/js/main.js
		</footer-portlet-javascript>
		<css-class-wrapper>
			office-supply-application-portlet
		</css-class-wrapper>
	</portlet>
	<portlet>
		<portlet-name>new</portlet-name>
		<icon>/icon.png</icon>
		<header-portlet-css>/css/main.css</header-portlet-css>
		<footer-portlet-javascript>
			/js/main.js
		</footer-portlet-javascript>
		<css-class-wrapper>new-portlet</css-class-wrapper>
	</portlet>
	<role-mapper>
		<role-name>administrator</role-name>
		<role-link>Administrator</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>guest</role-name>
		<role-link>Guest</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>power-user</role-name>
		<role-link>Power User</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>user</role-name>
		<role-link>User</role-link>
	</role-mapper>
