<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
	<class
		name="ProjBid"
		table="PROJ_BID"
	>
		
		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">false</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">项目标段管理</meta>
        <meta attribute="tableComment">项目标段管理</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">project</meta>

		<id
			name="id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
		</id>

		<property
			name="code"
			column="CODE"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">标段编号</meta>
            <meta attribute="ColumnComment">标段编号</meta>
        </property>

		<property
			name="name"
			column="NAME"
			type="string"
			not-null="false"
			length="500"
        >
            <meta attribute="ColumnName">标段名称</meta>
            <meta attribute="ColumnComment">标段名称</meta>
        </property>

        <property
                name="typeCode"
                column="TYPE_CODE"
                type="string"
                not-null="false"
                length="50"
                >
            <meta attribute="ColumnName">标段类别编码(形象进度/办证推进)</meta>
            <meta attribute="ColumnComment">标段类别编码(形象进度/办证推进)</meta>
        </property>

		<property
			name="buildMileage"
			column="BUILD_MILEAGE"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">建设里程</meta>
            <meta attribute="ColumnComment">建设里程</meta>
        </property>

		<property
			name="projLink"
			column="PROJ_LINK"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">项目联系人</meta>
            <meta attribute="ColumnComment">项目联系人</meta>
        </property>

		<property
			name="linkTel"
			column="LINK_TEL"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">联系电话</meta>
            <meta attribute="ColumnComment">联系电话</meta>
        </property>

		<property
			name="startDate"
			column="START_DATE"
			type="date"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">开工日期</meta>
            <meta attribute="ColumnComment">开工日期</meta>
        </property>

        <property
                name="createTime"
                column="CREATE_TIME"
                type="timestamp"
                not-null="false"
                length="16"
                >
            <meta attribute="ColumnName">创建时间</meta>
            <meta attribute="ColumnComment">创建时间</meta>
        </property>

        <property
                name="updateTime"
                column="UPDATE_TIME"
                type="timestamp"
                not-null="false"
                length="16"
                >
            <meta attribute="ColumnName">更新时间</meta>
            <meta attribute="ColumnComment">更新时间</meta>
        </property>

        <property
                name="createUser"
                column="CREATE_USER"
                type="string"
                not-null="false"
                length="100"
                >
            <meta attribute="ColumnName">创建人</meta>
            <meta attribute="ColumnComment">创建人(记录帐号）</meta>
        </property>

        <property
                name="updateUser"
                column="UPDATE_USER"
                type="string"
                not-null="false"
                length="100"
                >
            <meta attribute="ColumnName">更新人</meta>
            <meta attribute="ColumnComment">更新人(记录帐号）</meta>
        </property>

		<many-to-one
			name="project"
			column="PROJECT_ID"
			class="ProjInfo"
			not-null="false"
		>
		</many-to-one>

        <set name="dataNodeReports" inverse="true">
            <key column="BID_ID"/>
            <one-to-many class="DataNodeReport"/>
        </set>
        <set name="dataStageReports" inverse="true">
            <key column="BID_ID"/>
            <one-to-many class="DataStageReport"/>
        </set>
        <set name="projBidAreas" inverse="true" order-by="id asc">
            <key column="BID_ID"/>
            <one-to-many class="ProjBidArea"/>
        </set>
	</class>	
    <query name="projbid_all_data_query">
        <![CDATA[FROM ProjBid]]>
    </query>
</hibernate-mapping>