<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
	<class
		name="AreaAuditOpinion"
		table="AREA_AUDIT_OPINION"
	>

		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">false</meta>
		<meta attribute="projectable">false</meta>
		<meta attribute="tableName">区县施工审核意见</meta>
		<meta attribute="tableComment">区县施工审核意见</meta>
		<meta attribute="sync-templatePatterns">list\w*</meta>
		<meta attribute="sub-system">area</meta>

		<id
			name="id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
		</id>

		<property
				name="no"
				column="NO"
				type="java.lang.Long"
				not-null="false"
				length="10"
				>
			<meta attribute="ColumnName">序号</meta>
			<meta attribute="ColumnComment">序号</meta>
		</property>

		<property
				name="isCsOpinion"
				column="IS_CS_OPINION"
				type="java.lang.Boolean"
				not-null="false"
				length="1"
				>
			<meta attribute="ColumnName">初审意见</meta>
			<meta attribute="ColumnComment">初审意见</meta>
		</property>

		<property
				name="isFhOpinion"
				column="IS_FH_OPINION"
				type="java.lang.Boolean"
				not-null="false"
				length="1"
				>
			<meta attribute="ColumnName">复核意见</meta>
			<meta attribute="ColumnComment">复核意见</meta>
		</property>

		<property
				name="isShOpinion"
				column="IS_SH_OPINION"
				type="java.lang.Boolean"
				not-null="false"
				length="1"
				>
			<meta attribute="ColumnName">审核意见</meta>
			<meta attribute="ColumnComment">审核意见</meta>
		</property>

		<property
				name="status"
				column="STATUS"
				type="integer"
				not-null="false"
				length="2"
				>
			<meta attribute="ColumnName">状态</meta>
			<meta attribute="ColumnComment">状态</meta>
		</property>

		<property
				name="createTime"
				column="CREATE_TIME"
				type="timestamp"
				not-null="false"
				length="7"
				>
			<meta attribute="ColumnName">创建时间</meta>
			<meta attribute="ColumnComment">创建时间</meta>
		</property>

		<property
				name="createUser"
				column="CREATE_USER"
				type="string"
				not-null="false"
				length="100"
				>
			<meta attribute="ColumnName">创建用户名</meta>
			<meta attribute="ColumnComment">创建用户名</meta>
		</property>

		<property
				name="updateTime"
				column="UPDATE_TIME"
				type="timestamp"
				not-null="false"
				length="7"
				>
			<meta attribute="ColumnName">更新时间</meta>
			<meta attribute="ColumnComment">更新时间</meta>
		</property>

		<property
				name="updateUser"
				column="UPDATE_USER"
				type="string"
				not-null="false"
				length="100"
				>
			<meta attribute="ColumnName">更新用户名</meta>
			<meta attribute="ColumnComment">更新用户名</meta>
		</property>

		<many-to-one
			name="sgPermit"
			column="SG_PERMIT_ID"
			class="AreaSgPermit"
			not-null="false"
		>
		</many-to-one>

	</class>
    <query name="areaauditopinion_all_data_query">
        <![CDATA[FROM AreaAuditOpinion]]>
    </query>
</hibernate-mapping>
