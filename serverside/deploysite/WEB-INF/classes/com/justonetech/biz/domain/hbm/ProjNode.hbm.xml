<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
	<class
		name="ProjNode"
		table="PROJ_NODE"
	>
		
		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">true</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">项目形象进度节点管理</meta>
        <meta attribute="tableComment">项目形象进度节点管理</meta>
            <meta attribute="sync-templatePatterns">tree\w*</meta>
        <meta attribute="sub-system">project</meta>

		<id
			name="id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
		</id>

        <property
                name="year"
                column="YEAR"
                type="integer"
                not-null="false"
                length="4"
                >
            <meta attribute="ColumnName">年份</meta>
            <meta attribute="ColumnComment">年份</meta>
        </property>

		<property
			name="code"
			column="CODE"
			type="string"
			not-null="false"
			length="200"
        >
            <meta attribute="ColumnName">编码</meta>
            <meta attribute="ColumnComment">编码</meta>
        </property>

		<property
			name="name"
			column="NAME"
			type="string"
			not-null="false"
			length="200"
        >
            <meta attribute="ColumnName">名称</meta>
            <meta attribute="ColumnComment">名称</meta>
        </property>

		<property
			name="isValid"
			column="IS_VALID"
			type="java.lang.Boolean"
			not-null="false"
			length="1"
        >
            <meta attribute="ColumnName">是否有效</meta>
            <meta attribute="ColumnComment">是否有效</meta>
        </property>

		<property
			name="description"
			column="DESCRIPTION"
			type="string"
			not-null="false"
        >
            <meta attribute="ColumnName">备注</meta>
            <meta attribute="ColumnComment">备注</meta>
        </property>

		<property
			name="isLeaf"
			column="IS_LEAF"
			type="java.lang.Boolean"
			not-null="false"
			length="1"
        >
            <meta attribute="ColumnName">是否叶节点</meta>
            <meta attribute="ColumnComment">是否叶节点</meta>
        </property>

		<property
			name="treeId"
			column="TREE_ID"
			type="string"
			not-null="false"
			length="300"
        >
            <meta attribute="ColumnName">树形层次</meta>
            <meta attribute="ColumnComment">树形层次</meta>
        </property>

        <property
                name="createTime"
                column="CREATE_TIME"
                type="timestamp"
                not-null="false"
                length="16"
                >
            <meta attribute="ColumnName">创建时间</meta>
            <meta attribute="ColumnComment">创建时间</meta>
        </property>

        <property
                name="updateTime"
                column="UPDATE_TIME"
                type="timestamp"
                not-null="false"
                length="16"
                >
            <meta attribute="ColumnName">更新时间</meta>
            <meta attribute="ColumnComment">更新时间</meta>
        </property>

        <property
                name="createUser"
                column="CREATE_USER"
                type="string"
                not-null="false"
                length="100"
                >
            <meta attribute="ColumnName">创建人</meta>
            <meta attribute="ColumnComment">创建人(记录帐号）</meta>
        </property>

        <property
                name="updateUser"
                column="UPDATE_USER"
                type="string"
                not-null="false"
                length="100"
                >
            <meta attribute="ColumnName">更新人</meta>
            <meta attribute="ColumnComment">更新人(记录帐号）</meta>
        </property>

		<many-to-one
			name="parent"
			column="PARENT_ID"
			class="ProjNode"
			not-null="false"
		>
		</many-to-one>
	

		<set name="projNodes" inverse="true">
			<key column="PARENT_ID"/>
			<one-to-many class="ProjNode"/>
		</set>
        <set name="dataNodeReportItems" inverse="true">
            <key column="NODE_ID"/>
            <one-to-many class="DataNodeReportItem"/>
        </set>

	</class>	
    <query name="projnode_all_data_query">
        <![CDATA[FROM ProjNode]]>
    </query>
</hibernate-mapping>