<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.system.domain">
	<class
		name="SysDept"
		table="SYS_DEPT"
	>
		
		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">true</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">单位部门</meta>
        <meta attribute="tableComment">单位部门</meta>
            <meta attribute="sync-templatePatterns">tree\w*</meta>
        <meta attribute="sub-system">system</meta>

		<id
			name="id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
		</id>

		<property
			name="code"
			column="CODE"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">单位代码</meta>
            <meta attribute="ColumnComment">单位代码</meta>
        </property>

		<property
			name="name"
			column="NAME"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">单位名称</meta>
            <meta attribute="ColumnComment">单位名称</meta>
        </property>

		<property
			name="shortName"
			column="SHORT_NAME"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">简称</meta>
            <meta attribute="ColumnComment">简称</meta>
        </property>

		<property
			name="isLeaf"
			column="IS_LEAF"
			type="java.lang.Boolean"
			not-null="false"
			length="1"
        >
            <meta attribute="ColumnName">叶节点</meta>
            <meta attribute="ColumnComment">叶节点</meta>
        </property>

		<property
			name="treeId"
			column="TREE_ID"
			type="string"
			not-null="false"
			length="200"
        >
            <meta attribute="ColumnName">树形层次</meta>
            <meta attribute="ColumnComment">树形层次</meta>
        </property>

		<property
			name="cardNo"
			column="CARD_NO"
			type="string"
			not-null="false"
			length="40"
        >
            <meta attribute="ColumnName">机构代码证</meta>
            <meta attribute="ColumnComment">机构代码证</meta>
        </property>

		<property
			name="cityCode"
			column="CITY_CODE"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">注册地编码</meta>
            <meta attribute="ColumnComment">注册地编码</meta>
        </property>

		<property
			name="cityName"
			column="CITY_NAME"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">注册地名称</meta>
            <meta attribute="ColumnComment">注册地名称</meta>
        </property>

		<property
			name="provinceCode"
			column="PROVINCE_CODE"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">隶属省市代码</meta>
            <meta attribute="ColumnComment">隶属省市代码</meta>
        </property>

		<property
			name="provinceName"
			column="PROVINCE_NAME"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">隶属省市名称</meta>
            <meta attribute="ColumnComment">隶属省市名称</meta>
        </property>

		<property
			name="address"
			column="ADDRESS"
			type="string"
			not-null="false"
			length="200"
        >
            <meta attribute="ColumnName">注册地址</meta>
            <meta attribute="ColumnComment">注册地址</meta>
        </property>

		<property
			name="orderNo"
			column="ORDER_NO"
			type="java.lang.Long"
			not-null="false"
			length="10"
        >
            <meta attribute="ColumnName">排序</meta>
            <meta attribute="ColumnComment">排序</meta>
        </property>

		<property
			name="isTag"
			column="IS_TAG"
			type="java.lang.Boolean"
			not-null="false"
			length="1"
        >
            <meta attribute="ColumnName">单位标志</meta>
            <meta attribute="ColumnComment">单位标志  0：单位  1：部门</meta>
        </property>

		<property
			name="memo"
			column="MEMO"
			type="string"
			not-null="false"
			length="500"
        >
            <meta attribute="ColumnName">备注</meta>
            <meta attribute="ColumnComment">备注</meta>
        </property>

		<property
			name="createTime"
			column="CREATE_TIME"
			type="timestamp"
			not-null="false"
			length="16"
        >
            <meta attribute="ColumnName">创建时间</meta>
            <meta attribute="ColumnComment">创建时间</meta>
        </property>

		<property
			name="updateTime"
			column="UPDATE_TIME"
			type="timestamp"
			not-null="false"
			length="16"
        >
            <meta attribute="ColumnName">更新时间</meta>
            <meta attribute="ColumnComment">更新时间</meta>
        </property>

		<property
			name="createUser"
			column="CREATE_USER"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">创建人</meta>
            <meta attribute="ColumnComment">创建人(记录帐号）</meta>
        </property>

		<property
			name="updateUser"
			column="UPDATE_USER"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">更新人</meta>
            <meta attribute="ColumnComment">更新人(记录帐号）</meta>
        </property>

        <property
			name="isValid"
			column="IS_VALID"
			type="java.lang.Boolean"
			not-null="false"
			length="1"
        >
            <meta attribute="ColumnName">是否有效</meta>
            <meta attribute="ColumnComment">是否有效</meta>
        </property>

		<many-to-one
			name="parent"
			column="PARENT_ID"
			class="SysDept"
			not-null="false"
		>
		</many-to-one>

        <set name="children" inverse="true">
			<key column="PARENT_ID"/>
			<one-to-many class="SysDept"/>
		</set>

        <set name="sysPersonDepts" inverse="true">
			<key column="DEPT_ID"/>
			<one-to-many class="SysPersonDept"/>
		</set>

<!--

		<set name="sysDeptUsers" inverse="true">
			<key column="DEPT_ID"/>
			<one-to-many class="SysDeptUser"/>
		</set>

-->
<!--



-->
<!--

		<set name="projectWorkSites" inverse="true">
			<key column="DEPT_ID"/>
			<one-to-many class="ProjectWorkSite"/>
		</set>

-->

	</class>	
    <query name="sysdept_all_data_query">
        <![CDATA[FROM SysDept]]>
    </query>
</hibernate-mapping>