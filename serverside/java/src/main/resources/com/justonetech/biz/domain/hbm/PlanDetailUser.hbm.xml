<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
	<class
		name="PlanDetailUser"
		table="PLAN_DETAIL_USER"
	>
		
		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">false</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">监督计划负责人</meta>
        <meta attribute="tableComment">监督计划负责人</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">plan</meta>

		<id
			name="id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
		</id>

		<property
			name="isManager"
			column="IS_MANAGER"
			type="java.lang.Boolean"
			not-null="false"
			length="1"
        >
            <meta attribute="ColumnName">是否负责人</meta>
            <meta attribute="ColumnComment">是否负责人</meta>
        </property>

		<many-to-one
			name="user"
			column="USER_ID"
			class="com.justonetech.system.domain.SysUser"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="planDetail"
			column="PLAN_DETAIL_ID"
			class="PlanDetail"
			not-null="false"
		>
		</many-to-one>

	</class>	
    <query name="plandetailuser_all_data_query">
        <![CDATA[FROM PlanDetailUser]]>
    </query>
</hibernate-mapping>