<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
	<class
		name="WebUnitPersonInfo"
		table="WEB_UNIT_PERSON_INFO"
	>
		
		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">false</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">企业人员信息</meta>
        <meta attribute="tableComment">企业人员信息</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">web</meta>

		<id
			name="id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
		</id>

		<property
			name="personCategor"
			column="PERSON_CATEGOR"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">人员类型</meta>
            <meta attribute="ColumnComment">人员类型</meta>
        </property>

		<property
			name="name"
			column="NAME"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">姓名</meta>
            <meta attribute="ColumnComment">姓名</meta>
        </property>

		<property
			name="position"
			column="POSITION"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">职务</meta>
            <meta attribute="ColumnComment">职务</meta>
        </property>

		<property
			name="title"
			column="TITLE"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">职称</meta>
            <meta attribute="ColumnComment">职称</meta>
        </property>

		<property
			name="major"
			column="MAJOR"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">所学专业</meta>
            <meta attribute="ColumnComment">所学专业</meta>
        </property>

		<property
			name="zcType"
			column="ZC_TYPE"
			type="string"
			not-null="false"
			length="500"
        >
            <meta attribute="ColumnName">注册类型</meta>
            <meta attribute="ColumnComment">注册类型</meta>
        </property>

		<many-to-one
			name="qualifyAcceptInfo"
			column="QUALIFY_ACCEPT_INFO_ID"
			class="WebQualifyAcceptInfo"
			not-null="false"
		>
		</many-to-one>
	

		<set name="webPersonPerformances" inverse="true">
			<key column="UNIT_PERSON_INFO_ID"/>
			<one-to-many class="WebPersonPerformance"/>
		</set>

	</class>	
    <query name="webunitpersoninfo_all_data_query">
        <![CDATA[FROM WebUnitPersonInfo]]>
    </query>
</hibernate-mapping>