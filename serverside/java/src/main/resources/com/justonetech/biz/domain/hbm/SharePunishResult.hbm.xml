<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
	<class
		name="SharePunishResult"
		table="SHARE_PUNISH_RESULT"
	>
		
		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">false</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">行政处罚结果</meta>
        <meta attribute="tableComment">行政处罚结果</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">share</meta>

		<id
			name="id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
		</id>

		<property
			name="punishCaseId"
			column="PUNISH_CASE_ID"
			type="java.lang.Long"
			not-null="false"
			length="10"
        >
            <meta attribute="ColumnName">案件标识</meta>
            <meta attribute="ColumnComment">案件标识</meta>
        </property>

		<property
			name="punishType"
			column="PUNISH_TYPE"
			type="java.lang.Long"
			not-null="false"
			length="10"
        >
            <meta attribute="ColumnName">处罚类型</meta>
            <meta attribute="ColumnComment">处罚类型</meta>
        </property>

		<property
			name="punishTypeDesc"
			column="PUNISH_TYPE_DESC"
			type="string"
			not-null="false"
			length="200"
        >
            <meta attribute="ColumnName">处罚类型描述</meta>
            <meta attribute="ColumnComment">处罚类型描述</meta>
        </property>

		<property
			name="relateMoney"
			column="RELATE_MONEY"
			type="java.lang.Double"
			not-null="false"
			length="15"
        >
            <meta attribute="ColumnName">涉及金额（元）</meta>
            <meta attribute="ColumnComment">涉及金额（元）</meta>
        </property>

		<property
			name="relateMoneyChinese"
			column="RELATE_MONEY_CHINESE"
			type="string"
			not-null="false"
			length="400"
        >
            <meta attribute="ColumnName">涉及金额（大写）</meta>
            <meta attribute="ColumnComment">涉及金额（大写）</meta>
        </property>

		<property
			name="relateDays"
			column="RELATE_DAYS"
			type="java.lang.Long"
			not-null="false"
			length="10"
        >
            <meta attribute="ColumnName">涉及时间（天）</meta>
            <meta attribute="ColumnComment">涉及时间（天）</meta>
        </property>

		<property
			name="memo"
			column="MEMO"
			type="string"
			not-null="false"
			length="1000"
        >
            <meta attribute="ColumnName">其他说明</meta>
            <meta attribute="ColumnComment">其他说明</meta>
        </property>


	</class>	
    <query name="sharepunishresult_all_data_query">
        <![CDATA[FROM SharePunishResult]]>
    </query>
</hibernate-mapping>