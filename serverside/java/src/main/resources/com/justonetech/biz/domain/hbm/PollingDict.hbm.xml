<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
	<class
		name="PollingDict"
		table="POLLING_DICT"
	>

		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">true</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">巡查字典</meta>
        <meta attribute="tableComment">巡查字典</meta>
            <meta attribute="sync-templatePatterns">tree\w*</meta>
        <meta attribute="sub-system">polling</meta>

		<id
			name="id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
		</id>

		<property
			name="refRule"
			column="REF_RULE"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">法规依据</meta>
            <meta attribute="ColumnComment">法规依据</meta>
        </property>

		<property
			name="name"
			column="NAME"
			type="string"
			not-null="false"
			length="200"
        >
            <meta attribute="ColumnName">名称</meta>
            <meta attribute="ColumnComment">名称</meta>
        </property>

		<property
			name="isLeaf"
			column="IS_LEAF"
			type="java.lang.Boolean"
			not-null="false"
			length="1"
        >
            <meta attribute="ColumnName">叶节点</meta>
            <meta attribute="ColumnComment">叶节点</meta>
        </property>

		<property
			name="treeId"
			column="TREE_ID"
			type="string"
			not-null="false"
			length="200"
        >
            <meta attribute="ColumnName">树形层次</meta>
            <meta attribute="ColumnComment">树形层次</meta>
        </property>

		<property
			name="isValid"
			column="IS_VALID"
			type="java.lang.Boolean"
			not-null="false"
			length="1"
        >
            <meta attribute="ColumnName">是否有效</meta>
            <meta attribute="ColumnComment">是否有效</meta>
        </property>

		<property
			name="orderNo"
			column="ORDER_NO"
			type="java.lang.Long"
			not-null="false"
			length="10"
        >
            <meta attribute="ColumnName">排序号</meta>
            <meta attribute="ColumnComment">排序号</meta>
        </property>

		<property
			name="description"
			column="DESCRIPTION"
			type="string"
			not-null="false"
			length="200"
        >
            <meta attribute="ColumnName">备注</meta>
            <meta attribute="ColumnComment">备注</meta>
        </property>

		<property
			name="createTime"
			column="CREATE_TIME"
			type="timestamp"
			not-null="false"
			length="16"
        >
            <meta attribute="ColumnName">创建时间</meta>
            <meta attribute="ColumnComment">创建时间</meta>
        </property>

		<property
			name="updateTime"
			column="UPDATE_TIME"
			type="timestamp"
			not-null="false"
			length="16"
        >
            <meta attribute="ColumnName">更新时间</meta>
            <meta attribute="ColumnComment">更新时间</meta>
        </property>

		<property
			name="updateUser"
			column="UPDATE_USER"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">更新人</meta>
            <meta attribute="ColumnComment">更新人</meta>
        </property>

		<property
			name="createUser"
			column="CREATE_USER"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">创建人</meta>
            <meta attribute="ColumnComment">创建人</meta>
        </property>

		<many-to-one
			name="reformType"
			column="REFORM_TYPE"
			class="com.justonetech.system.domain.SysCodeDetail"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="registerType"
			column="REGISTER_TYPE"
			class="com.justonetech.system.domain.SysCodeDetail"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="safeType"
			column="SAFE_TYPE"
			class="com.justonetech.system.domain.SysCodeDetail"
			not-null="false"
		>
		</many-to-one>

        <many-to-one
			name="parent"
			column="PARENT_ID"
			class="PollingDict"
			not-null="false"
		>
		</many-to-one>

        <set name="pollingDicts" inverse="true">
			<key column="PARENT_ID"/>
			<one-to-many class="PollingDict"/>
		</set>

	</class>
    <query name="pollingdict_all_data_query">
        <![CDATA[FROM PollingDict]]>
    </query>
</hibernate-mapping>