<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
	<class
		name="ReportStructureVer"
		table="REPORT_STRUCTURE_VER"
	>
		
		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">false</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">综合报表结构版本</meta>
        <meta attribute="tableComment">综合报表结构版本</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">report</meta>

		<id
			name="id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
		</id>

		<property
			name="name"
			column="NAME"
			type="string"
			not-null="false"
			length="200"
        >
            <meta attribute="ColumnName">版本名称</meta>
            <meta attribute="ColumnComment">名称</meta>
        </property>

		<property
			name="isValid"
			column="IS_VALID"
			type="java.lang.Boolean"
			not-null="false"
			length="1"
        >
            <meta attribute="ColumnName">是否有效</meta>
            <meta attribute="ColumnComment">是否有效</meta>
        </property>

		<property
			name="createTime"
			column="CREATE_TIME"
			type="timestamp"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">创建时间</meta>
            <meta attribute="ColumnComment">创建时间</meta>
        </property>

		<property
			name="createUser"
			column="CREATE_USER"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">创建用户名</meta>
            <meta attribute="ColumnComment">创建用户名</meta>
        </property>

		<property
			name="updateTime"
			column="UPDATE_TIME"
			type="timestamp"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">更新时间</meta>
            <meta attribute="ColumnComment">更新时间</meta>
        </property>

		<property
			name="updateUser"
			column="UPDATE_USER"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">更新用户名</meta>
            <meta attribute="ColumnComment">更新用户名</meta>
        </property>

	

		<set name="reportStructures" inverse="true">
			<key column="VERSION_ID"/>
			<one-to-many class="ReportStructure"/>
		</set>

		<set name="reportMonths" inverse="true">
			<key column="VERSION_ID"/>
			<one-to-many class="ReportMonth"/>
		</set>

	</class>	
    <query name="reportstructurever_all_data_query">
        <![CDATA[FROM ReportStructureVer]]>
    </query>
</hibernate-mapping>