<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
	<class
		name="JdStopOrderQuestion"
		table="JD_STOP_ORDER_QUESTION"
	>

		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">false</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">安全质量监督--整改指令单选择问题</meta>
        <meta attribute="tableComment">安全质量监督--整改指令单选择问题</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">jd</meta>

		<id
			name="id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
		</id>

		<many-to-one
			name="recordQuestion"
			column="RECORD_QUESTION_ID"
			class="JdRecordQuestion"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="reformOrder"
			column="REFORM_ORDER_ID"
			class="JdStopOrder"
			not-null="false"
		>
		</many-to-one>

	</class>
    <query name="jdstoporderquestion_all_data_query">
        <![CDATA[FROM JdStopOrderQuestion]]>
    </query>
</hibernate-mapping>