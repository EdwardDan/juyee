<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
	<class
		name="OaMaterial"
		table="OA_MATERIAL"
	>
		
		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">false</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">物资设备采购管理</meta>
        <meta attribute="tableComment">物资设备采购管理</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">oa</meta>

		<id
			name="id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
		</id>

		<property
			name="code"
			column="CODE"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">申请编号</meta>
            <meta attribute="ColumnComment">申请编号</meta>
        </property>

		<property
			name="reportDate"
			column="REPORT_DATE"
			type="date"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">申请日期</meta>
            <meta attribute="ColumnComment">申请日期</meta>
        </property>

		<property
			name="reportPerson"
			column="REPORT_PERSON"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">申请人</meta>
            <meta attribute="ColumnComment">申请人</meta>
        </property>

		<property
			name="reportDept"
			column="REPORT_DEPT"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">申请部门</meta>
            <meta attribute="ColumnComment">申请部门</meta>
        </property>

		<property
			name="requireDesc"
			column="REQUIRE_DESC"
			type="string"
			not-null="false"
        >
            <meta attribute="ColumnName">备注要求</meta>
            <meta attribute="ColumnComment">备注要求</meta>
        </property>

		<property
			name="documentId"
			column="DOCUMENT_ID"
			type="java.lang.Long"
			not-null="false"
			length="10"
        >
            <meta attribute="ColumnName">附件ID</meta>
            <meta attribute="ColumnComment">附件ID</meta>
        </property>

		<property
			name="status"
			column="STATUS"
			type="integer"
			not-null="false"
			length="2"
        >
            <meta attribute="ColumnName">状态</meta>
            <meta attribute="ColumnComment">状态</meta>
        </property>

		<property
			name="officeOpinion"
			column="OFFICE_OPINION"
			type="string"
			not-null="false"
        >
            <meta attribute="ColumnName">办公室审核意见</meta>
            <meta attribute="ColumnComment">办公室审核意见</meta>
        </property>

		<property
			name="officeAuditTime"
			column="OFFICE_AUDIT_TIME"
			type="timestamp"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">办公室审核时间</meta>
            <meta attribute="ColumnComment">办公室审核时间</meta>
        </property>

		<property
			name="officeAuditUser"
			column="OFFICE_AUDIT_USER"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">办公室审核用户名</meta>
            <meta attribute="ColumnComment">办公室审核用户名</meta>
        </property>

		<property
			name="leaderOpinion"
			column="LEADER_OPINION"
			type="string"
			not-null="false"
        >
            <meta attribute="ColumnName">主管领导审核意见</meta>
            <meta attribute="ColumnComment">主管领导审核意见</meta>
        </property>

		<property
			name="leaderAuditTime"
			column="LEADER_AUDIT_TIME"
			type="timestamp"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">主管领导审核时间</meta>
            <meta attribute="ColumnComment">主管领导审核时间</meta>
        </property>

		<property
			name="leaderAuditUser"
			column="LEADER_AUDIT_USER"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">主管领导审核用户名</meta>
            <meta attribute="ColumnComment">主管领导审核用户名</meta>
        </property>

		<property
			name="createTime"
			column="CREATE_TIME"
			type="timestamp"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">创建时间</meta>
            <meta attribute="ColumnComment">创建时间</meta>
        </property>

		<property
			name="createUser"
			column="CREATE_USER"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">创建用户名</meta>
            <meta attribute="ColumnComment">创建用户名</meta>
        </property>

		<property
			name="updateTime"
			column="UPDATE_TIME"
			type="timestamp"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">更新时间</meta>
            <meta attribute="ColumnComment">更新时间</meta>
        </property>

		<property
			name="updateUser"
			column="UPDATE_USER"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">更新用户名</meta>
            <meta attribute="ColumnComment">更新用户名</meta>
        </property>

	

		<set name="oaMaterialItems" inverse="true" order-by="id">
			<key column="MATERIAL_ID"/>
			<one-to-many class="OaMaterialItem"/>
		</set>

	</class>	
    <query name="oamaterial_all_data_query">
        <![CDATA[FROM OaMaterial]]>
    </query>
</hibernate-mapping>