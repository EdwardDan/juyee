<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
	<class
		name="OaQuarterplanSum"
		table="OA_QUARTERPLAN_SUM"
	>
		
		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">false</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">季度年度总结计划汇总</meta>
        <meta attribute="tableComment">季度年度总结计划汇总</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">oa</meta>

		<id
			name="id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
		</id>

		<property
			name="year"
			column="YEAR"
			type="integer"
			not-null="false"
			length="4"
        >
            <meta attribute="ColumnName">报告年份</meta>
            <meta attribute="ColumnComment">报告年份</meta>
        </property>

		<property
			name="period"
			column="PERIOD"
			type="integer"
			not-null="false"
			length="2"
        >
            <meta attribute="ColumnName">报告季度</meta>
            <meta attribute="ColumnComment">报告季度</meta>
        </property>

		<property
			name="reportDate"
			column="REPORT_DATE"
			type="date"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">提交时间</meta>
            <meta attribute="ColumnComment">提交时间</meta>
        </property>

		<property
			name="status"
			column="STATUS"
			type="integer"
			not-null="false"
			length="2"
        >
            <meta attribute="ColumnName">状态</meta>
            <meta attribute="ColumnComment">状态</meta>
        </property>

		<property
			name="leaderOpinion"
			column="LEADER_OPINION"
			type="string"
			not-null="false"
        >
            <meta attribute="ColumnName">领导审核意见</meta>
            <meta attribute="ColumnComment">领导审核意见</meta>
        </property>

		<property
			name="leaderAuditTime"
			column="LEADER_AUDIT_TIME"
			type="timestamp"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">领导审核时间</meta>
            <meta attribute="ColumnComment">领导审核时间</meta>
        </property>

		<property
			name="leaderAuditUser"
			column="LEADER_AUDIT_USER"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">领导审核用户名</meta>
            <meta attribute="ColumnComment">领导审核用户名</meta>
        </property>

		<property
			name="createTime"
			column="CREATE_TIME"
			type="timestamp"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">创建时间</meta>
            <meta attribute="ColumnComment">创建时间</meta>
        </property>

		<property
			name="createUser"
			column="CREATE_USER"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">创建用户名</meta>
            <meta attribute="ColumnComment">创建用户名</meta>
        </property>

		<property
			name="updateTime"
			column="UPDATE_TIME"
			type="timestamp"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">更新时间</meta>
            <meta attribute="ColumnComment">更新时间</meta>
        </property>

		<property
			name="updateUser"
			column="UPDATE_USER"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">更新用户名</meta>
            <meta attribute="ColumnComment">更新用户名</meta>
        </property>

        <property
                name="sumContent"
                column="SUM_CONTENT"
                type="string"
                not-null="false"
                >
            <meta attribute="ColumnName">总工作内容</meta>
            <meta attribute="ColumnComment">总工作内容</meta>
        </property>

        <many-to-one
                name="document"
                column="DOCUMENT_ID"
                class="DocDocument"
                not-null="false"
                >
        </many-to-one>

		<set name="oaQuarterplanSumItems" inverse="true" order-by="id">
			<key column="QUARTERPLAN_SUM_ID"/>
			<one-to-many class="OaQuarterplanSumItem"/>
		</set>

	</class>	
    <query name="oaquarterplansum_all_data_query">
        <![CDATA[FROM OaQuarterplanSum]]>
    </query>
</hibernate-mapping>