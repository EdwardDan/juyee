<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
	<class
		name="ShareGdInfo"
		table="SHARE_GD_INFO"
	>
		
		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">false</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">工地基本信息</meta>
        <meta attribute="tableComment">工地基本信息</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">share</meta>

        <id
			name="gdCode"
			column="GD_CODE"
			type="string"
			length="50"
        >
        </id>

		<property
			name="bjbh"
			column="BJBH"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">报建编号</meta>
            <meta attribute="ColumnComment">报建编号</meta>
        </property>

		<property
			name="gdName"
			column="GD_NAME"
			type="string"
			not-null="false"
			length="200"
        >
            <meta attribute="ColumnName">工地名称</meta>
            <meta attribute="ColumnComment">工地名称</meta>
        </property>

		<property
			name="gdCreateDate"
			column="GD_CREATE_DATE"
			type="date"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">工地生成日期</meta>
            <meta attribute="ColumnComment">工地生成日期</meta>
        </property>

		<property
			name="gdAddress"
			column="GD_ADDRESS"
			type="string"
			not-null="false"
			length="200"
        >
            <meta attribute="ColumnName">工地地址</meta>
            <meta attribute="ColumnComment">工地地址</meta>
        </property>

		<property
			name="gdStatusCode"
			column="GD_STATUS_CODE"
			type="string"
			not-null="false"
			length="1"
        >
            <meta attribute="ColumnName">工地状态</meta>
            <meta attribute="ColumnComment">工地状态</meta>
        </property>

		<property
			name="gdStatusDesc"
			column="GD_STATUS_DESC"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">工地状态翻译</meta>
            <meta attribute="ColumnComment">工地状态翻译</meta>
        </property>

		<property
			name="gdChangetime"
			column="GD_CHANGETIME"
			type="timestamp"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">更新时间</meta>
            <meta attribute="ColumnComment">更新时间</meta>
        </property>

		<property
			name="handingUnitCode"
			column="HANDING_UNIT_CODE"
			type="string"
			not-null="false"
			length="20"
        >
            <meta attribute="ColumnName">经办单位代码</meta>
            <meta attribute="ColumnComment">经办单位代码</meta>
        </property>

		<property
			name="handingUnitName"
			column="HANDING_UNIT_NAME"
			type="string"
			not-null="false"
			length="200"
        >
            <meta attribute="ColumnName">经办单位名称</meta>
            <meta attribute="ColumnComment">经办单位名称</meta>
        </property>

		<property
			name="sgTotalUnitCode"
			column="SG_TOTAL_UNIT_CODE"
			type="string"
			not-null="false"
			length="20"
        >
            <meta attribute="ColumnName">施工总包单位代码</meta>
            <meta attribute="ColumnComment">施工总包单位代码</meta>
        </property>

		<property
			name="sgTotalUnitName"
			column="SG_TOTAL_UNIT_NAME"
			type="string"
			not-null="false"
			length="200"
        >
            <meta attribute="ColumnName">施工总包单位名称</meta>
            <meta attribute="ColumnComment">施工总包单位名称</meta>
        </property>

		<property
			name="jlUnitCode"
			column="JL_UNIT_CODE"
			type="string"
			not-null="false"
			length="20"
        >
            <meta attribute="ColumnName">监理单位代码</meta>
            <meta attribute="ColumnComment">监理单位代码</meta>
        </property>

		<property
			name="jlUnitName"
			column="JL_UNIT_NAME"
			type="string"
			not-null="false"
			length="200"
        >
            <meta attribute="ColumnName">监理单位名称</meta>
            <meta attribute="ColumnComment">监理单位名称</meta>
        </property>


	</class>	
    <query name="sharegdinfo_all_data_query">
        <![CDATA[FROM ShareGdInfo]]>
    </query>
</hibernate-mapping>