<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
	<class
		name="JdCysb"
		table="JD_CYSB"
	>
		
		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">false</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">创优申报</meta>
        <meta attribute="tableComment">创优申报</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">jd</meta>

		<id
			name="id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
		</id>

		<property
			name="bjbh"
			column="BJBH"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">报建编号</meta>
            <meta attribute="ColumnComment">报建编号</meta>
        </property>

		<property
			name="reportTypeDesc"
			column="REPORT_TYPE_DESC"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">申报类型中文描述</meta>
            <meta attribute="ColumnComment">申报类型中文描述</meta>
        </property>

		<property
			name="projectAddress"
			column="PROJECT_ADDRESS"
			type="string"
			not-null="false"
			length="200"
        >
            <meta attribute="ColumnName">工程地址</meta>
            <meta attribute="ColumnComment">工程地址</meta>
        </property>

		<property
			name="buildArea"
			column="BUILD_AREA"
			type="java.lang.Double"
			not-null="false"
			length="9"
        >
            <meta attribute="ColumnName">建筑面积</meta>
            <meta attribute="ColumnComment">建筑面积</meta>
        </property>

		<property
			name="structureType"
			column="STRUCTURE_TYPE"
			type="string"
			not-null="false"
			length="200"
        >
            <meta attribute="ColumnName">结构类型</meta>
            <meta attribute="ColumnComment">结构类型</meta>
        </property>

		<property
			name="structureLevel"
			column="STRUCTURE_LEVEL"
			type="string"
			not-null="false"
			length="200"
        >
            <meta attribute="ColumnName">结构层次</meta>
            <meta attribute="ColumnComment">结构层次</meta>
        </property>

		<property
			name="status"
			column="STATUS"
			type="integer"
			not-null="false"
			length="2"
        >
            <meta attribute="ColumnName">状态</meta>
            <meta attribute="ColumnComment">状态</meta>
        </property>

		<property
			name="auditOpinion"
			column="AUDIT_OPINION"
			type="string"
			not-null="false"
        >
            <meta attribute="ColumnName">质量科审核意见</meta>
            <meta attribute="ColumnComment">质量科审核意见</meta>
        </property>

		<property
			name="auditTime"
			column="AUDIT_TIME"
			type="timestamp"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">质量科审核时间</meta>
            <meta attribute="ColumnComment">质量科审核时间</meta>
        </property>

		<property
			name="auditUser"
			column="AUDIT_USER"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">质量科审核用户名</meta>
            <meta attribute="ColumnComment">质量科审核用户名</meta>
        </property>

		<property
			name="createTime"
			column="CREATE_TIME"
			type="timestamp"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">创建时间</meta>
            <meta attribute="ColumnComment">创建时间</meta>
        </property>

		<property
			name="createUser"
			column="CREATE_USER"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">创建用户名</meta>
            <meta attribute="ColumnComment">创建用户名</meta>
        </property>

		<property
			name="updateTime"
			column="UPDATE_TIME"
			type="timestamp"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">更新时间</meta>
            <meta attribute="ColumnComment">更新时间</meta>
        </property>

		<property
			name="updateUser"
			column="UPDATE_USER"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">更新用户名</meta>
            <meta attribute="ColumnComment">更新用户名</meta>
        </property>

		<many-to-one
			name="reportType"
			column="REPORT_TYPE_ID"
			class="com.justonetech.system.domain.SysCodeDetail"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="document"
			column="DOCUMENT_ID"
			class="DocDocument"
			not-null="false"
		>
		</many-to-one>

	</class>	
    <query name="jdcysb_all_data_query">
        <![CDATA[FROM JdCysb]]>
    </query>
</hibernate-mapping>