<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
	<class
		name="WebContractBack"
		table="WEB_CONTRACT_BACK"
	>
		
		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">false</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">合同登记备案情况</meta>
        <meta attribute="tableComment">合同登记备案情况</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">web</meta>

		<id
			name="id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
		</id>

		<property
			name="projectName"
			column="PROJECT_NAME"
			type="string"
			not-null="false"
			length="500"
        >
            <meta attribute="ColumnName">项目名称</meta>
            <meta attribute="ColumnComment">项目名称</meta>
        </property>

		<property
			name="cbUnit"
			column="CB_UNIT"
			type="string"
			not-null="false"
			length="500"
        >
            <meta attribute="ColumnName">承包方</meta>
            <meta attribute="ColumnComment">承包方</meta>
        </property>

		<property
			name="contractCategory"
			column="CONTRACT_CATEGORY"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">合同类别</meta>
            <meta attribute="ColumnComment">合同类别</meta>
        </property>

		<property
			name="contractMonry"
			column="CONTRACT_MONRY"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">合同金额（万元）</meta>
            <meta attribute="ColumnComment">合同金额（万元）</meta>
        </property>

		<many-to-one
			name="projectInfo"
			column="PROJECT_INFO_ID"
			class="WebProjectInfo"
			not-null="false"
		>
		</many-to-one>
	

		<set name="webContractBackPags" inverse="true">
			<key column="CONTRACT_BACK_ID"/>
			<one-to-many class="WebContractBackPags"/>
		</set>

	</class>	
    <query name="webcontractback_all_data_query">
        <![CDATA[FROM WebContractBack]]>
    </query>
</hibernate-mapping>