<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
    <class
            name="OaKnowledge"
            table="OA_KNOWLEDGE"
            >

        <meta attribute="sync-DAO">false</meta>
        <meta attribute="sync-jsp">true</meta>
        <meta attribute="sync-boolean">get</meta>
        <meta attribute="treeable">false</meta>
        <meta attribute="projectable">false</meta>
        <meta attribute="tableName">业务知识交流</meta>
        <meta attribute="tableComment">业务知识交流</meta>
        <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">oa</meta>

        <id
                name="id"
                type="java.lang.Long"
                column="ID"
                >
            <generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
        </id>

        <property
                name="typeName"
                column="TYPE_NAME"
                type="string"
                not-null="false"
                length="200"
                >
            <meta attribute="ColumnName">类型中文描述</meta>
            <meta attribute="ColumnComment">类型中文描述</meta>
        </property>

        <property
                name="title"
                column="TITLE"
                type="string"
                not-null="false"
                length="200"
                >
            <meta attribute="ColumnName">主题名称</meta>
            <meta attribute="ColumnComment">主题名称</meta>
        </property>

        <property
                name="reportPerson"
                column="REPORT_PERSON"
                type="string"
                not-null="false"
                length="50"
                >
            <meta attribute="ColumnName">提出人</meta>
            <meta attribute="ColumnComment">提出人</meta>
        </property>

        <property
                name="reportDept"
                column="REPORT_DEPT"
                type="string"
                not-null="false"
                length="100"
                >
            <meta attribute="ColumnName">提出部门</meta>
            <meta attribute="ColumnComment">提出部门</meta>
        </property>

        <property
                name="content"
                column="CONTENT"
                type="string"
                not-null="false"
                >
            <meta attribute="ColumnName">主要内容</meta>
            <meta attribute="ColumnComment">主要内容</meta>
        </property>

        <property
                name="reportDate"
                column="REPORT_DATE"
                type="timestamp"
                not-null="false"
                length="7"
                >
            <meta attribute="ColumnName">发布时间</meta>
            <meta attribute="ColumnComment">发布时间</meta>
        </property>

        <property
                name="visitTimes"
                column="VISIT_TIMES"
                type="java.lang.Long"
                not-null="false"
                length="10"
                >
            <meta attribute="ColumnName">访问次数</meta>
            <meta attribute="ColumnComment">访问次数</meta>
        </property>

        <property
                name="isValid"
                column="IS_VALID"
                type="java.lang.Boolean"
                not-null="false"
                length="1"
                >
            <meta attribute="ColumnName">是否有效</meta>
            <meta attribute="ColumnComment">是否有效</meta>
        </property>

        <property
                name="createTime"
                column="CREATE_TIME"
                type="timestamp"
                not-null="false"
                length="7"
                >
            <meta attribute="ColumnName">创建时间</meta>
            <meta attribute="ColumnComment">创建时间</meta>
        </property>

        <property
                name="createUser"
                column="CREATE_USER"
                type="string"
                not-null="false"
                length="100"
                >
            <meta attribute="ColumnName">创建用户名</meta>
            <meta attribute="ColumnComment">创建用户名</meta>
        </property>

        <property
                name="updateTime"
                column="UPDATE_TIME"
                type="timestamp"
                not-null="false"
                length="7"
                >
            <meta attribute="ColumnName">更新时间</meta>
            <meta attribute="ColumnComment">更新时间</meta>
        </property>

        <property
                name="updateUser"
                column="UPDATE_USER"
                type="string"
                not-null="false"
                length="100"
                >
            <meta attribute="ColumnName">更新用户名</meta>
            <meta attribute="ColumnComment">更新用户名</meta>
        </property>

        <many-to-one
                name="refer"
                column="REFER_ID"
                class="OaKnowledge"
                not-null="false"
                >
        </many-to-one>
        <many-to-one
                name="document"
                column="DOCUMENT_ID"
                class="DocDocument"
                not-null="false"
                >
        </many-to-one>
        <many-to-one
                name="type"
                column="TYPE_ID"
                class="com.justonetech.system.domain.SysCodeDetail"
                not-null="false"
                >
        </many-to-one>


        <set name="oaKnowledges" inverse="true" order-by="REPORT_DATE">
            <key column="REFER_ID"/>
            <one-to-many class="OaKnowledge"/>
        </set>

    </class>
    <query name="oaknowledge_all_data_query">
        <![CDATA[FROM OaKnowledge]]>
    </query>
</hibernate-mapping>