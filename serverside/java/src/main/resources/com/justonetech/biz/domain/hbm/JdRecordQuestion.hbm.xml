<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
	<class
		name="JdRecordQuestion"
		table="JD_RECORD_QUESTION"
	>
		
		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">false</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">安全质量监督--问题</meta>
        <meta attribute="tableComment">安全质量监督--问题</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">jd</meta>

		<id
			name="id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
		</id>

		<property
			name="recordId"
			column="RECORD_ID"
			type="java.lang.Long"
			not-null="false"
			length="10"
        >
            <meta attribute="ColumnName">监督id(首次/安全/质量/竣工)</meta>
            <meta attribute="ColumnComment">监督id(首次/安全/质量/竣工)</meta>
        </property>

        <property
                name="jdTypeCode"
                column="JD_TYPE_CODE"
                type="string"
                not-null="false"
                length="50"
                >
            <meta attribute="ColumnName">监督类别编码</meta>
            <meta attribute="ColumnComment">监督类别编码</meta>
        </property>

		<property
			name="typeCode"
			column="TYPE_CODE"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">问题类别编码</meta>
            <meta attribute="ColumnComment">问题类别编码</meta>
        </property>

		<property
			name="orderNo"
			column="ORDER_NO"
			type="integer"
			not-null="false"
			length="4"
        >
            <meta attribute="ColumnName">序号</meta>
            <meta attribute="ColumnComment">序号</meta>
        </property>

		<property
			name="questionId"
			column="QUESTION_ID"
			type="java.lang.Long"
			not-null="false"
			length="10"
        >
            <meta attribute="ColumnName">问题id</meta>
            <meta attribute="ColumnComment">问题id</meta>
        </property>

		<property
			name="questionDesc"
			column="QUESTION_DESC"
			type="string"
			not-null="false"
        >
            <meta attribute="ColumnName">问题描述</meta>
            <meta attribute="ColumnComment">问题描述</meta>
        </property>

		<property
			name="dealAdvise"
			column="DEAL_ADVISE"
			type="string"
			not-null="false"
        >
            <meta attribute="ColumnName">处理建议</meta>
            <meta attribute="ColumnComment">处理建议</meta>
        </property>

		<property
			name="unlawId"
			column="UNLAW_ID"
			type="java.lang.Long"
			not-null="false"
			length="10"
        >
            <meta attribute="ColumnName">违法条款id</meta>
            <meta attribute="ColumnComment">违法条款id</meta>
        </property>

		<property
			name="unlawDesc"
			column="UNLAW_DESC"
			type="string"
			not-null="false"
        >
            <meta attribute="ColumnName">违法条款描述</meta>
            <meta attribute="ColumnComment">违法条款描述</meta>
        </property>

		<property
			name="jdPersonId"
			column="JD_PERSON_ID"
			type="java.lang.Long"
			not-null="false"
			length="10"
        >
            <meta attribute="ColumnName">监督员id</meta>
            <meta attribute="ColumnComment">监督员id</meta>
        </property>

		<property
			name="jdPersonDesc"
			column="JD_PERSON_DESC"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">监督员描述</meta>
            <meta attribute="ColumnComment">监督员描述</meta>
        </property>

		<property
			name="planEndDate"
			column="PLAN_END_DATE"
			type="date"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">拟整改截止日期</meta>
            <meta attribute="ColumnComment">拟整改截止日期</meta>
        </property>

		<property
			name="isForm"
			column="IS_FORM"
			type="java.lang.Boolean"
			not-null="false"
			length="1"
        >
            <meta attribute="ColumnName">是否开单</meta>
            <meta attribute="ColumnComment">是否开单</meta>
        </property>


	</class>	
    <query name="jdrecordquestion_all_data_query">
        <![CDATA[FROM JdRecordQuestion]]>
    </query>
</hibernate-mapping>