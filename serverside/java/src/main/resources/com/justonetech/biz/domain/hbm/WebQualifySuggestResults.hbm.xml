<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
	<class
		name="WebQualifySuggestResults"
		table="WEB_QUALIFY_SUGGEST_RESULTS"
	>
		
		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">false</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">企业资质审查结果</meta>
        <meta attribute="tableComment">企业资质审查结果</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">web</meta>

		<id
			name="id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
		</id>

		<property
			name="ayyly"
			column="AYYLY"
			type="string"
			not-null="false"
			length="500"
        >
            <meta attribute="ColumnName">受理事项</meta>
            <meta attribute="ColumnComment">受理事项</meta>
        </property>

		<property
			name="advice"
			column="ADVICE"
			type="string"
			not-null="false"
			length="500"
        >
            <meta attribute="ColumnName">公示意见</meta>
            <meta attribute="ColumnComment">公示意见</meta>
        </property>

		<property
			name="content"
			column="CONTENT"
			type="string"
			not-null="false"
			length="1000"
        >
            <meta attribute="ColumnName">公示内容</meta>
            <meta attribute="ColumnComment">公示内容</meta>
        </property>

		<many-to-one
			name="qualifySuggest"
			column="QUALIFY_SUGGEST_ID"
			class="WebUnitQualifySuggest"
			not-null="false"
		>
		</many-to-one>

	</class>	
    <query name="webqualifysuggestresults_all_data_query">
        <![CDATA[FROM WebQualifySuggestResults]]>
    </query>
</hibernate-mapping>