<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
	<class
		name="WebSgPermit"
		table="WEB_SG_PERMIT"
	>
		
		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">false</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">施工许可</meta>
        <meta attribute="tableComment">施工许可</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">web</meta>

		<id
			name="id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
		</id>

		<property
			name="permitNo"
			column="PERMIT_NO"
			type="string"
			not-null="false"
			length="500"
        >
            <meta attribute="ColumnName">许可证号</meta>
            <meta attribute="ColumnComment">许可证号</meta>
        </property>

		<property
			name="permitDate"
			column="PERMIT_DATE"
			type="date"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">许可日期</meta>
            <meta attribute="ColumnComment">许可日期</meta>
        </property>

		<property
			name="sjUnit"
			column="SJ_UNIT"
			type="string"
			not-null="false"
			length="500"
        >
            <meta attribute="ColumnName">设计单位</meta>
            <meta attribute="ColumnComment">设计单位</meta>
        </property>

		<property
			name="sgUnit"
			column="SG_UNIT"
			type="string"
			not-null="false"
			length="500"
        >
            <meta attribute="ColumnName">施工总包单位</meta>
            <meta attribute="ColumnComment">施工总包单位</meta>
        </property>

		<property
			name="jlUnit"
			column="JL_UNIT"
			type="string"
			not-null="false"
			length="500"
        >
            <meta attribute="ColumnName">监理单位</meta>
            <meta attribute="ColumnComment">监理单位</meta>
        </property>

		<property
			name="contractMoney"
			column="CONTRACT_MONEY"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">合同价格</meta>
            <meta attribute="ColumnComment">合同价格</meta>
        </property>

		<property
			name="area"
			column="AREA"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">建设规模（面积）</meta>
            <meta attribute="ColumnComment">建设规模（面积）</meta>
        </property>

		<property
			name="startDate"
			column="START_DATE"
			type="date"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">合同开工日期</meta>
            <meta attribute="ColumnComment">合同开工日期</meta>
        </property>

		<property
			name="completeDate"
			column="COMPLETE_DATE"
			type="date"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">合同竣工日期</meta>
            <meta attribute="ColumnComment">合同竣工日期</meta>
        </property>

		<many-to-one
			name="projectInfo"
			column="PROJECT_INFO_ID"
			class="WebProjectInfo"
			not-null="false"
		>
		</many-to-one>
	

		<set name="webSgPermitScopes" inverse="true">
			<key column="SG_PERMIT_ID"/>
			<one-to-many class="WebSgPermitScope"/>
		</set>

	</class>	
    <query name="websgpermit_all_data_query">
        <![CDATA[FROM WebSgPermit]]>
    </query>
</hibernate-mapping>