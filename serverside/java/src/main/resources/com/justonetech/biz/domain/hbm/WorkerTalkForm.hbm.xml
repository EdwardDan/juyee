<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
	<class
		name="WorkerTalkForm"
		table="WORKER_TALK_FORM"
	>
		
		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">false</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">执法管理谈话通知书</meta>
        <meta attribute="tableComment">执法管理谈话通知书</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">worker</meta>

		<id
			name="id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
		</id>

		<property
			name="projectName"
			column="PROJECT_NAME"
			type="string"
			not-null="false"
			length="200"
        >
            <meta attribute="ColumnName">工程名称</meta>
            <meta attribute="ColumnComment">工程名称</meta>
        </property>

		<property
			name="formCode"
			column="FORM_CODE"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">号</meta>
            <meta attribute="ColumnComment">号</meta>
        </property>

		<property
			name="unitName1"
			column="UNIT_NAME1"
			type="string"
			not-null="false"
			length="200"
        >
            <meta attribute="ColumnName">谈话单位1</meta>
            <meta attribute="ColumnComment">谈话单位1</meta>
        </property>

		<property
			name="unitName2"
			column="UNIT_NAME2"
			type="string"
			not-null="false"
			length="200"
        >
            <meta attribute="ColumnName">谈话单位2</meta>
            <meta attribute="ColumnComment">谈话单位2</meta>
        </property>

		<property
			name="content"
			column="CONTENT"
			type="string"
			not-null="false"
        >
            <meta attribute="ColumnName">谈话事由</meta>
            <meta attribute="ColumnComment">谈话事由</meta>
        </property>

		<property
			name="talkTime"
			column="TALK_TIME"
			type="timestamp"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">谈话时间</meta>
            <meta attribute="ColumnComment">谈话时间</meta>
        </property>

		<property
			name="signDate"
			column="SIGN_DATE"
			type="date"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">签发时间</meta>
            <meta attribute="ColumnComment">签发时间</meta>
        </property>

		<property
			name="signPerson"
			column="SIGN_PERSON"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">签发人</meta>
            <meta attribute="ColumnComment">签发人</meta>
        </property>

		<property
			name="talkPerson"
			column="TALK_PERSON"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">内部谈话人</meta>
            <meta attribute="ColumnComment">内部谈话人</meta>
        </property>

		<property
			name="materialIds"
			column="MATERIAL_IDS"
			type="string"
			not-null="false"
        >
            <meta attribute="ColumnName">携带材料</meta>
            <meta attribute="ColumnComment">携带材料</meta>
        </property>

		<property
			name="createTime"
			column="CREATE_TIME"
			type="timestamp"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">创建时间</meta>
            <meta attribute="ColumnComment">创建时间</meta>
        </property>

		<property
			name="createUser"
			column="CREATE_USER"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">创建用户名</meta>
            <meta attribute="ColumnComment">创建用户名</meta>
        </property>

		<property
			name="updateTime"
			column="UPDATE_TIME"
			type="timestamp"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">更新时间</meta>
            <meta attribute="ColumnComment">更新时间</meta>
        </property>

		<property
			name="updateUser"
			column="UPDATE_USER"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">更新用户名</meta>
            <meta attribute="ColumnComment">更新用户名</meta>
        </property>


	</class>	
    <query name="workertalkform_all_data_query">
        <![CDATA[FROM WorkerTalkForm]]>
    </query>
</hibernate-mapping>