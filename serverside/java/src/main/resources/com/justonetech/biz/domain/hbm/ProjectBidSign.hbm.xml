<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
	<class
		name="ProjectBidSign"
		table="PROJECT_BID_SIGN"
	>
		
		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">false</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">招标登记</meta>
        <meta attribute="tableComment">招标登记</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">project</meta>

		<id
			name="id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
		</id>

		<property
			name="bjbh"
			column="BJBH"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">报建编号</meta>
            <meta attribute="ColumnComment">报建编号</meta>
        </property>

		<property
			name="bdh"
			column="BDH"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">标段号</meta>
            <meta attribute="ColumnComment">标段号</meta>
        </property>

		<property
			name="projectName"
			column="PROJECT_NAME"
			type="string"
			not-null="false"
			length="200"
        >
            <meta attribute="ColumnName">项目名称</meta>
            <meta attribute="ColumnComment">项目名称</meta>
        </property>

		<property
			name="buildUnit"
			column="BUILD_UNIT"
			type="string"
			not-null="false"
			length="200"
        >
            <meta attribute="ColumnName">建设单位</meta>
            <meta attribute="ColumnComment">建设单位</meta>
        </property>

		<property
			name="buildAddress"
			column="BUILD_ADDRESS"
			type="string"
			not-null="false"
			length="200"
        >
            <meta attribute="ColumnName">建设地点</meta>
            <meta attribute="ColumnComment">建设地点</meta>
        </property>
        <property
                name="signDate"
                column="SIGN_DATE"
                type="timestamp"
                not-null="false"
                length="7"
                >
            <meta attribute="ColumnName">开标日期</meta>
            <meta attribute="ColumnComment">开标日期</meta>
        </property>
		<property
			name="signLastDate"
			column="SIGN_LAST_DATE"
			type="timestamp"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">报名截至时间</meta>
            <meta attribute="ColumnComment">报名截至时间</meta>
        </property>

		<property
			name="signProxyUnit"
			column="SIGN_PROXY_UNIT"
			type="string"
			not-null="false"
			length="200"
        >
            <meta attribute="ColumnName">招标代理单位</meta>
            <meta attribute="ColumnComment">招标代理单位</meta>
        </property>

		<property
			name="linkPerson"
			column="LINK_PERSON"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">联系人</meta>
            <meta attribute="ColumnComment">联系人</meta>
        </property>

		<property
			name="linkTel"
			column="LINK_TEL"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">联系电话</meta>
            <meta attribute="ColumnComment">联系电话</meta>
        </property>

        <property
                name="signAccount"
                column="SIGN_ACCOUNT"
                type="java.lang.Double"
                not-null="false"
                length="12"
                >
            <meta attribute="ColumnName">投标保证金</meta>
            <meta attribute="ColumnComment">投标保证金</meta>
        </property>

		<property
			name="createTime"
			column="CREATE_TIME"
			type="timestamp"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">创建时间</meta>
            <meta attribute="ColumnComment">创建时间</meta>
        </property>

		<property
			name="createUser"
			column="CREATE_USER"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">创建用户名</meta>
            <meta attribute="ColumnComment">创建用户名</meta>
        </property>

		<property
			name="updateTime"
			column="UPDATE_TIME"
			type="timestamp"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">更新时间</meta>
            <meta attribute="ColumnComment">更新时间</meta>
        </property>

		<property
			name="updateUser"
			column="UPDATE_USER"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">更新用户名</meta>
            <meta attribute="ColumnComment">更新用户名</meta>
        </property>

        <many-to-one
                name="status"
                column="STATUS_ID"
                class="com.justonetech.system.domain.SysCodeDetail"
                not-null="false"
                >
        </many-to-one>

		<set name="projectBidSignDepts" inverse="true">
			<key column="PROJECT_BID_SIGN_ID"/>
			<one-to-many class="ProjectBidSignDept"/>
		</set>

	</class>	
    <query name="projectbidsign_all_data_query">
        <![CDATA[FROM ProjectBidSign]]>
    </query>
</hibernate-mapping>