<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
	<class
		name="OrderFormDeploy"
		table="ORDER_FORM_DEPLOY"
	>
		
		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">false</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">预约单调配</meta>
        <meta attribute="tableComment">预约单调配</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">order</meta>

		<id
			name="id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
		</id>

		<property
			name="acceptUserName"
			column="ACCEPT_USER_NAME"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">受理人</meta>
            <meta attribute="ColumnComment">受理人</meta>
        </property>

		<property
			name="acceptDatetime"
			column="ACCEPT_DATETIME"
			type="timestamp"
			not-null="false"
			length="16"
        >
            <meta attribute="ColumnName">受理时间</meta>
            <meta attribute="ColumnComment">受理时间</meta>
        </property>

		<property
			name="acceptOpinionDesc"
			column="ACCEPT_OPINION_DESC"
			type="string"
			not-null="false"
			length="500"
        >
            <meta attribute="ColumnName">受理备注</meta>
            <meta attribute="ColumnComment">受理备注</meta>
        </property>

		<many-to-one
			name="orderForm"
			column="ORDER_FORM_ID"
			class="OrderForm"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="workGroup"
			column="WORK_GROUP_ID"
			class="PollingWorkGroup"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="acceptOpinion"
			column="ACCEPT_OPINION"
			class="com.justonetech.system.domain.SysCodeDetail"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="acceptUser"
			column="ACCEPT_USER_ID"
			class="com.justonetech.system.domain.SysUser"
			not-null="false"
		>
		</many-to-one>
        <set name="orderFormDeployUser" inverse="true" >
            <key column="ORDER_FORM_DEPLOY_ID"/>
            <one-to-many class="OrderFormDeployUser"/>
        </set>
	</class>	
    <query name="orderformdeploy_all_data_query">
        <![CDATA[FROM OrderFormDeploy]]>
    </query>
</hibernate-mapping>