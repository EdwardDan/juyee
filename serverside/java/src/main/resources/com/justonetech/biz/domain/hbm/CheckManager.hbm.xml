<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
    <class
            name="com.justonetech.biz.domain.CheckManager"
            table="CHECK_MANAGER"
            >

        <meta attribute="sync-DAO">false</meta>
        <meta attribute="sync-jsp">true</meta>
        <meta attribute="sync-boolean">get</meta>
        <meta attribute="treeable">false</meta>
        <meta attribute="projectable">false</meta>
        <meta attribute="tableName">管理人员表</meta>
        <meta attribute="tableComment">管理人员表</meta>
        <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">check</meta>

        <id
                name="id"
                type="java.lang.Long"
                column="ID"
                >
            <generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
        </id>

        <property
                name="name"
                column="NAME"
                type="string"
                not-null="false"
                length="50"
                >
            <meta attribute="ColumnName">姓名</meta>
            <meta attribute="ColumnComment">姓名</meta>
        </property>

        <property
                name="sex"
                column="SEX"
                type="string"
                not-null="false"
                length="1"
                >
            <meta attribute="ColumnName">性别</meta>
            <meta attribute="ColumnComment">性别</meta>
        </property>

        <property
                name="cardNo"
                column="CARD_NO"
                type="string"
                not-null="false"
                length="50"
                >
            <meta attribute="ColumnName">身份证号</meta>
            <meta attribute="ColumnComment">身份证号</meta>
        </property>

        <property
                name="mobile"
                column="MOBILE"
                type="string"
                not-null="false"
                length="50"
                >
            <meta attribute="ColumnName">手机号码</meta>
            <meta attribute="ColumnComment">手机号码</meta>
        </property>

        <property
                name="photoFile"
                column="PHOTO_FILE"
                type="string"
                not-null="false"
                length="100"
                >
            <meta attribute="ColumnName">照片</meta>
            <meta attribute="ColumnComment">照片</meta>
        </property>

        <property
                name="createTime"
                column="CREATE_TIME"
                type="timestamp"
                not-null="false"
                length="19"
                >
            <meta attribute="ColumnName">创建时间</meta>
            <meta attribute="ColumnComment">创建时间</meta>
        </property>

        <property
                name="updateTime"
                column="UPDATE_TIME"
                type="timestamp"
                not-null="false"
                length="19"
                >
            <meta attribute="ColumnName">更新时间</meta>
            <meta attribute="ColumnComment">更新时间</meta>
        </property>

        <property
                name="updateUser"
                column="UPDATE_USER"
                type="string"
                not-null="false"
                length="100"
                >
            <meta attribute="ColumnName">更新人</meta>
            <meta attribute="ColumnComment">更新人</meta>
        </property>

        <property
                name="createUser"
                column="CREATE_USER"
                type="string"
                not-null="false"
                length="100"
                >
            <meta attribute="ColumnName">创建人</meta>
            <meta attribute="ColumnComment">创建人</meta>
        </property>

        <property
                name="deleteUser"
                column="DELETE_USER"
                type="string"
                not-null="false"
                length="100"
                >
            <meta attribute="ColumnName">删除人</meta>
            <meta attribute="ColumnComment">删除人</meta>
        </property>

        <property
                name="deleteTime"
                column="DELETE_TIME"
                type="timestamp"
                not-null="false"
                length="19"
                >
            <meta attribute="ColumnName">删除时间</meta>
            <meta attribute="ColumnComment">删除时间</meta>
        </property>

        <property
                name="isDelete"
                column="IS_DELETE"
                type="java.lang.Boolean"
                not-null="false"
                length="1"
                >
            <meta attribute="ColumnName">逻辑删除</meta>
            <meta attribute="ColumnComment">逻辑删除</meta>
        </property>

        <property
                name="guid"
                column="GUID"
                type="string"
                not-null="false"
                length="50"
                >
            <meta attribute="ColumnName">GUID</meta>
            <meta attribute="ColumnComment">GUID</meta>
        </property>

        <set name="checkManagerPositions" inverse="true">
            <key column="MANAGER_ID"/>
            <one-to-many class="CheckManagerPosition"/>
        </set>

        <set name="checkFingerPrints" inverse="true">
            <key column="MANAGER_ID"/>
            <one-to-many class="CheckFingerPrint"/>
        </set>

        <set name="checkInfos" inverse="true">
            <key column="MANAGER_ID"/>
            <one-to-many class="com.justonetech.biz.domain.CheckInfo"/>
        </set>

        <set name="checkReasons" inverse="true">
            <key column="MANAGER_ID"/>
            <one-to-many class="CheckReason"/>
        </set>

    </class>
    <query name="checkmanager_all_data_query">
        <![CDATA[FROM CheckManager]]>
    </query>
</hibernate-mapping>