<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
	<class
		name="NetAcceptStatus"
		table="NET_ACCEPT_STATUS"
	>
		
		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">false</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">项目审批_受理状态设置</meta>
        <meta attribute="tableComment">项目审批_受理状态设置</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">net</meta>

		<id
			name="id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
		</id>

		<property
			name="no"
			column="NO"
			type="integer"
			not-null="false"
			length="3"
        >
            <meta attribute="ColumnName">序号</meta>
            <meta attribute="ColumnComment">序号</meta>
        </property>

		<property
			name="name"
			column="NAME"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">状态名称</meta>
            <meta attribute="ColumnComment">状态名称</meta>
        </property>

        <property
                name="label"
                column="LABEL"
                type="string"
                not-null="false"
                length="50"
                >
            <meta attribute="ColumnName">受理日期重命名</meta>
            <meta attribute="ColumnComment">受理日期重命名</meta>
        </property>

		<property
			name="isPass"
			column="IS_PASS"
			type="java.lang.Boolean"
			not-null="false"
			length="1"
        >
            <meta attribute="ColumnName">是否通过</meta>
            <meta attribute="ColumnComment">是否通过</meta>
        </property>

		<property
			name="isStart"
			column="IS_START"
			type="java.lang.Boolean"
			not-null="false"
			length="1"
        >
            <meta attribute="ColumnName">是否开始状态</meta>
            <meta attribute="ColumnComment">是否开始状态</meta>
        </property>

		<property
			name="isEnd"
			column="IS_END"
			type="java.lang.Boolean"
			not-null="false"
			length="1"
        >
            <meta attribute="ColumnName">是否结束状态</meta>
            <meta attribute="ColumnComment">是否结束状态</meta>
        </property>

		<many-to-one
			name="acceptStep"
			column="ACCEPT_STEP_ID"
			class="NetAcceptStep"
			not-null="false"
		>
		</many-to-one>
	

		<set name="netOpLogs" inverse="true">
			<key column="ACCEPT_STATUS_ID"/>
			<one-to-many class="NetOpLog"/>
		</set>

		<set name="netMonitors" inverse="true">
			<key column="ACCEPT_STATUS_ID"/>
			<one-to-many class="NetMonitor"/>
		</set>

		<set name="netApplies" inverse="true">
			<key column="ACCEPT_STATUS_ID"/>
			<one-to-many class="NetApply"/>
		</set>

		<set name="netContracts" inverse="true">
			<key column="ACCEPT_STATUS_ID"/>
			<one-to-many class="NetContract"/>
		</set>

		<set name="netCfbs" inverse="true">
			<key column="ACCEPT_STATUS_ID"/>
			<one-to-many class="NetCfb"/>
		</set>

		<set name="netSgpermits" inverse="true">
			<key column="ACCEPT_STATUS_ID"/>
			<one-to-many class="NetSgpermit"/>
		</set>

	</class>	
    <query name="netacceptstatus_all_data_query">
        <![CDATA[FROM NetAcceptStatus]]>
    </query>
</hibernate-mapping>