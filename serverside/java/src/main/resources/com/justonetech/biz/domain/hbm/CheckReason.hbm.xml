<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
    <class
            name="CheckReason"
            table="CHECK_REASON"
            >

        <meta attribute="sync-DAO">false</meta>
        <meta attribute="sync-jsp">true</meta>
        <meta attribute="sync-boolean">get</meta>
        <meta attribute="treeable">false</meta>
        <meta attribute="projectable">false</meta>
        <meta attribute="tableName">未考勤理由</meta>
        <meta attribute="tableComment">未考勤理由</meta>
        <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">check</meta>

        <id
                name="id"
                type="java.lang.Long"
                column="ID"
                >
            <generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
        </id>

        <property
                name="gdCode"
                column="GD_CODE"
                type="string"
                not-null="false"
                length="50"
                >
            <meta attribute="ColumnName">工地编号</meta>
            <meta attribute="ColumnComment">工地编号</meta>
        </property>

        <property
                name="checkReason"
                column="CHECK_REASON"
                type="string"
                not-null="false"
                length="500"
                >
            <meta attribute="ColumnName">未考勤理由</meta>
            <meta attribute="ColumnComment">未考勤理由</meta>
        </property>

        <property
                name="checkDate"
                column="CHECK_DATE"
                type="date"
                not-null="false"
                length="10"
                >
            <meta attribute="ColumnName">未考勤时间</meta>
            <meta attribute="ColumnComment">未考勤时间</meta>
        </property>

        <property
                name="createTime"
                column="CREATE_TIME"
                type="timestamp"
                not-null="false"
                length="19"
                >
            <meta attribute="ColumnName">创建时间</meta>
            <meta attribute="ColumnComment">创建时间</meta>
        </property>

        <property
                name="updateTime"
                column="UPDATE_TIME"
                type="timestamp"
                not-null="false"
                length="19"
                >
            <meta attribute="ColumnName">更新时间</meta>
            <meta attribute="ColumnComment">更新时间</meta>
        </property>

        <property
                name="updateUser"
                column="UPDATE_USER"
                type="string"
                not-null="false"
                length="100"
                >
            <meta attribute="ColumnName">更新人</meta>
            <meta attribute="ColumnComment">更新人</meta>
        </property>

        <property
                name="createUser"
                column="CREATE_USER"
                type="string"
                not-null="false"
                length="100"
                >
            <meta attribute="ColumnName">创建人</meta>
            <meta attribute="ColumnComment">创建人</meta>
        </property>

        <property
                name="deleteUser"
                column="DELETE_USER"
                type="string"
                not-null="false"
                length="100"
                >
            <meta attribute="ColumnName">删除人</meta>
            <meta attribute="ColumnComment">删除人</meta>
        </property>

        <property
                name="deleteTime"
                column="DELETE_TIME"
                type="timestamp"
                not-null="false"
                length="19"
                >
            <meta attribute="ColumnName">删除时间</meta>
            <meta attribute="ColumnComment">删除时间</meta>
        </property>

        <property
                name="isDelete"
                column="IS_DELETE"
                type="java.lang.Boolean"
                not-null="false"
                length="1"
                >
            <meta attribute="ColumnName">逻辑删除</meta>
            <meta attribute="ColumnComment">逻辑删除</meta>
        </property>

        <property
                name="guid"
                column="GUID"
                type="string"
                not-null="false"
                length="50"
                >
            <meta attribute="ColumnName">GUID</meta>
            <meta attribute="ColumnComment">GUID</meta>
        </property>

        <many-to-one
                name="manager"
                column="MANAGER_ID"
                class="com.justonetech.biz.domain.CheckManager"
                not-null="false"
                >
        </many-to-one>

    </class>
    <query name="checkreason_all_data_query">
        <![CDATA[FROM CheckReason]]>
    </query>
</hibernate-mapping>