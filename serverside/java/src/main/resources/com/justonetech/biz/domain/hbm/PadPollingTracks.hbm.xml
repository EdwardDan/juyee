<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
	<class
		name="PadPollingTracks"
		table="PAD_POLLING_TRACKS"
	>
		
		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">false</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">移动巡检轨迹</meta>
        <meta attribute="tableComment">移动巡检轨迹</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">pad</meta>

		<id
			name="id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
		</id>

		<property
			name="userName"
			column="USER_NAME"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">用户名称</meta>
            <meta attribute="ColumnComment">用户名称</meta>
        </property>

		<property
			name="imei"
			column="IMEI"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">设备标识码</meta>
            <meta attribute="ColumnComment">设备标识码</meta>
        </property>

		<property
			name="longitude"
			column="LONGITUDE"
			type="java.lang.Double"
			not-null="false"
			length="18"
        >
            <meta attribute="ColumnName">经度</meta>
            <meta attribute="ColumnComment">经度</meta>
        </property>

		<property
			name="latitude"
			column="LATITUDE"
			type="java.lang.Double"
			not-null="false"
			length="18"
        >
            <meta attribute="ColumnName">纬度</meta>
            <meta attribute="ColumnComment">纬度</meta>
        </property>

		<property
			name="moveTime"
			column="MOVE_TIME"
			type="timestamp"
			not-null="false"
			length="19"
        >
            <meta attribute="ColumnName">移动时间</meta>
            <meta attribute="ColumnComment">移动时间</meta>
        </property>

		<property
			name="createTime"
			column="CREATE_TIME"
			type="timestamp"
			not-null="false"
			length="19"
        >
            <meta attribute="ColumnName">创建时间</meta>
            <meta attribute="ColumnComment">创建时间</meta>
        </property>

		<property
			name="updateTime"
			column="UPDATE_TIME"
			type="timestamp"
			not-null="false"
			length="19"
        >
            <meta attribute="ColumnName">更新时间</meta>
            <meta attribute="ColumnComment">更新时间</meta>
        </property>

		<many-to-one
			name="user"
			column="USER_ID"
			class="com.justonetech.system.domain.SysUser"
			not-null="false"
		>
		</many-to-one>

	</class>	
    <query name="padpollingtracks_all_data_query">
        <![CDATA[FROM PadPollingTracks]]>
    </query>
</hibernate-mapping>