<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
	<class
		name="WebUnitQualifyPermit"
		table="WEB_UNIT_QUALIFY_PERMIT"
	>
		
		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">false</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">企业资质核准</meta>
        <meta attribute="tableComment">企业资质核准</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">web</meta>

		<id
			name="id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
		</id>

		<property
			name="unitName"
			column="UNIT_NAME"
			type="string"
			not-null="false"
			length="500"
        >
            <meta attribute="ColumnName">企业名称</meta>
            <meta attribute="ColumnComment">企业名称</meta>
        </property>

		<property
			name="permitThing"
			column="PERMIT_THING"
			type="string"
			not-null="false"
			length="500"
        >
            <meta attribute="ColumnName">核准事项</meta>
            <meta attribute="ColumnComment">核准事项</meta>
        </property>

		<property
			name="applyNo"
			column="APPLY_NO"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">受理批次</meta>
            <meta attribute="ColumnComment">受理批次</meta>
        </property>

		<property
			name="titleDate"
			column="TITLE_DATE"
			type="date"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">公告日期</meta>
            <meta attribute="ColumnComment">公告日期</meta>
        </property>

	

		<set name="webQualifyPermitItems" inverse="true">
			<key column="QUALIFY_PERMIT_ID"/>
			<one-to-many class="WebQualifyPermitItems"/>
		</set>

	</class>	
    <query name="webunitqualifypermit_all_data_query">
        <![CDATA[FROM WebUnitQualifyPermit]]>
    </query>
</hibernate-mapping>