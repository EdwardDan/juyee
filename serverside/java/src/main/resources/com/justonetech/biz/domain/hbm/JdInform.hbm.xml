<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
	<class
		name="JdInform"
		table="JD_INFORM"
	>
		
		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">false</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">质量监督告知书</meta>
        <meta attribute="tableComment">质量监督告知书</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">jd</meta>

		<id
			name="id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
		</id>

		<property
			name="bjbh"
			column="BJBH"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">报建编号</meta>
            <meta attribute="ColumnComment">报建编号</meta>
        </property>

		<property
			name="lxr"
			column="LXR"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">监督联系人</meta>
            <meta attribute="ColumnComment">监督联系人</meta>
        </property>

		<property
			name="tel"
			column="TEL"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">联系电话</meta>
            <meta attribute="ColumnComment">联系电话</meta>
        </property>

		<property
			name="zipCode"
			column="ZIP_CODE"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">邮政编码</meta>
            <meta attribute="ColumnComment">邮政编码</meta>
        </property>

		<property
			name="address"
			column="ADDRESS"
			type="string"
			not-null="false"
			length="200"
        >
            <meta attribute="ColumnName">联系地址</meta>
            <meta attribute="ColumnComment">联系地址</meta>
        </property>

		<property
			name="fax"
			column="FAX"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">传真号码</meta>
            <meta attribute="ColumnComment">传真号码</meta>
        </property>

		<property
			name="description"
			column="DESCRIPTION"
			type="string"
			not-null="false"
        >
            <meta attribute="ColumnName">备注</meta>
            <meta attribute="ColumnComment">备注</meta>
        </property>

		<property
			name="status"
			column="STATUS"
			type="integer"
			not-null="false"
			length="2"
        >
            <meta attribute="ColumnName">状态</meta>
            <meta attribute="ColumnComment">状态</meta>
        </property>

		<property
			name="auditOpinion"
			column="AUDIT_OPINION"
			type="string"
			not-null="false"
        >
            <meta attribute="ColumnName">科长审核意见</meta>
            <meta attribute="ColumnComment">科长审核意见</meta>
        </property>

		<property
			name="auditTime"
			column="AUDIT_TIME"
			type="timestamp"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">科长审核时间</meta>
            <meta attribute="ColumnComment">科长审核时间</meta>
        </property>

		<property
			name="auditUser"
			column="AUDIT_USER"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">科长审核用户名</meta>
            <meta attribute="ColumnComment">科长审核用户名</meta>
        </property>

		<property
			name="fgOpinion"
			column="FG_OPINION"
			type="string"
			not-null="false"
        >
            <meta attribute="ColumnName">分管领导审核意见</meta>
            <meta attribute="ColumnComment">分管领导审核意见</meta>
        </property>

		<property
			name="fgAuditTime"
			column="FG_AUDIT_TIME"
			type="timestamp"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">分管领导审核时间</meta>
            <meta attribute="ColumnComment">分管领导审核时间</meta>
        </property>

		<property
			name="fgAuditUser"
			column="FG_AUDIT_USER"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">分管领导审核用户名</meta>
            <meta attribute="ColumnComment">分管领导审核用户名</meta>
        </property>

		<property
			name="createTime"
			column="CREATE_TIME"
			type="timestamp"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">创建时间</meta>
            <meta attribute="ColumnComment">创建时间</meta>
        </property>

		<property
			name="createUser"
			column="CREATE_USER"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">创建用户名</meta>
            <meta attribute="ColumnComment">创建用户名</meta>
        </property>

		<property
			name="updateTime"
			column="UPDATE_TIME"
			type="timestamp"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">更新时间</meta>
            <meta attribute="ColumnComment">更新时间</meta>
        </property>

		<property
			name="updateUser"
			column="UPDATE_USER"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">更新用户名</meta>
            <meta attribute="ColumnComment">更新用户名</meta>
        </property>

		<property
			name="sgxkDate"
			column="SGXK_DATE"
			type="date"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">施工许可发放日期</meta>
            <meta attribute="ColumnComment">施工许可发放日期</meta>
        </property>

		<many-to-one
			name="document"
			column="DOCUMENT_ID"
			class="DocDocument"
			not-null="false"
		>
		</many-to-one>

	</class>	
    <query name="jdinform_all_data_query">
        <![CDATA[FROM JdInform]]>
    </query>
</hibernate-mapping>