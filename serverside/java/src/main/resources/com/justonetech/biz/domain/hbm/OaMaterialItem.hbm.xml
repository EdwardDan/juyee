<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
	<class
		name="OaMaterialItem"
		table="OA_MATERIAL_ITEM"
	>
		
		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">false</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">物资设备采购管理明细</meta>
        <meta attribute="tableComment">物资设备采购管理明细</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">oa</meta>

		<id
			name="id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
		</id>

		<property
			name="materialName"
			column="MATERIAL_NAME"
			type="string"
			not-null="false"
			length="200"
        >
            <meta attribute="ColumnName">物资设备名称</meta>
            <meta attribute="ColumnComment">物资设备名称</meta>
        </property>

		<property
			name="materialModel"
			column="MATERIAL_MODEL"
			type="string"
			not-null="false"
			length="500"
        >
            <meta attribute="ColumnName">具体规格型号</meta>
            <meta attribute="ColumnComment">具体规格型号</meta>
        </property>

		<property
			name="supplier"
			column="SUPPLIER"
			type="string"
			not-null="false"
			length="500"
        >
            <meta attribute="ColumnName">供应商情况</meta>
            <meta attribute="ColumnComment">供应商情况</meta>
        </property>

		<property
			name="materialNum"
			column="MATERIAL_NUM"
			type="java.lang.Double"
			not-null="false"
			length="15"
        >
            <meta attribute="ColumnName">数量</meta>
            <meta attribute="ColumnComment">数量</meta>
        </property>

		<property
			name="materialUnit"
			column="MATERIAL_UNIT"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">单位</meta>
            <meta attribute="ColumnComment">单位</meta>
        </property>

		<property
			name="description"
			column="DESCRIPTION"
			type="string"
			not-null="false"
			length="500"
        >
            <meta attribute="ColumnName">备注</meta>
            <meta attribute="ColumnComment">备注</meta>
        </property>

		<many-to-one
			name="material"
			column="MATERIAL_ID"
			class="OaMaterial"
			not-null="false"
		>
		</many-to-one>

	</class>	
    <query name="oamaterialitem_all_data_query">
        <![CDATA[FROM OaMaterialItem]]>
    </query>
</hibernate-mapping>