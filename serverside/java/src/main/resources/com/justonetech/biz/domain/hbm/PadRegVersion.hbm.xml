<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
	<class
		name="PadRegVersion"
		table="PAD_REG_VERSION"
	>
		
		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">false</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">终端注册版本信息</meta>
        <meta attribute="tableComment">终端注册版本信息</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">pad</meta>

		<id
			name="id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
		</id>

		<property
			name="padImei"
			column="PAD_IMEI"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">设备标识码</meta>
            <meta attribute="ColumnComment">设备标识码</meta>
        </property>

		<property
			name="versionCode"
			column="VERSION_CODE"
			type="string"
			not-null="false"
			length="10"
        >
            <meta attribute="ColumnName">程序版本号</meta>
            <meta attribute="ColumnComment">程序版本号</meta>
        </property>

		<property
			name="versionType"
			column="VERSION_TYPE"
			type="java.lang.Long"
			not-null="false"
			length="10"
        >
            <meta attribute="ColumnName">版本类型</meta>
            <meta attribute="ColumnComment">版本类型</meta>
        </property>

		<property
			name="updateTime"
			column="UPDATE_TIME"
			type="timestamp"
			not-null="false"
			length="19"
        >
            <meta attribute="ColumnName">更新时间</meta>
            <meta attribute="ColumnComment">更新时间</meta>
        </property>


	</class>	
    <query name="padregversion_all_data_query">
        <![CDATA[FROM PadRegVersion]]>
    </query>
</hibernate-mapping>