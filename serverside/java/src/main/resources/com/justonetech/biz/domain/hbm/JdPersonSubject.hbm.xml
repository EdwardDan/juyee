<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
	<class
		name="JdPersonSubject"
		table="JD_PERSON_SUBJECT"
	>
		
		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">false</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">监督人员专业管理</meta>
        <meta attribute="tableComment">监督人员专业管理</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">report</meta>

		<id
			name="id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
		</id>

		<property
			name="certificateCode"
			column="CERTIFICATE_CODE"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">监督证书编号</meta>
            <meta attribute="ColumnComment">监督证书编号</meta>
        </property>

		<property
			name="memo"
			column="MEMO"
			type="string"
			not-null="false"
			length="200"
        >
            <meta attribute="ColumnName">备注</meta>
            <meta attribute="ColumnComment">备注</meta>
        </property>

		<property
			name="createTime"
			column="CREATE_TIME"
			type="timestamp"
			not-null="false"
			length="19"
        >
            <meta attribute="ColumnName">创建时间</meta>
            <meta attribute="ColumnComment">创建时间</meta>
        </property>
        
        <property
			name="createUser"
			column="CREATE_USER"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">创建人</meta>
            <meta attribute="ColumnComment">创建人</meta>
        </property>

		<many-to-one
			name="person"
			column="PERSON_ID"
			class="com.justonetech.system.domain.SysPerson"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="subject"
			column="SUBJECT_ID"
			class="com.justonetech.system.domain.SysCodeDetail"
			not-null="false"
		>
		</many-to-one>

	</class>	
    <query name="jdpersonsubject_all_data_query">
        <![CDATA[FROM JdPersonSubject]]>
    </query>
</hibernate-mapping>