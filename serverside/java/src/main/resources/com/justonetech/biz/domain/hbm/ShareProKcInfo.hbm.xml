            <meta attribute="ColumnName">中标日期</meta>
            <meta attribute="ColumnComment">中标日期</meta>
        </property>

		<property
			name="kcFee"
			column="KC_FEE"
			type="java.lang.Double"
			not-null="false"
			length="15"
