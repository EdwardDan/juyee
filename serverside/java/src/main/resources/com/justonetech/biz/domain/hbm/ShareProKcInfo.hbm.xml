        <meta attribute="tableComment">项目勘察承发包信息</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">share</meta>

		<composite-id name="id" class="ShareProKcInfoPK">
