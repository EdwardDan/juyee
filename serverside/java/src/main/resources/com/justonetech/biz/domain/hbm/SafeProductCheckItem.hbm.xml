<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
	<class
		name="SafeProductCheckItem"
		table="SAFE_PRODUCT_CHECK_ITEM"
	>
		
		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">false</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">安全生产季度考核明细</meta>
        <meta attribute="tableComment">安全生产季度考核明细</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">safe</meta>

		<id
			name="id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
		</id>

		<property
			name="score"
			column="SCORE"
			type="integer"
			not-null="false"
			length="5"
        >
            <meta attribute="ColumnName">扣分</meta>
            <meta attribute="ColumnComment">扣分</meta>
        </property>

		<many-to-one
			name="productCheck"
			column="PRODUCT_CHECK_ID"
			class="SafeProductCheck"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="productStandard"
			column="PRODUCT_STANDARD_ID"
			class="SafeProductStandard"
			not-null="false"
		>
		</many-to-one>

	</class>	
    <query name="safeproductcheckitem_all_data_query">
        <![CDATA[FROM SafeProductCheckItem]]>
    </query>
</hibernate-mapping>