<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
	<class
		name="WebPersonPerformance"
		table="WEB_PERSON_PERFORMANCE"
	>
		
		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">false</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">人员业绩</meta>
        <meta attribute="tableComment">人员业绩</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">web</meta>

		<id
			name="id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
		</id>

		<property
			name="projectName"
			column="PROJECT_NAME"
			type="string"
			not-null="false"
			length="500"
        >
            <meta attribute="ColumnName">项目名称</meta>
            <meta attribute="ColumnComment">项目名称</meta>
        </property>

		<property
			name="projectSummary"
			column="PROJECT_SUMMARY"
			type="string"
			not-null="false"
			length="1000"
        >
            <meta attribute="ColumnName">项目规模及技术指标</meta>
            <meta attribute="ColumnComment">项目规模及技术指标</meta>
        </property>

		<property
			name="startEndTime"
			column="START_END_TIME"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">起止时间</meta>
            <meta attribute="ColumnComment">起止时间</meta>
        </property>

		<property
			name="responsibility"
			column="RESPONSIBILITY"
			type="string"
			not-null="false"
			length="1000"
        >
            <meta attribute="ColumnName">项目中作用</meta>
            <meta attribute="ColumnComment">项目中作用</meta>
        </property>

		<property
			name="completeUnitLevel"
			column="COMPLETE_UNIT_LEVEL"
			type="string"
			not-null="false"
			length="500"
        >
            <meta attribute="ColumnName">完成项目的单位名称和资质等级</meta>
            <meta attribute="ColumnComment">完成项目的单位名称和资质等级</meta>
        </property>

		<many-to-one
			name="unitPersonInfo"
			column="UNIT_PERSON_INFO_ID"
			class="WebUnitPersonInfo"
			not-null="false"
		>
		</many-to-one>

	</class>	
    <query name="webpersonperformance_all_data_query">
        <![CDATA[FROM WebPersonPerformance]]>
    </query>
</hibernate-mapping>