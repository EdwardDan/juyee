<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
	<class
		name="JdRecoverForm"
		table="JD_RECOVER_FORM"
	>
		
		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">false</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">安全质量监督--复工通知单</meta>
        <meta attribute="tableComment">安全质量监督--复工通知单</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">jd</meta>

		<id
			name="id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
		</id>

		<property
			name="year"
			column="YEAR"
			type="integer"
			not-null="false"
			length="4"
        >
            <meta attribute="ColumnName">年份</meta>
            <meta attribute="ColumnComment">年份</meta>
        </property>

		<property
			name="orderNo"
			column="ORDER_NO"
			type="string"
			not-null="false"
			length="4"
        >
            <meta attribute="ColumnName">序号</meta>
            <meta attribute="ColumnComment">序号</meta>
        </property>

		<property
			name="dutyDept"
			column="DUTY_DEPT"
			type="string"
			not-null="false"
			length="200"
        >
            <meta attribute="ColumnName">责任主体</meta>
            <meta attribute="ColumnComment">责任主体</meta>
        </property>

		<property
			name="projectName"
			column="PROJECT_NAME"
			type="string"
			not-null="false"
			length="200"
        >
            <meta attribute="ColumnName">工程名称(冗余)</meta>
            <meta attribute="ColumnComment">工程名称(冗余)</meta>
        </property>

		<property
			name="reformBeginDate"
			column="REFORM_BEGIN_DATE"
			type="date"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">整改起始时间</meta>
            <meta attribute="ColumnComment">整改起始时间</meta>
        </property>

		<property
			name="recoverDate"
			column="RECOVER_DATE"
			type="date"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">恢复施工时间</meta>
            <meta attribute="ColumnComment">恢复施工时间</meta>
        </property>

		<property
			name="signDept"
			column="SIGN_DEPT"
			type="string"
			not-null="false"
			length="200"
        >
            <meta attribute="ColumnName">签发单位</meta>
            <meta attribute="ColumnComment">签发单位</meta>
        </property>

		<property
			name="signPerson"
			column="SIGN_PERSON"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">签发人</meta>
            <meta attribute="ColumnComment">签发人</meta>
        </property>

		<property
			name="signDate"
			column="SIGN_DATE"
			type="date"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">签发日期</meta>
            <meta attribute="ColumnComment">签发日期</meta>
        </property>

		<property
			name="description"
			column="DESCRIPTION"
			type="string"
			not-null="false"
        >
            <meta attribute="ColumnName">备注</meta>
            <meta attribute="ColumnComment">备注</meta>
        </property>

		<property
			name="status"
			column="STATUS"
			type="integer"
			not-null="false"
			length="2"
        >
            <meta attribute="ColumnName">状态</meta>
            <meta attribute="ColumnComment">状态</meta>
        </property>

		<property
			name="createTime"
			column="CREATE_TIME"
			type="timestamp"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">创建时间</meta>
            <meta attribute="ColumnComment">创建时间</meta>
        </property>

		<property
			name="createUser"
			column="CREATE_USER"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">创建用户名</meta>
            <meta attribute="ColumnComment">创建用户名</meta>
        </property>

		<property
			name="updateTime"
			column="UPDATE_TIME"
			type="timestamp"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">更新时间</meta>
            <meta attribute="ColumnComment">更新时间</meta>
        </property>

		<property
			name="updateUser"
			column="UPDATE_USER"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">更新用户名</meta>
            <meta attribute="ColumnComment">更新用户名</meta>
        </property>

        <many-to-one
                name="document"
                column="DOCUMENT_ID"
                class="DocDocument"
                not-null="false"
                >
        </many-to-one>

		<many-to-one
			name="stopForm"
			column="STOP_FORM_ID"
			class="JdStopOrder"
			not-null="false"
		>
		</many-to-one>

	</class>	
    <query name="jdrecoverform_all_data_query">
        <![CDATA[FROM JdRecoverForm]]>
    </query>
</hibernate-mapping>