<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
    <class
            name="PadEventLog"
            table="PAD_EVENT_LOG"
            >

        <meta attribute="sync-DAO">false</meta>
        <meta attribute="sync-jsp">true</meta>
        <meta attribute="sync-boolean">get</meta>
        <meta attribute="treeable">false</meta>
        <meta attribute="projectable">false</meta>
        <meta attribute="tableName">移动巡检事件日志</meta>
        <meta attribute="tableComment">移动巡检事件日志</meta>
        <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">pad</meta>

        <id
                name="id"
                type="java.lang.Long"
                column="ID"
                >
            <generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
        </id>

        <property
                name="padImei"
                column="PAD_IMEI"
                type="string"
                not-null="false"
                length="50"
                >
            <meta attribute="ColumnName">设备标识码</meta>
            <meta attribute="ColumnComment">设备标识码</meta>
        </property>

        <property
                name="eventMsg"
                column="EVENT_MSG"
                type="string"
                not-null="false"
                length="200"
                >
            <meta attribute="ColumnName">事件描述</meta>
            <meta attribute="ColumnComment">事件描述</meta>
        </property>

        <property
                name="eventType"
                column="EVENT_TYPE"
                type="java.lang.Long"
                not-null="false"
                length="200"
                >
            <meta attribute="ColumnName">事件类型</meta>
            <meta attribute="ColumnComment">事件类型</meta>
        </property>

        <property
                name="eventTime"
                column="EVENT_TIME"
                type="timestamp"
                not-null="false"
                length="19"
                >
            <meta attribute="ColumnName">事件发生时间</meta>
            <meta attribute="ColumnComment">事件发生时间</meta>
        </property>

        <property
                name="padCommitTime"
                column="PAD_COMMIT_TIME"
                type="timestamp"
                not-null="false"
                length="19"
                >
            <meta attribute="ColumnName">设备上传时间</meta>
            <meta attribute="ColumnComment">设备上传时间</meta>
        </property>

        <property
                name="remark"
                column="REMARK"
                type="string"
                not-null="false"
                length="200"
                >
            <meta attribute="ColumnName">备注</meta>
            <meta attribute="ColumnComment">备注</meta>
        </property>

        <property
                name="userName"
                column="USER_NAME"
                type="string"
                not-null="false"
                length="50"
                >
            <meta attribute="ColumnName">用户名</meta>
            <meta attribute="ColumnComment">用户名</meta>
        </property>

        <!--<many-to-one-->
        <!--name="eventType"-->
        <!--column="EVENT_TYPE"-->
        <!--class="com.justonetech.system.domain.SysCodeDetail"-->
        <!--not-null="false"-->
        <!-->-->
        <!--</many-to-one>-->

    </class>
    <query name="padeventlog_all_data_query">
        <![CDATA[FROM PadEventLog]]>
    </query>
</hibernate-mapping>