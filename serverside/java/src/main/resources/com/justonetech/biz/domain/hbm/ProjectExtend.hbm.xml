<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
	<class
		name="ProjectExtend"
		table="PROJECT_EXTEND"
	>
		
		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">false</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">项目扩展信息</meta>
        <meta attribute="tableComment">项目扩展信息</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">project</meta>

		<id
			name="id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
		</id>

		<property
			name="bjbh"
			column="BJBH"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">报建编号</meta>
            <meta attribute="ColumnComment">报建编号</meta>
        </property>

		<property
			name="belongArea"
			column="BELONG_AREA"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">项目属地</meta>
            <meta attribute="ColumnComment">项目属地</meta>
        </property>

		<property
			name="posX"
			column="POS_X"
			type="java.lang.Double"
			not-null="false"
			length="15"
        >
            <meta attribute="ColumnName">坐标X</meta>
            <meta attribute="ColumnComment">坐标X</meta>
        </property>

		<property
			name="posY"
			column="POS_Y"
			type="java.lang.Double"
			not-null="false"
			length="15"
        >
            <meta attribute="ColumnName">坐标Y</meta>
            <meta attribute="ColumnComment">坐标Y</meta>
        </property>
        <property
                name="completeDate"
                column="COMPLETE_DATE"
                type="timestamp"
                not-null="false"
                length="7"
                >
            <meta attribute="ColumnName">竣工时间</meta>
            <meta attribute="ColumnComment">竣工时间</meta>
        </property>
        <property
                name="description"
                column="DESCRIPTION"
                type="string"
                not-null="false"
                length="2000"
                >
            <meta attribute="ColumnName">竣工描述</meta>
            <meta attribute="ColumnComment">竣工描述</meta>
        </property>
		<property
			name="createTime"
			column="CREATE_TIME"
			type="timestamp"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">创建时间</meta>
            <meta attribute="ColumnComment">创建时间</meta>
        </property>

		<property
			name="createUser"
			column="CREATE_USER"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">创建用户名</meta>
            <meta attribute="ColumnComment">创建用户名</meta>
        </property>

		<property
			name="updateTime"
			column="UPDATE_TIME"
			type="timestamp"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">更新时间</meta>
            <meta attribute="ColumnComment">更新时间</meta>
        </property>

		<property
			name="updateUser"
			column="UPDATE_USER"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">更新用户名</meta>
            <meta attribute="ColumnComment">更新用户名</meta>
        </property>


	</class>	
    <query name="projectextend_all_data_query">
        <![CDATA[FROM ProjectExtend]]>
    </query>
</hibernate-mapping>