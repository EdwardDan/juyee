<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
	<class
		name="OaExpertiseUser"
		table="OA_EXPERTISE_USER"
	>
		
		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">false</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">专业知识学习参与人员</meta>
        <meta attribute="tableComment">专业知识学习参与人员</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">oa</meta>

		<id
			name="id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
		</id>

		<property
			name="isRead"
			column="IS_READ"
			type="java.lang.Boolean"
			not-null="false"
			length="1"
        >
            <meta attribute="ColumnName">是否已读</meta>
            <meta attribute="ColumnComment">是否已读</meta>
        </property>

		<property
			name="readTime"
			column="READ_TIME"
			type="timestamp"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">阅读时间</meta>
            <meta attribute="ColumnComment">阅读时间</meta>
        </property>

		<many-to-one
			name="expertise"
			column="EXPERTISE_ID"
			class="OaExpertise"
			not-null="false"
		>
		</many-to-one>

        <many-to-one
                name="user"
                column="USER_ID"
                class="com.justonetech.system.domain.SysUser"
                not-null="false"
                >
        </many-to-one>


    </class>
    <query name="oaexpertiseuser_all_data_query">
        <![CDATA[FROM OaExpertiseUser]]>
    </query>
</hibernate-mapping>