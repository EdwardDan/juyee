<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
	<class
		name="NetAcceptStep"
		table="NET_ACCEPT_STEP"
	>
		
		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">false</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">项目审批_受理环节设置</meta>
        <meta attribute="tableComment">项目审批_受理环节设置</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">net</meta>

		<id
			name="id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
		</id>

		<property
			name="no"
			column="NO"
			type="integer"
			not-null="false"
			length="3"
        >
            <meta attribute="ColumnName">序号</meta>
            <meta attribute="ColumnComment">序号</meta>
        </property>

		<property
			name="matterCode"
			column="MATTER_CODE"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">审批事项编码</meta>
            <meta attribute="ColumnComment">审批事项编码</meta>
        </property>

        <property
                name="code"
                column="CODE"
                type="string"
                not-null="false"
                length="50"
                >
            <meta attribute="ColumnName">受理环节编码</meta>
            <meta attribute="ColumnComment">受理环节编码</meta>
        </property>

		<property
			name="name"
			column="NAME"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">受理环节名称</meta>
            <meta attribute="ColumnComment">受理环节名称</meta>
        </property>

		<property
			name="note"
			column="NOTE"
			type="string"
			not-null="false"
			length="500"
        >
            <meta attribute="ColumnName">说明</meta>
            <meta attribute="ColumnComment">说明</meta>
        </property>

        <property
                name="isShowButton"
                column="IS_SHOW_BUTTON"
                type="java.lang.Boolean"
                not-null="false"
                length="1"
                >
            <meta attribute="ColumnName">是否显示通过退回按钮</meta>
            <meta attribute="ColumnComment">是否显示通过退回按钮</meta>
        </property>

		<set name="netAcceptStatus" inverse="true" order-by="no">
			<key column="ACCEPT_STEP_ID"/>
			<one-to-many class="NetAcceptStatus"/>
		</set>

	</class>	
    <query name="netacceptstep_all_data_query">
        <![CDATA[FROM NetAcceptStep]]>
    </query>
</hibernate-mapping>