<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
	<class
		name="JdJlReport"
		table="JD_JL_REPORT"
	>
		
		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">false</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">监理报告</meta>
        <meta attribute="tableComment">监理报告</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">jd</meta>

		<id
			name="id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
		</id>

		<property
			name="bjbh"
			column="BJBH"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">报建编号</meta>
            <meta attribute="ColumnComment">报建编号</meta>
        </property>

		<property
			name="reportTypeDesc"
			column="REPORT_TYPE_DESC"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">报告类型中文描述</meta>
            <meta attribute="ColumnComment">报告类型中文描述</meta>
        </property>

        <property
                name="jlDeptName"
                column="JL_DEPT_NAME"
                type="string"
                not-null="false"
                length="200"
                >
            <meta attribute="ColumnName">监理单位名称</meta>
            <meta attribute="ColumnComment">监理单位名称</meta>
        </property>

		<property
			name="reportDate"
			column="REPORT_DATE"
			type="date"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">提交日期</meta>
            <meta attribute="ColumnComment">提交日期</meta>
        </property>

		<property
			name="description"
			column="DESCRIPTION"
			type="string"
			not-null="false"
        >
            <meta attribute="ColumnName">备注</meta>
            <meta attribute="ColumnComment">备注</meta>
        </property>

		<property
			name="createTime"
			column="CREATE_TIME"
			type="timestamp"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">创建时间</meta>
            <meta attribute="ColumnComment">创建时间</meta>
        </property>

		<property
			name="createUser"
			column="CREATE_USER"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">创建用户名</meta>
            <meta attribute="ColumnComment">创建用户名</meta>
        </property>

		<property
			name="updateTime"
			column="UPDATE_TIME"
			type="timestamp"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">更新时间</meta>
            <meta attribute="ColumnComment">更新时间</meta>
        </property>

		<property
			name="updateUser"
			column="UPDATE_USER"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">更新用户名</meta>
            <meta attribute="ColumnComment">更新用户名</meta>
        </property>

		<many-to-one
			name="reportType"
			column="REPORT_TYPE_ID"
			class="com.justonetech.system.domain.SysCodeDetail"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="document"
			column="DOCUMENT_ID"
			class="DocDocument"
			not-null="false"
		>
		</many-to-one>

	</class>	
    <query name="jdjlreport_all_data_query">
        <![CDATA[FROM JdJlReport]]>
    </query>
</hibernate-mapping>