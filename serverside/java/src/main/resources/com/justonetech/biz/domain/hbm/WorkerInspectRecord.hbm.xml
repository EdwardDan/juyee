<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
	<class
		name="WorkerInspectRecord"
		table="WORKER_INSPECT_RECORD"
	>
		
		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">false</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">现场检查记录</meta>
        <meta attribute="tableComment">现场检查记录</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">worker</meta>

		<id
			name="id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
		</id>

		<property
			name="bjbh"
			column="BJBH"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">报建编号</meta>
            <meta attribute="ColumnComment">报建编号</meta>
        </property>

		<property
			name="checkAddress"
			column="CHECK_ADDRESS"
			type="string"
			not-null="false"
			length="200"
        >
            <meta attribute="ColumnName">检查地点</meta>
            <meta attribute="ColumnComment">检查地点</meta>
        </property>

		<property
			name="checkDate"
			column="CHECK_DATE"
			type="date"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">检查日期</meta>
            <meta attribute="ColumnComment">检查日期</meta>
        </property>

		<property
			name="startTime"
			column="START_TIME"
			type="string"
			not-null="false"
			length="10"
        >
            <meta attribute="ColumnName">检查起始时分</meta>
            <meta attribute="ColumnComment">检查起始时分</meta>
        </property>

		<property
			name="endTime"
			column="END_TIME"
			type="string"
			not-null="false"
			length="10"
        >
            <meta attribute="ColumnName">检查结束时分</meta>
            <meta attribute="ColumnComment">检查结束时分</meta>
        </property>

		<property
			name="currentPerson"
			column="CURRENT_PERSON"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">当事人</meta>
            <meta attribute="ColumnComment">当事人</meta>
        </property>

		<property
			name="address"
			column="ADDRESS"
			type="string"
			not-null="false"
			length="200"
        >
            <meta attribute="ColumnName">地址</meta>
            <meta attribute="ColumnComment">地址</meta>
        </property>

		<property
			name="fr"
			column="FR"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">法定代表人</meta>
            <meta attribute="ColumnComment">法定代表人</meta>
        </property>

		<property
			name="tel"
			column="TEL"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">电话</meta>
            <meta attribute="ColumnComment">电话</meta>
        </property>

		<property
			name="checkPerson"
			column="CHECK_PERSON"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">检查人员</meta>
            <meta attribute="ColumnComment">检查人员</meta>
        </property>

		<property
			name="recordPerson"
			column="RECORD_PERSON"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">记录人</meta>
            <meta attribute="ColumnComment">记录人</meta>
        </property>

		<property
			name="checkContent"
			column="CHECK_CONTENT"
			type="string"
			not-null="false"
        >
            <meta attribute="ColumnName">现场检查情况</meta>
            <meta attribute="ColumnComment">现场检查情况</meta>
        </property>

		<property
			name="isFromAndroid"
			column="IS_FROM_ANDROID"
			type="java.lang.Boolean"
			not-null="false"
			length="1"
        >
            <meta attribute="ColumnName">是否来源手持终端</meta>
            <meta attribute="ColumnComment">是否来源手持终端</meta>
        </property>

		<property
			name="createTime"
			column="CREATE_TIME"
			type="timestamp"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">创建时间</meta>
            <meta attribute="ColumnComment">创建时间</meta>
        </property>

		<property
			name="createUser"
			column="CREATE_USER"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">创建用户名</meta>
            <meta attribute="ColumnComment">创建用户名</meta>
        </property>

		<property
			name="updateTime"
			column="UPDATE_TIME"
			type="timestamp"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">更新时间</meta>
            <meta attribute="ColumnComment">更新时间</meta>
        </property>

		<property
			name="updateUser"
			column="UPDATE_USER"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">更新用户名</meta>
            <meta attribute="ColumnComment">更新用户名</meta>
        </property>


	</class>	
    <query name="workerinspectrecord_all_data_query">
        <![CDATA[FROM WorkerInspectRecord]]>
    </query>
</hibernate-mapping>