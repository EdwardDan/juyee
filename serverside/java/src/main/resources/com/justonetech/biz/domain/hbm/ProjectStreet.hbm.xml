<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
	<class
		name="ProjectStreet"
		table="PROJECT_STREET"
	>
		
		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">false</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">行政区街镇划分</meta>
        <meta attribute="tableComment">行政区街镇划分</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">project</meta>

		<id
			name="id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
		</id>

		<property
			name="year"
			column="YEAR"
			type="integer"
			not-null="false"
			length="4"
        >
            <meta attribute="ColumnName">年份</meta>
            <meta attribute="ColumnComment">年份</meta>
        </property>

		<property
			name="streetName"
			column="STREET_NAME"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">街镇名称</meta>
            <meta attribute="ColumnComment">街镇名称</meta>
        </property>

		<property
			name="color"
			column="COLOR"
			type="string"
			not-null="false"
			length="10"
        >
            <meta attribute="ColumnName">颜色</meta>
            <meta attribute="ColumnComment">颜色</meta>
        </property>

		<property
			name="pointData"
			column="POINT_DATA"
			type="string"
			not-null="false"
        >
            <meta attribute="ColumnName">点数据</meta>
            <meta attribute="ColumnComment">点数据</meta>
        </property>

		<property
			name="createTime"
			column="CREATE_TIME"
			type="timestamp"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">创建时间</meta>
            <meta attribute="ColumnComment">创建时间</meta>
        </property>

		<property
			name="createUser"
			column="CREATE_USER"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">创建用户名</meta>
            <meta attribute="ColumnComment">创建用户名</meta>
        </property>

		<property
			name="updateTime"
			column="UPDATE_TIME"
			type="timestamp"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">更新时间</meta>
            <meta attribute="ColumnComment">更新时间</meta>
        </property>

		<property
			name="updateUser"
			column="UPDATE_USER"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">更新用户名</meta>
            <meta attribute="ColumnComment">更新用户名</meta>
        </property>

		<many-to-one
			name="street"
			column="STREET_ID"
			class="com.justonetech.system.domain.SysCodeDetail"
			not-null="false"
		>
		</many-to-one>

	</class>	
    <query name="projectstreet_all_data_query">
        <![CDATA[FROM ProjectStreet]]>
    </query>
</hibernate-mapping>