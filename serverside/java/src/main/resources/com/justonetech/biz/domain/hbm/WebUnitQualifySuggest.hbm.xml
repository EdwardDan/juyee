<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
	<class
		name="WebUnitQualifySuggest"
		table="WEB_UNIT_QUALIFY_SUGGEST"
	>
		
		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">false</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">企业资质审查意见公示</meta>
        <meta attribute="tableComment">企业资质审查意见公示</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">web</meta>

		<id
			name="id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
		</id>

		<property
			name="unitName"
			column="UNIT_NAME"
			type="string"
			not-null="false"
			length="500"
        >
            <meta attribute="ColumnName">企业名称</meta>
            <meta attribute="ColumnComment">企业名称</meta>
        </property>

		<property
			name="applys"
			column="APPLYS"
			type="string"
			not-null="false"
			length="500"
        >
            <meta attribute="ColumnName">申请事项</meta>
            <meta attribute="ColumnComment">申请事项</meta>
        </property>

		<property
			name="acceptNo"
			column="ACCEPT_NO"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">受理批次</meta>
            <meta attribute="ColumnComment">受理批次</meta>
        </property>

		<property
			name="publicDate"
			column="PUBLIC_DATE"
			type="date"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">公示时间</meta>
            <meta attribute="ColumnComment">公示时间</meta>
        </property>

	

		<set name="webQualifySuggestResults" inverse="true">
			<key column="QUALIFY_SUGGEST_ID"/>
			<one-to-many class="WebQualifySuggestResults"/>
		</set>

	</class>	
    <query name="webunitqualifysuggest_all_data_query">
        <![CDATA[FROM WebUnitQualifySuggest]]>
    </query>
</hibernate-mapping>