<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
	<class
		name="NetApply"
		table="NET_APPLY"
	>
		
		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">false</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">项目报建</meta>
        <meta attribute="tableComment">项目报建</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">net</meta>

		<id
			name="id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
		</id>

		<property
			name="netCode"
			column="NET_CODE"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">网上受理编号</meta>
            <meta attribute="ColumnComment">网上受理编号</meta>
        </property>

		<property
			name="netDate"
			column="NET_DATE"
			type="date"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">网上提交日期</meta>
            <meta attribute="ColumnComment">网上提交日期</meta>
        </property>

		<property
			name="bjbh"
			column="BJBH"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">报建编号</meta>
            <meta attribute="ColumnComment">报建编号</meta>
        </property>

		<property
			name="projectName"
			column="PROJECT_NAME"
			type="string"
			not-null="false"
			length="200"
        >
            <meta attribute="ColumnName">项目名称</meta>
            <meta attribute="ColumnComment">项目名称</meta>
        </property>

		<property
			name="buildUnitName"
			column="BUILD_UNIT_NAME"
			type="string"
			not-null="false"
			length="200"
        >
            <meta attribute="ColumnName">建设单位</meta>
            <meta attribute="ColumnComment">建设单位</meta>
        </property>

		<property
			name="buildAddress"
			column="BUILD_ADDRESS"
			type="string"
			not-null="false"
			length="200"
        >
            <meta attribute="ColumnName">建设地点</meta>
            <meta attribute="ColumnComment">建设地点</meta>
        </property>

		<property
			name="applyUnitName"
			column="APPLY_UNIT_NAME"
			type="string"
			not-null="false"
			length="200"
        >
            <meta attribute="ColumnName">申请单位</meta>
            <meta attribute="ColumnComment">申请单位</meta>
        </property>

		<property
			name="isCheckDraw"
			column="IS_CHECK_DRAW"
			type="java.lang.Boolean"
			not-null="false"
			length="1"
        >
            <meta attribute="ColumnName">是否需要施工图审查</meta>
            <meta attribute="ColumnComment">是否需要施工图审查</meta>
        </property>

		<property
			name="isImportant"
			column="IS_IMPORTANT"
			type="java.lang.Boolean"
			not-null="false"
			length="1"
        >
            <meta attribute="ColumnName">是否重大推进项目</meta>
            <meta attribute="ColumnComment">是否重大推进项目</meta>
        </property>

		<property
			name="createTime"
			column="CREATE_TIME"
			type="timestamp"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">创建时间</meta>
            <meta attribute="ColumnComment">创建时间</meta>
        </property>

		<property
			name="createUser"
			column="CREATE_USER"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">创建用户名</meta>
            <meta attribute="ColumnComment">创建用户名</meta>
        </property>

		<property
			name="updateTime"
			column="UPDATE_TIME"
			type="timestamp"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">更新时间</meta>
            <meta attribute="ColumnComment">更新时间</meta>
        </property>

		<property
			name="updateUser"
			column="UPDATE_USER"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">更新用户名</meta>
            <meta attribute="ColumnComment">更新用户名</meta>
        </property>

		<many-to-one
			name="acceptStatus"
			column="ACCEPT_STATUS_ID"
			class="NetAcceptStatus"
			not-null="false"
		>
		</many-to-one>
	
        <set name="netMonitors" inverse="true">
            <key column="APPLY_ID"/>
            <one-to-many class="NetMonitor"/>
        </set>

        <set name="netApplyFbTypes" inverse="true">
            <key column="APPLY_ID"/>
            <one-to-many class="NetApplyFbType"/>
        </set>

        <set name="netCfbs" inverse="true">
            <key column="APPLY_ID"/>
            <one-to-many class="NetCfb"/>
        </set>

        <set name="netContracts" inverse="true">
            <key column="APPLY_ID"/>
            <one-to-many class="NetContract"/>
        </set>

        <set name="netSgpermits" inverse="true">
            <key column="APPLY_ID"/>
            <one-to-many class="NetSgpermit"/>
        </set>

	</class>	
    <query name="netapply_all_data_query">
        <![CDATA[FROM NetApply]]>
    </query>
</hibernate-mapping>