<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
	<class
		name="OrderFormDeployUser"
		table="ORDER_FORM_DEPLOY_USER"
	>
		
		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">false</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">监督预约监督人员</meta>
        <meta attribute="tableComment">监督预约监督人员</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">order</meta>

		<id
			name="id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
		</id>

		<property
			name="isManager"
			column="IS_MANAGER"
			type="java.lang.Boolean"
			not-null="false"
			length="1"
        >
            <meta attribute="ColumnName">是否负责人</meta>
            <meta attribute="ColumnComment">是否负责人</meta>
        </property>

		<many-to-one
			name="orderFormDeploy"
			column="ORDER_FORM_DEPLOY_ID"
			class="OrderFormDeploy"
			not-null="false"
		>
		</many-to-one>
        <many-to-one
                name="user"
                column="USER_ID"
                class="com.justonetech.system.domain.SysUser"
                not-null="false"
                >
        </many-to-one>

	</class>	
    <query name="orderformdeployuser_all_data_query">
        <![CDATA[FROM OrderFormDeployUser]]>
    </query>
</hibernate-mapping>