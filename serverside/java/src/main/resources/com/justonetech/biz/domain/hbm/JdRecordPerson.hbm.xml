<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
	<class
		name="JdRecordPerson"
		table="JD_RECORD_PERSON"
	>
		
		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">false</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">安全质量监督--监督人员</meta>
        <meta attribute="tableComment">安全质量监督--监督人员</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">jd</meta>

		<id
			name="id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
		</id>

		<property
			name="recordId"
			column="RECORD_ID"
			type="java.lang.Long"
			not-null="false"
			length="10"
        >
            <meta attribute="ColumnName">监督id(首次/安全/质量/竣工)</meta>
            <meta attribute="ColumnComment">监督id(首次/安全/质量/竣工)</meta>
        </property>

		<property
			name="jdTypeCode"
			column="JD_TYPE_CODE"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">监督类别编码</meta>
            <meta attribute="ColumnComment">监督类别编码</meta>
        </property>

		<property
			name="jdPersonDesc"
			column="JD_PERSON_DESC"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">监督员描述</meta>
            <meta attribute="ColumnComment">监督员描述</meta>
        </property>


        <many-to-one
                name="jdPerson"
                column="JD_PERSON_ID"
                class="com.justonetech.system.domain.SysPerson"
                not-null="false"
                >
        </many-to-one>
	</class>	
    <query name="jdrecordperson_all_data_query">
        <![CDATA[FROM JdRecordPerson]]>
    </query>
</hibernate-mapping>