<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
	<class
		name="PollingLaw"
		table="POLLING_LAW"
	>
		
		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">false</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">法律法规查询</meta>
        <meta attribute="tableComment">法律法规查询</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">polling</meta>

		<id
			name="id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
		</id>

		<property
			name="stddocid"
			column="STDDOCID"
			type="java.lang.Long"
			not-null="false"
			length="10"
        >
            <meta attribute="ColumnName">关联字段ID</meta>
            <meta attribute="ColumnComment">关联字段ID</meta>
        </property>

		<property
			name="catalog"
			column="CATALOG"
			type="string"
			not-null="false"
			length="500"
        >
            <meta attribute="ColumnName">条款编号</meta>
            <meta attribute="ColumnComment">条款编号</meta>
        </property>

		<property
			name="descriptions"
			column="DESCRIPTIONS"
			type="string"
			not-null="false"
        >
            <meta attribute="ColumnName">条款内容</meta>
            <meta attribute="ColumnComment">条款内容</meta>
        </property>

		<property
			name="tksm"
			column="TKSM"
			type="string"
			not-null="false"
			length="200"
        >
            <meta attribute="ColumnName">标准名称</meta>
            <meta attribute="ColumnComment">标准名称</meta>
        </property>

		<property
			name="tkbh"
			column="TKBH"
			type="string"
			not-null="false"
			length="200"
        >
            <meta attribute="ColumnName">标准编号</meta>
            <meta attribute="ColumnComment">标准编号</meta>
        </property>

		<property
			name="lx"
			column="LX"
			type="string"
			not-null="false"
			length="200"
        >
            <meta attribute="ColumnName">分类</meta>
            <meta attribute="ColumnComment">分类</meta>
        </property>

		<property
			name="jdfl"
			column="JDFL"
			type="string"
			not-null="false"
			length="200"
        >
            <meta attribute="ColumnName">监督分类</meta>
            <meta attribute="ColumnComment">监督分类</meta>
        </property>

		<property
			name="iitemno"
			column="IITEMNO"
			type="string"
			not-null="false"
			length="200"
        >
            <meta attribute="ColumnName">序号</meta>
            <meta attribute="ColumnComment">序号</meta>
        </property>

		<property
			name="type"
			column="TYPE"
			type="integer"
			not-null="false"
			length="5"
        >
            <meta attribute="ColumnName">类型</meta>
            <meta attribute="ColumnComment">类型</meta>
        </property>

		<property
			name="shotdes"
			column="SHOTDES"
			type="string"
			not-null="false"
        >
            <meta attribute="ColumnName">条款简述</meta>
            <meta attribute="ColumnComment">条款简述</meta>
        </property>


	</class>	
    <query name="pollinglaw_all_data_query">
        <![CDATA[FROM PollingLaw]]>
    </query>
</hibernate-mapping>