<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
	<class
		name="PadVersionInfo"
		table="PAD_VERSION_INFO"
	>
		
		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">false</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">手持版本信息</meta>
        <meta attribute="tableComment">手持版本信息</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">pad</meta>

		<id
			name="id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
		</id>

		<property
			name="versionCode"
			column="VERSION_CODE"
			type="string"
			not-null="true"
			length="10"
        >
            <meta attribute="ColumnName">版本号</meta>
            <meta attribute="ColumnComment">版本号</meta>
        </property>

		<property
			name="versionForceFlage"
			column="VERSION_FORCE_FLAGE"
			type="string"
			not-null="false"
			length="10"
        >
            <meta attribute="ColumnName">强制更新标记</meta>
            <meta attribute="ColumnComment">强制更新标记</meta>
        </property>

		<property
			name="versionDesc"
			column="VERSION_DESC"
			type="string"
			not-null="false"
			length="500"
        >
            <meta attribute="ColumnName">版本描述</meta>
            <meta attribute="ColumnComment">版本描述</meta>
        </property>

		<property
			name="versionType"
			column="VERSION_TYPE"
			type="java.lang.Long"
			not-null="false"
			length="10"
        >
            <meta attribute="ColumnName">版本类型（1 普通版、2 领导版）</meta>
            <meta attribute="ColumnComment">版本类型（1 普通版、2 领导版）</meta>
        </property>

		<property
			name="createTime"
			column="CREATE_TIME"
			type="timestamp"
			not-null="false"
			length="19"
        >
            <meta attribute="ColumnName">创建时间</meta>
            <meta attribute="ColumnComment">创建时间</meta>
        </property>

		<property
			name="createUser"
			column="CREATE_USER"
			type="string"
			not-null="false"
			length="20"
        >
            <meta attribute="ColumnName">创建人</meta>
            <meta attribute="ColumnComment">创建人</meta>
        </property>

		<many-to-one
			name="apkDoc"
			column="APK_DOC_ID"
			class="DocDocument"
			not-null="false"
		>
		</many-to-one>

	</class>	
    <query name="padversioninfo_all_data_query">
        <![CDATA[FROM PadVersionInfo]]>
    </query>
</hibernate-mapping>