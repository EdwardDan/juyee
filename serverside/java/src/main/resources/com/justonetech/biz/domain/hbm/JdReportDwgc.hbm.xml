<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
	<class
		name="JdReportDwgc"
		table="JD_REPORT_DWGC"
	>
		
		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">false</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">监督报告单位工程</meta>
        <meta attribute="tableComment">监督报告单位工程</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">report</meta>

		<id
			name="id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
		</id>

		<many-to-one
			name="excelJdDwgc"
			column="Jd_task_Dwgc_ID"
			class="ExcelJdDwgc"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="jdReport"
			column="JD_REPORT_ID"
			class="JdReport"
			not-null="false"
		>
		</many-to-one>

	</class>	
    <query name="jdreportdwgc_all_data_query">
        <![CDATA[FROM JdReportDwgc]]>
    </query>
</hibernate-mapping>