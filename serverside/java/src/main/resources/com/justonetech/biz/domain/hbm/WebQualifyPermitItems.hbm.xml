<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
	<class
		name="WebQualifyPermitItems"
		table="WEB_QUALIFY_PERMIT_ITEMS"
	>
		
		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">false</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">企业资质核准条目</meta>
        <meta attribute="tableComment">企业资质核准条目</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">web</meta>

		<id
			name="id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
		</id>

		<property
			name="permitItem"
			column="PERMIT_ITEM"
			type="string"
			not-null="false"
			length="500"
        >
            <meta attribute="ColumnName">核准事项</meta>
            <meta attribute="ColumnComment">核准事项</meta>
        </property>

		<property
			name="qualifyType"
			column="QUALIFY_TYPE"
			type="string"
			not-null="false"
			length="500"
        >
            <meta attribute="ColumnName">资质类别</meta>
            <meta attribute="ColumnComment">资质类别</meta>
        </property>

		<property
			name="qualifyLevel"
			column="QUALIFY_LEVEL"
			type="string"
			not-null="false"
			length="500"
        >
            <meta attribute="ColumnName">资质等级</meta>
            <meta attribute="ColumnComment">资质等级</meta>
        </property>

		<many-to-one
			name="qualifyPermit"
			column="QUALIFY_PERMIT_ID"
			class="WebUnitQualifyPermit"
			not-null="false"
		>
		</many-to-one>

	</class>	
    <query name="webqualifypermititems_all_data_query">
        <![CDATA[FROM WebQualifyPermitItems]]>
    </query>
</hibernate-mapping>