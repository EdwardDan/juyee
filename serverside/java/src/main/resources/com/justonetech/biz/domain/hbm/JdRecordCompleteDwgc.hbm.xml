<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
	<class
		name="JdRecordCompleteDwgc"
		table="JD_RECORD_COMPLETE_DWGC"
	>
		
		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">false</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">安全质量监督--竣工验收--单位工程</meta>
        <meta attribute="tableComment">安全质量监督--竣工验收--单位工程</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">jd</meta>

		<id
			name="id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
		</id>

		<property
			name="dwgcCode"
			column="DWGC_CODE"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">单位工程号</meta>
            <meta attribute="ColumnComment">单位工程号</meta>
        </property>

		<property
			name="dwgcName"
			column="DWGC_NAME"
			type="string"
			not-null="false"
			length="200"
        >
            <meta attribute="ColumnName">单位名称名称</meta>
            <meta attribute="ColumnComment">单位名称名称</meta>
        </property>

		<many-to-one
			name="recordComplete"
			column="RECORD_COMPLETE_ID"
			class="JdRecordComplete"
			not-null="false"
		>
		</many-to-one>

	</class>	
    <query name="jdrecordcompletedwgc_all_data_query">
        <![CDATA[FROM JdRecordCompleteDwgc]]>
    </query>
</hibernate-mapping>