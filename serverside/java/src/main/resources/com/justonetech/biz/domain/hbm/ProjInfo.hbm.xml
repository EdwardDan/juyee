<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
	<class
		name="ProjInfo"
		table="PROJ_INFO"
	>
		
		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">false</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">项目基本信息管理</meta>
        <meta attribute="tableComment">项目基本信息管理</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">project</meta>

		<id
			name="id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
		</id>

		<property
			name="name"
			column="NAME"
			type="string"
			not-null="false"
			length="500"
        >
            <meta attribute="ColumnName">项目名称</meta>
            <meta attribute="ColumnComment">项目名称</meta>
        </property>

		<property
			name="year"
			column="YEAR"
			type="integer"
			not-null="false"
			length="4"
        >
            <meta attribute="ColumnName">年份</meta>
            <meta attribute="ColumnComment">年份</meta>
        </property>

		<property
			name="no"
			column="NO"
			type="integer"
			not-null="false"
			length="5"
        >
            <meta attribute="ColumnName">项目序号</meta>
            <meta attribute="ColumnComment">项目序号</meta>
        </property>

		<property
			name="buildMileage"
			column="BUILD_MILEAGE"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">建设里程</meta>
            <meta attribute="ColumnComment">建设里程</meta>
        </property>

		<property
			name="location"
			column="LOCATION"
			type="string"
			not-null="false"
			length="500"
        >
            <meta attribute="ColumnName">起讫地点</meta>
            <meta attribute="ColumnComment">起讫地点</meta>
        </property>

		<property
			name="startDate"
			column="START_DATE"
			type="date"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">开工日期</meta>
            <meta attribute="ColumnComment">开工日期</meta>
        </property>

		<property
			name="intro"
			column="INTRO"
			type="string"
			not-null="false"
        >
            <meta attribute="ColumnName">工程简介</meta>
            <meta attribute="ColumnComment">工程简介</meta>
        </property>

		<property
			name="jsDept"
			column="JS_DEPT"
			type="string"
			not-null="false"
			length="300"
        >
            <meta attribute="ColumnName">建设单位</meta>
            <meta attribute="ColumnComment">建设单位</meta>
        </property>

		<property
			name="sgDept"
			column="SG_DEPT"
			type="string"
			not-null="false"
			length="300"
        >
            <meta attribute="ColumnName">施工单位</meta>
            <meta attribute="ColumnComment">施工单位</meta>
        </property>

		<property
			name="sgDeptPerson"
			column="SG_DEPT_PERSON"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">施工单位联系人</meta>
            <meta attribute="ColumnComment">施工单位联系人</meta>
        </property>

		<property
			name="sgDeptTel"
			column="SG_DEPT_TEL"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">施工单位联系电话</meta>
            <meta attribute="ColumnComment">施工单位联系电话</meta>
        </property>

		<property
			name="jlDept"
			column="JL_DEPT"
			type="string"
			not-null="false"
			length="300"
        >
            <meta attribute="ColumnName">监理单位</meta>
            <meta attribute="ColumnComment">监理单位</meta>
        </property>

		<property
			name="jlDeptPerson"
			column="JL_DEPT_PERSON"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">监理单位联系人</meta>
            <meta attribute="ColumnComment">监理单位联系人</meta>
        </property>

		<property
			name="jlDeptTel"
			column="JL_DEPT_TEL"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">监理单位联系电话</meta>
            <meta attribute="ColumnComment">监理单位联系电话</meta>
        </property>

        <property
                name="createTime"
                column="CREATE_TIME"
                type="timestamp"
                not-null="false"
                length="16"
                >
            <meta attribute="ColumnName">创建时间</meta>
            <meta attribute="ColumnComment">创建时间</meta>
        </property>

        <property
                name="updateTime"
                column="UPDATE_TIME"
                type="timestamp"
                not-null="false"
                length="16"
                >
            <meta attribute="ColumnName">更新时间</meta>
            <meta attribute="ColumnComment">更新时间</meta>
        </property>

        <property
                name="createUser"
                column="CREATE_USER"
                type="string"
                not-null="false"
                length="100"
                >
            <meta attribute="ColumnName">创建人</meta>
            <meta attribute="ColumnComment">创建人(记录帐号）</meta>
        </property>

        <property
                name="updateUser"
                column="UPDATE_USER"
                type="string"
                not-null="false"
                length="100"
                >
            <meta attribute="ColumnName">更新人</meta>
            <meta attribute="ColumnComment">更新人(记录帐号）</meta>
        </property>

        <many-to-one
                name="property"
                column="PROPERTY_ID"
                class="com.justonetech.system.domain.SysCodeDetail"
                not-null="false"
                >
        </many-to-one>
        <many-to-one
                name="stage"
                column="STAGE_ID"
                class="com.justonetech.system.domain.SysCodeDetail"
                not-null="false"
                >
        </many-to-one>
        <many-to-one
                name="category"
                column="CATEGORY_ID"
                class="com.justonetech.system.domain.SysCodeDetail"
                not-null="false"
                >
        </many-to-one>
        <many-to-one
                name="belongArea"
                column="BELONG_AREA_ID"
                class="com.justonetech.system.domain.SysCodeDetail"
                not-null="false"
                >
        </many-to-one>

		<set name="projBids" inverse="true">
			<key column="POJECT_ID"/>
			<one-to-many class="ProjBid"/>
		</set>

		<set name="projRelatePersons" inverse="true">
			<key column="PROJECT_ID"/>
			<one-to-many class="ProjRelatePerson"/>
		</set>

		<set name="projRelateDepts" inverse="true">
			<key column="PROJECT_ID"/>
			<one-to-many class="ProjRelateDept"/>
		</set>
        <set name="dataNodeReports" inverse="true">
            <key column="PROJECT_ID"/>
            <one-to-many class="DataNodeReport"/>
        </set>
        <set name="dataStageReports" inverse="true">
            <key column="PROJECT_ID"/>
            <one-to-many class="DataStageReport"/>
        </set>

	</class>	
    <query name="projinfo_all_data_query">
        <![CDATA[FROM ProjInfo]]>
    </query>
</hibernate-mapping>