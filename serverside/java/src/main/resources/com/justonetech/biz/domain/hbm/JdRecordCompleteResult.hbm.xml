<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
	<class
		name="JdRecordCompleteResult"
		table="JD_RECORD_COMPLETE_RESULT"
	>
		
		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">false</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">安全质量监督--竣工验收--验收结果</meta>
        <meta attribute="tableComment">安全质量监督--竣工验收--验收结果</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">jd</meta>

		<id
			name="id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
		</id>

		<property
			name="checkResult"
			column="CHECK_RESULT"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">检查结果</meta>
            <meta attribute="ColumnComment">检查结果</meta>
        </property>

		<many-to-one
			name="recordComplete"
			column="RECORD_COMPLETE_ID"
			class="JdRecordComplete"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="checkitem"
			column="CHECKITEM_ID"
			class="JdRecordCompCheckitem"
			not-null="false"
		>
		</many-to-one>

	</class>	
    <query name="jdrecordcompleteresult_all_data_query">
        <![CDATA[FROM JdRecordCompleteResult]]>
    </query>
</hibernate-mapping>