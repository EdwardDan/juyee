		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">安全质量监督--首次监督</meta>
        <meta attribute="tableComment">安全质量监督--首次监督</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">jd</meta>

		<id
			name="id"
			type="java.lang.Long"
			column="ID"
		>
