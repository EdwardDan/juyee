        <many-to-one
                name="jdManager"
                column="JD_MANAGER_ID"
                class="com.justonetech.system.domain.SysPerson"
                not-null="false"
                >
        </many-to-one>

        <many-to-one
                name="dwgc"
                column="DWGC_ID"
                class="ExcelJdDwgc"
                not-null="false"
                >
        </many-to-one>
	</class>	
    <query name="jdrecordfirst_all_data_query">
        <![CDATA[FROM JdRecordFirst]]>
    </query>
</hibernate-mapping>