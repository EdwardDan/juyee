<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
	<class
		name="ShareProCompletedRecord"
		table="SHARE_PRO_COMPLETED_RECORD"
	>
		
		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">false</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">竣工备案主表</meta>
        <meta attribute="tableComment">竣工备案主表</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">share</meta>

		<id
			name="id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
		</id>

		<property
			name="bjbh"
			column="BJBH"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">报建编号</meta>
            <meta attribute="ColumnComment">报建编号</meta>
        </property>

		<property
			name="proName"
			column="PRO_NAME"
			type="string"
			not-null="false"
			length="200"
        >
            <meta attribute="ColumnName">项目名称</meta>
            <meta attribute="ColumnComment">项目名称</meta>
        </property>

		<property
			name="recordCode"
			column="RECORD_CODE"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">备案编码</meta>
            <meta attribute="ColumnComment">备案编码</meta>
        </property>

		<property
			name="acceptDatetime"
			column="ACCEPT_DATETIME"
			type="timestamp"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">受理日期</meta>
            <meta attribute="ColumnComment">受理日期</meta>
        </property>

		<property
			name="fgwExchangeCode"
			column="FGW_EXCHANGE_CODE"
			type="string"
			not-null="false"
			length="20"
        >
            <meta attribute="ColumnName">发改委交互码</meta>
            <meta attribute="ColumnComment">发改委交互码</meta>
        </property>

		<property
			name="jbr"
			column="JBR"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">经办人</meta>
            <meta attribute="ColumnComment">经办人</meta>
        </property>

		<property
			name="acceptUnitCode"
			column="ACCEPT_UNIT_CODE"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">受理单位代码</meta>
            <meta attribute="ColumnComment">受理单位代码</meta>
        </property>

		<property
			name="acceptUnitName"
			column="ACCEPT_UNIT_NAME"
			type="string"
			not-null="false"
			length="200"
        >
            <meta attribute="ColumnName">受理单位名称</meta>
            <meta attribute="ColumnComment">受理单位名称</meta>
        </property>

		<property
			name="shareTime"
			column="SHARE_TIME"
			type="timestamp"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">共享时间</meta>
            <meta attribute="ColumnComment">共享时间</meta>
        </property>

		<property
			name="shareStatus"
			column="SHARE_STATUS"
			type="string"
			not-null="false"
			length="10"
        >
            <meta attribute="ColumnName">共享状态</meta>
            <meta attribute="ColumnComment">共享状态</meta>
        </property>

		<property
			name="totalArea"
			column="TOTAL_AREA"
			type="java.lang.Double"
			not-null="false"
			length="15"
        >
            <meta attribute="ColumnName">总面积</meta>
            <meta attribute="ColumnComment">总面积</meta>
        </property>

		<property
			name="contMoney"
			column="CONT_MONEY"
			type="java.lang.Double"
			not-null="false"
			length="15"
        >
            <meta attribute="ColumnName">合同资金</meta>
            <meta attribute="ColumnComment">合同资金</meta>
        </property>


	</class>	
    <query name="shareprocompletedrecord_all_data_query">
        <![CDATA[FROM ShareProCompletedRecord]]>
    </query>
</hibernate-mapping>