<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
	<class
		name="JdRecordCompCheckitem"
		table="JD_RECORD_COMP_CHECKITEM"
	>

		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">false</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">安全质量监督--竣工验收--验收条件设置</meta>
        <meta attribute="tableComment">安全质量监督--竣工验收--验收条件设置</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">jd</meta>

		<id
			name="id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
		</id>

		<property
			name="orderNo"
			column="ORDER_NO"
			type="java.lang.Long"
			not-null="false"
			length="10"
        >
            <meta attribute="ColumnName">序号</meta>
            <meta attribute="ColumnComment">序号</meta>
        </property>

		<property
			name="condition"
			column="CONDITION"
			type="string"
			not-null="false"
			length="500"
        >
            <meta attribute="ColumnName">竣工验收条件</meta>
            <meta attribute="ColumnComment">竣工验收条件</meta>
        </property>

		<property
			name="selectItems"
			column="SELECT_ITEMS"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">符合形式</meta>
            <meta attribute="ColumnComment">符合形式</meta>
        </property>

        <many-to-one
                name="type"
                column="TYPE_ID"
                class="com.justonetech.system.domain.SysCodeDetail"
                not-null="false"
                >
        </many-to-one>

		<set name="jdRecordCompleteResults" inverse="true">
			<key column="CHECKITEM_ID"/>
			<one-to-many class="JdRecordCompleteResult"/>
		</set>

	</class>
    <query name="jdrecordcompcheckitem_all_data_query">
        <![CDATA[FROM JdRecordCompCheckitem]]>
    </query>
</hibernate-mapping>