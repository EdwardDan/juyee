<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
	<class
		name="ReportMonth"
		table="REPORT_MONTH"
	>
		
		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">false</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">综合报表月份上报</meta>
        <meta attribute="tableComment">综合报表月份上报</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">report</meta>

		<id
			name="id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
		</id>

		<property
			name="year"
			column="YEAR"
			type="integer"
			not-null="false"
			length="4"
        >
            <meta attribute="ColumnName">报表年份</meta>
            <meta attribute="ColumnComment">报表年份</meta>
        </property>

		<property
			name="month"
			column="MONTH"
			type="integer"
			not-null="false"
			length="2"
        >
            <meta attribute="ColumnName">报表月份</meta>
            <meta attribute="ColumnComment">报表月份</meta>
        </property>

		<property
			name="reportPerson"
			column="REPORT_PERSON"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">填报人</meta>
            <meta attribute="ColumnComment">填报人</meta>
        </property>

		<property
			name="reportDate"
			column="REPORT_DATE"
			type="date"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">提交时间</meta>
            <meta attribute="ColumnComment">提交时间</meta>
        </property>

		<property
			name="isValid"
			column="IS_VALID"
			type="java.lang.Boolean"
			not-null="false"
			length="1"
        >
            <meta attribute="ColumnName">是否提交</meta>
            <meta attribute="ColumnComment">是否提交</meta>
        </property>

		<property
			name="createTime"
			column="CREATE_TIME"
			type="timestamp"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">创建时间</meta>
            <meta attribute="ColumnComment">创建时间</meta>
        </property>

		<property
			name="createUser"
			column="CREATE_USER"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">创建用户名</meta>
            <meta attribute="ColumnComment">创建用户名</meta>
        </property>

		<property
			name="updateTime"
			column="UPDATE_TIME"
			type="timestamp"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">更新时间</meta>
            <meta attribute="ColumnComment">更新时间</meta>
        </property>

		<property
			name="updateUser"
			column="UPDATE_USER"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">更新用户名</meta>
            <meta attribute="ColumnComment">更新用户名</meta>
        </property>

		<many-to-one
			name="reportDeptDefine"
			column="REPORT_DEPT_DEFINE_ID"
			class="ReportDeptDefine"
			not-null="false"
		>
		</many-to-one>

        <many-to-one
                name="version"
                column="VERSION_ID"
                class="ReportStructureVer"
                not-null="false"
                >
        </many-to-one>

		<set name="reportMonthDetails" inverse="true">
			<key column="REPORT_MONTH_ID"/>
			<one-to-many class="ReportMonthDetail"/>
		</set>

	</class>	
    <query name="reportmonth_all_data_query">
        <![CDATA[FROM ReportMonth]]>
    </query>
</hibernate-mapping>