<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
	<class
		name="JdReport"
		table="JD_REPORT"
	>

		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">false</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">监督报告</meta>
        <meta attribute="tableComment">监督报告</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">report</meta>

		<id
			name="id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
		</id>
        <property
                name="jdTaskCode"
                column="JD_TASK_CODE"
                type="string"
                not-null="false"
                length="50"
                >
            <meta attribute="ColumnName">监督任务书编号</meta>
            <meta attribute="ColumnComment">监督任务书编号</meta>
        </property>
        <property
                name="reportTypeDesc"
                column="REPORT_TYPE_DESC"
                type="string"
                not-null="false"
                length="50"
                >
            <meta attribute="ColumnName">报告类型中文描述</meta>
            <meta attribute="ColumnComment">报告类型中文描述</meta>
        </property>
		<property
			name="bjbh"
			column="BJBH"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">报建编号</meta>
            <meta attribute="ColumnComment">报建编号</meta>
        </property>

		<property
			name="buildUnit"
			column="BUILD_UNIT"
			type="string"
			not-null="false"
			length="200"
        >
            <meta attribute="ColumnName">建设单位</meta>
            <meta attribute="ColumnComment">建设单位</meta>
        </property>
        <property
                name="reportPerson"
                column="REPORT_PERSON"
                type="string"
                not-null="false"
                length="50"
                >
            <meta attribute="ColumnName">监督报告编制人</meta>
            <meta attribute="ColumnComment">监督报告编制人</meta>
        </property>
		<property
			name="projectName"
			column="PROJECT_NAME"
			type="string"
			not-null="false"
			length="200"
        >
            <meta attribute="ColumnName">工程名称(取监督任务书中的名称）</meta>
            <meta attribute="ColumnComment">工程名称(取监督任务书中的名称）</meta>
        </property>

		<property
			name="reportDate"
			column="REPORT_DATE"
			type="date"
			not-null="false"
			length="10"
        >
            <meta attribute="ColumnName">监督报告日期</meta>
            <meta attribute="ColumnComment">监督报告日期</meta>
        </property>

		<property
			name="printReport"
			column="PRINT_REPORT"
			type="string"
			not-null="false"
        >
            <meta attribute="ColumnName">打印内容</meta>
            <meta attribute="ColumnComment">打印内容</meta>
        </property>

		<property
			name="printDatetime"
			column="PRINT_DATETIME"
			type="timestamp"
			not-null="false"
			length="19"
        >
            <meta attribute="ColumnName">打印时间</meta>
            <meta attribute="ColumnComment">打印时间</meta>
        </property>

		<many-to-one
			name="reportType"
			column="REPORT_TYPE_ID"
			class="com.justonetech.system.domain.SysCodeDetail"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="printUser"
			column="PRINT_USER_ID"
			class="com.justonetech.system.domain.SysUser"
			not-null="false"
		>
		</many-to-one>


		<set name="jdReportDwgcs" inverse="true" order-by="JD_TASK_DWGC_ID" >
			<key column="JD_REPORT_ID"/>
			<one-to-many class="JdReportDwgc"/>
		</set>

	</class>
    <query name="jdreport_all_data_query">
        <![CDATA[FROM JdReport]]>
    </query>
</hibernate-mapping>