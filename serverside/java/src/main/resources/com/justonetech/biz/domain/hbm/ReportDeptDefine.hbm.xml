<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
	<class
		name="ReportDeptDefine"
		table="REPORT_DEPT_DEFINE"
	>
		
		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">false</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">综合报表单位权限定义</meta>
        <meta attribute="tableComment">综合报表单位权限定义</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">report</meta>

		<id
			name="id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
		</id>

        <property
                name="code"
                column="CODE"
                type="string"
                not-null="false"
                length="50"
                >
            <meta attribute="ColumnName">部门编码</meta>
            <meta attribute="ColumnComment">部门编码</meta>
        </property>

		<property
			name="deptName"
			column="DEPT_NAME"
			type="string"
			not-null="false"
			length="200"
        >
            <meta attribute="ColumnName">部门名称</meta>
            <meta attribute="ColumnComment">部门名称</meta>
        </property>

		<property
			name="orderNo"
			column="ORDER_NO"
			type="java.lang.Long"
			not-null="false"
			length="10"
        >
            <meta attribute="ColumnName">部门序号</meta>
            <meta attribute="ColumnComment">部门序号</meta>
        </property>

		<property
			name="createTime"
			column="CREATE_TIME"
			type="timestamp"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">创建时间</meta>
            <meta attribute="ColumnComment">创建时间</meta>
        </property>

		<property
			name="createUser"
			column="CREATE_USER"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">创建用户名</meta>
            <meta attribute="ColumnComment">创建用户名</meta>
        </property>

		<property
			name="updateTime"
			column="UPDATE_TIME"
			type="timestamp"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">更新时间</meta>
            <meta attribute="ColumnComment">更新时间</meta>
        </property>

		<property
			name="updateUser"
			column="UPDATE_USER"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">更新用户名</meta>
            <meta attribute="ColumnComment">更新用户名</meta>
        </property>

        <many-to-one
                name="sysPrivilege"
                column="SYS_PRIVILEGE_ID"
                class="com.justonetech.system.domain.SysPrivilege"
                not-null="false"
                >
        </many-to-one>

		<set name="reportStructures" inverse="true" order-by="TREE_ID">
			<key column="REPORT_DEPT_DEFINE_ID"/>
			<one-to-many class="ReportStructure"/>
		</set>

		<set name="reportMonths" inverse="true">
			<key column="REPORT_DEPT_DEFINE_ID"/>
			<one-to-many class="ReportMonth"/>
		</set>

	</class>	
    <query name="reportdeptdefine_all_data_query">
        <![CDATA[FROM ReportDeptDefine]]>
    </query>
</hibernate-mapping>