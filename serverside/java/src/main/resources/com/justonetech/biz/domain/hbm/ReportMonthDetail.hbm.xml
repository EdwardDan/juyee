<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
	<class
		name="ReportMonthDetail"
		table="REPORT_MONTH_DETAIL"
	>
		
		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">false</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">综合报表月份上报明细</meta>
        <meta attribute="tableComment">综合报表月份上报明细</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">report</meta>

		<id
			name="id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
		</id>

		<property
			name="monthValue"
			column="MONTH_VALUE"
			type="java.lang.Double"
			not-null="false"
			length="14"
        >
            <meta attribute="ColumnName">本月填报</meta>
            <meta attribute="ColumnComment">本月填报</meta>
        </property>

		<property
			name="accuValue"
			column="ACCU_VALUE"
			type="java.lang.Double"
			not-null="false"
			length="14"
        >
            <meta attribute="ColumnName">累计</meta>
            <meta attribute="ColumnComment">累计</meta>
        </property>

		<property
			name="percentHb"
			column="PERCENT_HB"
			type="java.lang.Double"
			not-null="false"
			length="14"
        >
            <meta attribute="ColumnName">环比</meta>
            <meta attribute="ColumnComment">环比</meta>
        </property>

		<property
			name="percentTb"
			column="PERCENT_TB"
			type="java.lang.Double"
			not-null="false"
			length="14"
        >
            <meta attribute="ColumnName">同比</meta>
            <meta attribute="ColumnComment">同比</meta>
        </property>

		<property
			name="description"
			column="DESCRIPTION"
			type="string"
			not-null="false"
			length="1000"
        >
            <meta attribute="ColumnName">备注</meta>
            <meta attribute="ColumnComment">备注</meta>
        </property>

		<many-to-one
			name="reportMonth"
			column="REPORT_MONTH_ID"
			class="ReportMonth"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="reportStructure"
			column="REPORT_STRUCTURE_ID"
			class="ReportStructure"
			not-null="false"
		>
		</many-to-one>

	</class>	
    <query name="reportmonthdetail_all_data_query">
        <![CDATA[FROM ReportMonthDetail]]>
    </query>
</hibernate-mapping>