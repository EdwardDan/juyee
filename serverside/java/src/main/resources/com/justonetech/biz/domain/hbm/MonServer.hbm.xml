<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
	<class
		name="MonServer"
		table="MON_SERVER"
	>
		
		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">false</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">视频服务器</meta>
        <meta attribute="tableComment">视频服务器</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">monitor</meta>

		<id
			name="id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
		</id>

		<property
			name="name"
			column="NAME"
			type="string"
			not-null="false"
			length="200"
        >
            <meta attribute="ColumnName">项目名称</meta>
            <meta attribute="ColumnComment">项目名称</meta>
        </property>

		<property
			name="serverName"
			column="SERVER_NAME"
			type="string"
			not-null="false"
			length="200"
        >
            <meta attribute="ColumnName">主机名</meta>
            <meta attribute="ColumnComment">主机名</meta>
        </property>

		<property
			name="ip"
			column="IP"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">IP</meta>
            <meta attribute="ColumnComment">IP</meta>
        </property>

		<property
			name="port"
			column="PORT"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">端口</meta>
            <meta attribute="ColumnComment">端口</meta>
        </property>

		<property
			name="proxyIp"
			column="PROXY_IP"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">代理IP地址</meta>
            <meta attribute="ColumnComment">代理IP地址</meta>
        </property>

		<property
			name="proxyName"
			column="PROXY_NAME"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">代理IP主机名</meta>
            <meta attribute="ColumnComment">代理IP主机名</meta>
        </property>

		<property
			name="loginName"
			column="LOGIN_NAME"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">登录名</meta>
            <meta attribute="ColumnComment">登录名</meta>
        </property>

		<property
			name="password"
			column="PASSWORD"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">密码</meta>
            <meta attribute="ColumnComment">密码</meta>
        </property>

		<property
			name="description"
			column="DESCRIPTION"
			type="string"
			not-null="false"
        >
            <meta attribute="ColumnName">备注</meta>
            <meta attribute="ColumnComment">备注</meta>
        </property>

		<many-to-one
			name="connectType"
			column="CONNECT_TYPE_ID"
			class="com.justonetech.system.domain.SysCodeDetail"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="serverType"
			column="SERVER_TYPE_ID"
			class="com.justonetech.system.domain.SysCodeDetail"
			not-null="false"
		>
		</many-to-one>
	

		<set name="monCameras" inverse="true">
			<key column="MON_SERVER_ID"/>
			<one-to-many class="MonCamera"/>
		</set>

	</class>	
    <query name="monserver_all_data_query">
        <![CDATA[FROM MonServer]]>
    </query>
</hibernate-mapping>