<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
	<class
		name="SlryQualify"
		table="SLRY_QUALIFY"
	>
		
		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">false</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">三类人员</meta>
        <meta attribute="tableComment">三类人员</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">slry</meta>

		<id
			name="id"
			type="java.lang.Long"
			column="ID"
		>
            <generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
		</id>

		<property
			name="unitName"
			column="UNIT_NAME"
			type="java.lang.Long"
			not-null="false"
			length="10"
        >
            <meta attribute="ColumnName">企业名称</meta>
            <meta attribute="ColumnComment">企业名称</meta>
        </property>

		<property
			name="description"
			column="DESCRIPTION"
			type="string"
			not-null="false"
        >
            <meta attribute="ColumnName">备注</meta>
            <meta attribute="ColumnComment">备注</meta>
        </property>

		<property
			name="name"
			column="NAME"
			type="integer"
			not-null="false"
			length="2"
        >
            <meta attribute="ColumnName">姓名</meta>
            <meta attribute="ColumnComment">姓名</meta>
        </property>

		<property
			name="sex"
			column="SEX"
			type="java.lang.Boolean"
			not-null="false"
			length="1"
        >
            <meta attribute="ColumnName">性别</meta>
            <meta attribute="ColumnComment">性别</meta>
        </property>

		<property
			name="birthDate"
			column="BIRTH_DATE"
			type="date"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">出生日期</meta>
            <meta attribute="ColumnComment">出生日期</meta>
        </property>

		<property
			name="idCard"
			column="ID_CARD"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">证件号码</meta>
            <meta attribute="ColumnComment">证件号码</meta>
        </property>

        <property
                name="status"
                column="STATUS"
                type="integer"
                not-null="false"
                length="2"
                >
            <meta attribute="ColumnName">状态</meta>
            <meta attribute="ColumnComment">状态</meta>
        </property>

        <property
                name="csOpinion"
                column="CS_OPINION"
                type="string"
                not-null="false"
                length="2000"
                >
            <meta attribute="ColumnName">初审意见</meta>
            <meta attribute="ColumnComment">初审意见</meta>
        </property>

		<property
			name="createTime"
			column="CREATE_TIME"
			type="timestamp"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">创建时间</meta>
            <meta attribute="ColumnComment">创建时间</meta>
        </property>

		<property
			name="createUser"
			column="CREATE_USER"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">创建用户名</meta>
            <meta attribute="ColumnComment">创建用户名</meta>
        </property>

		<property
			name="updateTime"
			column="UPDATE_TIME"
			type="timestamp"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">更新时间</meta>
            <meta attribute="ColumnComment">更新时间</meta>
        </property>

		<property
			name="updateUser"
			column="UPDATE_USER"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">更新用户名</meta>
            <meta attribute="ColumnComment">更新用户名</meta>
        </property>

		<many-to-one
			name="applyCerType"
			column="APPLY_CER_TYPE_ID"
			class="com.justonetech.system.domain.SysCodeDetail"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="techType"
			column="TECH_TYPE_ID"
			class="com.justonetech.system.domain.SysCodeDetail"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="unitType"
			column="UNIT_TYPE_ID"
			class="com.justonetech.system.domain.SysCodeDetail"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="docCard"
			column="DOC_CARD_ID"
			class="DocDocument"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="applyType"
			column="APPLY_TYPE_ID"
			class="com.justonetech.system.domain.SysCodeDetail"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="educationType"
			column="EDUCATION_TYPE_ID"
			class="com.justonetech.system.domain.SysCodeDetail"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="docEducation"
			column="DOC_EDUCATION_ID"
			class="DocDocument"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="docPhoto"
			column="DOC_PHOTO_ID"
			class="DocDocument"
			not-null="false"
		>
		</many-to-one>
        <many-to-one
			name="docContract"
			column="DOC_CONTRACT_ID"
			class="DocDocument"
			not-null="false"
		>
		</many-to-one>

	</class>	
    <query name="slryqualify_all_data_query">
        <![CDATA[FROM SlryQualify]]>
    </query>
</hibernate-mapping>