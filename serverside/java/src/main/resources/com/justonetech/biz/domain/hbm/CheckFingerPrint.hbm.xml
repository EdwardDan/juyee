<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
    <class
            name="CheckFingerPrint"
            table="CHECK_FINGER_PRINT"
            >

        <meta attribute="sync-DAO">false</meta>
        <meta attribute="sync-jsp">true</meta>
        <meta attribute="sync-boolean">get</meta>
        <meta attribute="treeable">false</meta>
        <meta attribute="projectable">false</meta>
        <meta attribute="tableName">指纹信息表</meta>
        <meta attribute="tableComment">指纹信息表</meta>
        <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">check</meta>

        <id
                name="id"
                type="java.lang.Long"
                column="ID"
                >
            <generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
        </id>

        <property
                name="template"
                column="TEMPLATE"
                type="string"
                not-null="false"
                >
            <meta attribute="ColumnName">指纹模版</meta>
            <meta attribute="ColumnComment">指纹模版</meta>
        </property>

        <property
                name="fingerid"
                column="FINGERID"
                type="java.lang.Long"
                not-null="false"
                length="10"
                >
            <meta attribute="ColumnName">手指标识(0,1....9)</meta>
            <meta attribute="ColumnComment">手指标识</meta>
        </property>

        <property
                name="fingerVersion"
                column="FINGER_VERSION"
                type="string"
                not-null="false"
                length="20"
                >
            <meta attribute="ColumnName">指纹版本号</meta>
            <meta attribute="ColumnComment">指纹版本号</meta>
        </property>

        <property
                name="bioType"
                column="BIO_TYPE"
                type="java.lang.Long"
                not-null="false"
                length="10"
                >
            <meta attribute="ColumnName">模版类型（指纹、人脸）</meta>
            <meta attribute="ColumnComment">模版类型（指纹、人脸）</meta>
        </property>

        <property
                name="devSn"
                column="DEV_SN"
                type="string"
                not-null="false"
                length="50"
                >
            <meta attribute="ColumnName">设备序号</meta>
            <meta attribute="ColumnComment">设备序号</meta>
        </property>

        <property
                name="template1"
                column="TEMPLATE1"
                type="string"
                not-null="false"
                >
            <meta attribute="ColumnName">备注模版1</meta>
            <meta attribute="ColumnComment">备注模版1</meta>
        </property>

        <property
                name="template2"
                column="TEMPLATE2"
                type="string"
                not-null="false"
                >
            <meta attribute="ColumnName">备注模版2</meta>
            <meta attribute="ColumnComment">备注模版2</meta>
        </property>

        <property
                name="template3"
                column="TEMPLATE3"
                type="string"
                not-null="false"
                >
            <meta attribute="ColumnName">备注模版3</meta>
            <meta attribute="ColumnComment">备注模版3</meta>
        </property>

        <property
                name="createTime"
                column="CREATE_TIME"
                type="timestamp"
                not-null="false"
                length="19"
                >
            <meta attribute="ColumnName">创建时间</meta>
            <meta attribute="ColumnComment">创建时间</meta>
        </property>

        <property
                name="updateTime"
                column="UPDATE_TIME"
                type="timestamp"
                not-null="false"
                length="19"
                >
            <meta attribute="ColumnName">更新时间</meta>
            <meta attribute="ColumnComment">更新时间</meta>
        </property>

        <property
                name="createUser"
                column="CREATE_USER"
                type="string"
                not-null="false"
                length="100"
                >
            <meta attribute="ColumnName">创建人</meta>
            <meta attribute="ColumnComment">创建人</meta>
        </property>

        <property
                name="updateUser"
                column="UPDATE_USER"
                type="string"
                not-null="false"
                length="100"
                >
            <meta attribute="ColumnName">更新人</meta>
            <meta attribute="ColumnComment">更新人</meta>
        </property>

        <property
                name="deleteUser"
                column="DELETE_USER"
                type="string"
                not-null="false"
                length="100"
                >
            <meta attribute="ColumnName">删除人</meta>
            <meta attribute="ColumnComment">删除人</meta>
        </property>

        <property
                name="deleteTime"
                column="DELETE_TIME"
                type="timestamp"
                not-null="false"
                length="19"
                >
            <meta attribute="ColumnName">删除时间</meta>
            <meta attribute="ColumnComment">删除时间</meta>
        </property>

        <property
                name="isDelete"
                column="IS_DELETE"
                type="java.lang.Boolean"
                not-null="false"
                length="1"
                >
            <meta attribute="ColumnName">逻辑删除</meta>
            <meta attribute="ColumnComment">逻辑删除</meta>
        </property>

        <property
                name="guid"
                column="GUID"
                type="string"
                not-null="false"
                length="50"
                >
            <meta attribute="ColumnName">GUID</meta>
            <meta attribute="ColumnComment">GUID</meta>
        </property>

        <many-to-one
                name="manager"
                column="MANAGER_ID"
                class="com.justonetech.biz.domain.CheckManager"
                not-null="false"
                >
        </many-to-one>

    </class>
    <query name="checkfingerprint_all_data_query">
        <![CDATA[FROM CheckFingerPrint]]>
    </query>
</hibernate-mapping>