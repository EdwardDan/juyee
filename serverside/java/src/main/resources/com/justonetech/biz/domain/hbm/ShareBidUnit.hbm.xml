<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
	<class
		name="ShareBidUnit"
		table="SHARE_BID_UNIT"
	>
		
		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">false</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">承发包中标单位</meta>
        <meta attribute="tableComment">承发包中标单位</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">share</meta>

		<composite-id name="id" class="ShareBidUnitPK">
			<key-property
				name="bidUnitCode"
				column="BID_UNIT_CODE"
				type="string"
			/>
			<key-property
				name="bdh"
				column="BDH"
				type="string"
			/>
			<key-property
				name="bjbh"
				column="BJBH"
				type="string"
			/>
		</composite-id>

		<property
			name="bidUnit"
			column="BID_UNIT"
			type="string"
			not-null="false"
			length="200"
        >
            <meta attribute="ColumnName">中标单位</meta>
            <meta attribute="ColumnComment">中标单位</meta>
        </property>

		<property
			name="managerName"
			column="MANAGER_NAME"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">项目经理姓名</meta>
            <meta attribute="ColumnComment">项目经理姓名</meta>
        </property>

		<property
			name="managerCard"
			column="MANAGER_CARD"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">项目经理身份证号</meta>
            <meta attribute="ColumnComment">项目经理身份证号</meta>
        </property>

		<property
			name="mainJlName"
			column="MAIN_JL_NAME"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">项目总监姓名</meta>
            <meta attribute="ColumnComment">项目总监姓名</meta>
        </property>

		<property
			name="mainJlCertificate"
			column="MAIN_JL_CERTIFICATE"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">项目总监证书号</meta>
            <meta attribute="ColumnComment">项目总监证书号</meta>
        </property>


	</class>	
    <query name="sharebidunit_all_data_query">
        <![CDATA[FROM ShareBidUnit]]>
    </query>
</hibernate-mapping>