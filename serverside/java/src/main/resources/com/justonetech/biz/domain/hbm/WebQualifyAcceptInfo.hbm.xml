<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
	<class
		name="WebQualifyAcceptInfo"
		table="WEB_QUALIFY_ACCEPT_INFO"
	>
		
		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">false</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">企业资质受理情况</meta>
        <meta attribute="tableComment">企业资质受理情况</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">web</meta>

		<id
			name="id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
		</id>

		<property
			name="unitName"
			column="UNIT_NAME"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">企业名称</meta>
            <meta attribute="ColumnComment">企业名称</meta>
        </property>

		<property
			name="applys"
			column="APPLYS"
			type="string"
			not-null="false"
			length="1000"
        >
            <meta attribute="ColumnName">申请事项</meta>
            <meta attribute="ColumnComment">申请事项</meta>
        </property>

		<property
			name="acceptNo"
			column="ACCEPT_NO"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">受理批次</meta>
            <meta attribute="ColumnComment">受理批次</meta>
        </property>

		<property
			name="publicDate"
			column="PUBLIC_DATE"
			type="date"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">公示时间</meta>
            <meta attribute="ColumnComment">公示时间</meta>
        </property>

		<property
			name="zcAddress"
			column="ZC_ADDRESS"
			type="string"
			not-null="false"
			length="1000"
        >
            <meta attribute="ColumnName">注册地址</meta>
            <meta attribute="ColumnComment">注册地址</meta>
        </property>

		<property
			name="unitCategory"
			column="UNIT_CATEGORY"
			type="string"
			not-null="false"
			length="500"
        >
            <meta attribute="ColumnName">企业类型</meta>
            <meta attribute="ColumnComment">企业类型</meta>
        </property>

		<property
			name="zcMoney"
			column="ZC_MONEY"
			type="string"
			not-null="false"
			length="500"
        >
            <meta attribute="ColumnName">注册资本金</meta>
            <meta attribute="ColumnComment">注册资本金</meta>
        </property>

		<property
			name="legalPerson"
			column="LEGAL_PERSON"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">企业法人</meta>
            <meta attribute="ColumnComment">企业法人</meta>
        </property>

		<property
			name="legalPersonTitle"
			column="LEGAL_PERSON_TITLE"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">企业法人职称</meta>
            <meta attribute="ColumnComment">企业法人职称</meta>
        </property>

		<property
			name="legalPersonPersion"
			column="LEGAL_PERSON_PERSION"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">企业法人职务</meta>
            <meta attribute="ColumnComment">企业法人职务</meta>
        </property>

	

		<set name="webUnitApplyQualifies" inverse="true">
			<key column="QUALIFY_ACCEPT_INFO_ID"/>
			<one-to-many class="WebUnitApplyQualify"/>
		</set>

		<set name="webUnitPersonInfos" inverse="true">
			<key column="QUALIFY_ACCEPT_INFO_ID"/>
			<one-to-many class="WebUnitPersonInfo"/>
		</set>

	</class>	
    <query name="webqualifyacceptinfo_all_data_query">
        <![CDATA[FROM WebQualifyAcceptInfo]]>
    </query>
</hibernate-mapping>