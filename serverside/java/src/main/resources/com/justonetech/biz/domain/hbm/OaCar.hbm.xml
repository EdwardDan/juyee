<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
	<class
		name="OaCar"
		table="OA_CAR"
	>
		
		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">false</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">车辆管理</meta>
        <meta attribute="tableComment">车辆管理</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">car</meta>

		<id
			name="id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
		</id>

		<property
			name="applyDept"
			column="APPLY_DEPT"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">申请部门名称</meta>
            <meta attribute="ColumnComment">申请部门名称</meta>
        </property>

		<property
			name="applyPerson"
			column="APPLY_PERSON"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">申请人</meta>
            <meta attribute="ColumnComment">申请人</meta>
        </property>

		<property
			name="useReason"
			column="USE_REASON"
			type="string"
			not-null="false"
			length="500"
        >
            <meta attribute="ColumnName">用车事由</meta>
            <meta attribute="ColumnComment">用车事由</meta>
        </property>

		<property
			name="useDate"
			column="USE_DATE"
			type="date"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">使用车辆日期</meta>
            <meta attribute="ColumnComment">使用车辆日期</meta>
        </property>

		<property
			name="useBeginTime"
			column="USE_BEGIN_TIME"
			type="string"
			not-null="false"
			length="10"
        >
            <meta attribute="ColumnName">使用车辆开始时间</meta>
            <meta attribute="ColumnComment">使用车辆开始时间</meta>
        </property>

		<property
			name="useEndTime"
			column="USE_END_TIME"
			type="string"
			not-null="false"
			length="10"
        >
            <meta attribute="ColumnName">使用车辆结束时间</meta>
            <meta attribute="ColumnComment">使用车辆结束时间</meta>
        </property>

		<property
			name="arrivalPlace"
			column="ARRIVAL_PLACE"
			type="string"
			not-null="false"
			length="200"
        >
            <meta attribute="ColumnName">目的地</meta>
            <meta attribute="ColumnComment">目的地</meta>
        </property>

		<property
			name="auditOpinion"
			column="AUDIT_OPINION"
			type="string"
			not-null="false"
			length="1000"
        >
            <meta attribute="ColumnName">审核意见</meta>
            <meta attribute="ColumnComment">审核意见</meta>
        </property>

		<property
			name="auditTime"
			column="AUDIT_TIME"
			type="timestamp"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">审核时间</meta>
            <meta attribute="ColumnComment">审核时间</meta>
        </property>

		<property
			name="auditUser"
			column="AUDIT_USER"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">审核用户名</meta>
            <meta attribute="ColumnComment">审核用户名</meta>
        </property>

		<property
			name="personNum"
			column="PERSON_NUM"
			type="integer"
			not-null="false"
			length="4"
        >
            <meta attribute="ColumnName">用车人数</meta>
            <meta attribute="ColumnComment">用车人数</meta>
        </property>

		<property
			name="description"
			column="DESCRIPTION"
			type="string"
			not-null="false"
			length="500"
        >
            <meta attribute="ColumnName">备注</meta>
            <meta attribute="ColumnComment">备注</meta>
        </property>

		<property
			name="status"
			column="STATUS"
			type="integer"
			not-null="false"
			length="2"
        >
            <meta attribute="ColumnName">状态</meta>
            <meta attribute="ColumnComment">状态</meta>
        </property>

		<property
			name="carNo"
			column="CAR_NO"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">拟派车牌号</meta>
            <meta attribute="ColumnComment">拟派车牌号</meta>
        </property>

		<property
			name="isArrangeDriver"
			column="IS_ARRANGE_DRIVER"
			type="java.lang.Boolean"
			not-null="false"
			length="1"
        >
            <meta attribute="ColumnName">是否拟派司机</meta>
            <meta attribute="ColumnComment">是否拟派司机</meta>
        </property>

		<property
			name="driverName"
			column="DRIVER_NAME"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">拟派司机</meta>
            <meta attribute="ColumnComment">拟派司机</meta>
        </property>

		<property
			name="driverTel"
			column="DRIVER_TEL"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">司机联系方式</meta>
            <meta attribute="ColumnComment">司机联系方式</meta>
        </property>

		<property
			name="createTime"
			column="CREATE_TIME"
			type="timestamp"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">创建时间</meta>
            <meta attribute="ColumnComment">创建时间</meta>
        </property>

		<property
			name="createUser"
			column="CREATE_USER"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">创建用户名</meta>
            <meta attribute="ColumnComment">创建用户名</meta>
        </property>

		<property
			name="updateTime"
			column="UPDATE_TIME"
			type="timestamp"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">更新时间</meta>
            <meta attribute="ColumnComment">更新时间</meta>
        </property>

		<property
			name="updateUser"
			column="UPDATE_USER"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">更新用户名</meta>
            <meta attribute="ColumnComment">更新用户名</meta>
        </property>


	</class>	
    <query name="oacar_all_data_query">
        <![CDATA[FROM OaCar]]>
    </query>
</hibernate-mapping>