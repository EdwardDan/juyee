		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">Excel监督单位工程</meta>
        <meta attribute="tableComment">Excel监督单位工程</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">syn</meta>

		<id
			name="id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
		</id>

		<property
			name="flowCode"
			column="FLOW_CODE"
			type="string"
			not-null="false"
			length="10"
