<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
	<class
		name="ShareProComRecordDwgc"
		table="SHARE_PRO_COM_RECORD_DWGC"
	>
		
		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">false</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">竣工备案单位工程明细</meta>
        <meta attribute="tableComment">竣工备案单位工程明细</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">share</meta>

		<id
			name="id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
		</id>

		<property
			name="completedRecordId"
			column="COMPLETED_RECORD_ID"
			type="java.lang.Long"
			not-null="false"
			length="10"
        >
            <meta attribute="ColumnName">竣工备案标识</meta>
            <meta attribute="ColumnComment">竣工备案标识</meta>
        </property>

		<property
			name="bjbh"
			column="BJBH"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">报建编号</meta>
            <meta attribute="ColumnComment">报建编号</meta>
        </property>

		<property
			name="dwgcCode"
			column="DWGC_CODE"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">单位工程号</meta>
            <meta attribute="ColumnComment">单位工程号</meta>
        </property>

		<property
			name="dwgcName"
			column="DWGC_NAME"
			type="string"
			not-null="false"
			length="300"
        >
            <meta attribute="ColumnName">单位工程名称</meta>
            <meta attribute="ColumnComment">单位工程名称</meta>
        </property>


	</class>	
    <query name="shareprocomrecorddwgc_all_data_query">
        <![CDATA[FROM ShareProComRecordDwgc]]>
    </query>
</hibernate-mapping>