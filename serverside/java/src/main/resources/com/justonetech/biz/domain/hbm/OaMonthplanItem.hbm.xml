<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
	<class
		name="OaMonthplanItem"
		table="OA_MONTHPLAN_ITEM"
	>
		
		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">false</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">月度总结和计划明细</meta>
        <meta attribute="tableComment">月度总结和计划明细</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">oa</meta>

		<id
			name="id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
		</id>

		<property
			name="orderNo"
			column="ORDER_NO"
			type="integer"
			not-null="false"
			length="4"
        >
            <meta attribute="ColumnName">序号</meta>
            <meta attribute="ColumnComment">序号</meta>
        </property>

		<property
			name="content"
			column="CONTENT"
			type="string"
			not-null="false"
        >
            <meta attribute="ColumnName">工作内容</meta>
            <meta attribute="ColumnComment">工作内容</meta>
        </property>

		<property
			name="timeNode"
			column="TIME_NODE"
			type="date"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">时间节点</meta>
            <meta attribute="ColumnComment">时间节点</meta>
        </property>

		<property
			name="reportType"
			column="REPORT_TYPE"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">报告类型</meta>
            <meta attribute="ColumnComment">报告类型</meta>
        </property>

		<property
			name="isComplete"
			column="IS_COMPLETE"
			type="java.lang.Boolean"
			not-null="false"
			length="1"
        >
            <meta attribute="ColumnName">是否完成</meta>
            <meta attribute="ColumnComment">是否完成</meta>
        </property>

        <property
			name="isSumDelete"
			column="IS_SUM_DELETE"
			type="java.lang.Boolean"
			not-null="false"
			length="1"
        >
            <meta attribute="ColumnName">是否删除</meta>
            <meta attribute="ColumnComment">是否删除</meta>
        </property>

		<many-to-one
			name="monthplan"
			column="MONTHPLAN_ID"
			class="OaMonthplan"
			not-null="false"
		>
		</many-to-one>
	

		<set name="oaMonthplanSumItems" inverse="true" order-by="orderNo">
			<key column="MONTHPLAN_ITEM_ID"/>
			<one-to-many class="OaMonthplanSumItem"/>
		</set>

	</class>	
    <query name="oamonthplanitem_all_data_query">
        <![CDATA[FROM OaMonthplanItem]]>
    </query>
</hibernate-mapping>