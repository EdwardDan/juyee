<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
	<class
		name="MonCamera"
		table="MON_CAMERA"
	>
		
		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">false</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">视频探头</meta>
        <meta attribute="tableComment">视频探头</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">monitor</meta>

		<id
			name="id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
		</id>

		<property
			name="name"
			column="NAME"
			type="string"
			not-null="false"
			length="200"
        >
            <meta attribute="ColumnName">探头名称</meta>
            <meta attribute="ColumnComment">探头名称</meta>
        </property>

		<property
			name="orderNo"
			column="ORDER_NO"
			type="integer"
			not-null="false"
			length="4"
        >
            <meta attribute="ColumnName">探头序号</meta>
            <meta attribute="ColumnComment">探头序号</meta>
        </property>

		<property
			name="description"
			column="DESCRIPTION"
			type="string"
			not-null="false"
        >
            <meta attribute="ColumnName">备注</meta>
            <meta attribute="ColumnComment">备注</meta>
        </property>

		<many-to-one
			name="monServer"
			column="MON_SERVER_ID"
			class="MonServer"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="cameraType"
			column="CAMERA_TYPE_ID"
			class="com.justonetech.system.domain.SysCodeDetail"
			not-null="false"
		>
		</many-to-one>

	</class>	
    <query name="moncamera_all_data_query">
        <![CDATA[FROM MonCamera]]>
    </query>
</hibernate-mapping>