<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
    <class
            name="com.justonetech.biz.domain.CheckInfo"
            table="CHECK_INFO"
            >

        <meta attribute="sync-DAO">false</meta>
        <meta attribute="sync-jsp">true</meta>
        <meta attribute="sync-boolean">get</meta>
        <meta attribute="treeable">false</meta>
        <meta attribute="projectable">false</meta>
        <meta attribute="tableName">考勤记录表</meta>
        <meta attribute="tableComment">考勤记录表</meta>
        <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">check</meta>

        <id
                name="id"
                type="java.lang.Long"
                column="ID"
                >
            <generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
        </id>

        <property
                name="managerName"
                column="MANAGER_NAME"
                type="string"
                not-null="false"
                length="50"
                >
            <meta attribute="ColumnName">人员姓名</meta>
            <meta attribute="ColumnComment">人员姓名</meta>
        </property>

        <property
                name="cardNo"
                column="CARD_NO"
                type="string"
                not-null="false"
                length="50"
                >
            <meta attribute="ColumnName">身份证号</meta>
            <meta attribute="ColumnComment">身份证号</meta>
        </property>

        <property
                name="checkTime"
                column="CHECK_TIME"
                type="timestamp"
                not-null="false"
                length="19"
                >
            <meta attribute="ColumnName">考勤时间</meta>
            <meta attribute="ColumnComment">考勤时间</meta>
        </property>

        <property
                name="gdCode"
                column="GD_CODE"
                type="string"
                not-null="false"
                length="50"
                >
            <meta attribute="ColumnName">工地编号</meta>
            <meta attribute="ColumnComment">工地编号</meta>
        </property>

        <property
                name="devSn"
                column="DEV_SN"
                type="string"
                not-null="false"
                length="50"
                >
            <meta attribute="ColumnName">设备序号</meta>
            <meta attribute="ColumnComment">设备序号</meta>
        </property>

        <property
                name="checkStatus"
                column="CHECK_STATUS"
                type="string"
                not-null="false"
                length="10"
                >
            <meta attribute="ColumnName">考勤状态（上班、下班）</meta>
            <meta attribute="ColumnComment">考勤状态（上班、下班）</meta>
        </property>

        <property
                name="verifyCode"
                column="VERIFY_CODE"
                type="string"
                not-null="false"
                length="10"
                >
            <meta attribute="ColumnName">识别标识（指纹、卡、指纹+面部）</meta>
            <meta attribute="ColumnComment">识别标识（指纹、卡、指纹+面部）</meta>
        </property>

        <property
                name="longitude"
                column="LONGITUDE"
                type="java.lang.Double"
                not-null="false"
                length="18"
                >
            <meta attribute="ColumnName">经度</meta>
            <meta attribute="ColumnComment">经度</meta>
        </property>

        <property
                name="latitude"
                column="LATITUDE"
                type="java.lang.Double"
                not-null="false"
                length="18"
                >
            <meta attribute="ColumnName">纬度</meta>
            <meta attribute="ColumnComment">纬度</meta>
        </property>

        <property
                name="positionTime"
                column="POSITION_TIME"
                type="timestamp"
                not-null="false"
                length="19"
                >
            <meta attribute="ColumnName">定位时间</meta>
            <meta attribute="ColumnComment">定位时间</meta>
        </property>

        <property
                name="guid"
                column="GUID"
                type="string"
                not-null="false"
                length="50"
                >
            <meta attribute="ColumnName">GUID</meta>
            <meta attribute="ColumnComment">GUID</meta>
        </property>

        <property
                name="photoFile"
                column="PHOTO_FILE"
                type="string"
                not-null="false"
                length="100"
                >
            <meta attribute="ColumnName">照片</meta>
            <meta attribute="ColumnComment">照片</meta>
        </property>


        <property
                name="createTime"
                column="CREATE_TIME"
                type="timestamp"
                not-null="false"
                length="19"
                >
            <meta attribute="ColumnName">创建时间</meta>
            <meta attribute="ColumnComment">创建时间</meta>
        </property>

        <property
                name="updateTime"
                column="UPDATE_TIME"
                type="timestamp"
                not-null="false"
                length="19"
                >
            <meta attribute="ColumnName">更新时间</meta>
            <meta attribute="ColumnComment">更新时间</meta>
        </property>

        <property
                name="updateUser"
                column="UPDATE_USER"
                type="string"
                not-null="false"
                length="100"
                >
            <meta attribute="ColumnName">更新人</meta>
            <meta attribute="ColumnComment">更新人</meta>
        </property>

        <property
                name="createUser"
                column="CREATE_USER"
                type="string"
                not-null="false"
                length="100"
                >
            <meta attribute="ColumnName">创建人</meta>
            <meta attribute="ColumnComment">创建人</meta>
        </property>

        <many-to-one
                name="manager"
                column="MANAGER_ID"
                class="com.justonetech.biz.domain.CheckManager"
                not-null="false"
                >
        </many-to-one>
        <many-to-one
                name="dev"
                column="DEV_ID"
                class="com.justonetech.biz.domain.CheckDeviceInfo"
                not-null="false"
                >
        </many-to-one>

    </class>
    <query name="checkinfo_all_data_query">
        <![CDATA[FROM CheckInfo]]>
    </query>
</hibernate-mapping>