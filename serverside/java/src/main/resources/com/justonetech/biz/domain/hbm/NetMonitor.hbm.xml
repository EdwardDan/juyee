<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
	<class
		name="NetMonitor"
		table="NET_MONITOR"
	>
		
		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">false</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">项目报监</meta>
        <meta attribute="tableComment">项目报监</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">net</meta>

		<id
			name="id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
		</id>

		<property
			name="netCode"
			column="NET_CODE"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">网上受理编号</meta>
            <meta attribute="ColumnComment">网上受理编号</meta>
        </property>

        <property
                name="bdh"
                column="BDH"
                type="string"
                not-null="false"
                length="50"
                >
            <meta attribute="ColumnName">标段编号</meta>
            <meta attribute="ColumnComment">标段编号</meta>
        </property>

		<property
			name="netDate"
			column="NET_DATE"
			type="date"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">网上提交日期</meta>
            <meta attribute="ColumnComment">网上提交日期</meta>
        </property>


		<property
			name="createTime"
			column="CREATE_TIME"
			type="timestamp"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">创建时间</meta>
            <meta attribute="ColumnComment">创建时间</meta>
        </property>

		<property
			name="createUser"
			column="CREATE_USER"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">创建用户名</meta>
            <meta attribute="ColumnComment">创建用户名</meta>
        </property>

		<property
			name="updateTime"
			column="UPDATE_TIME"
			type="timestamp"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">更新时间</meta>
            <meta attribute="ColumnComment">更新时间</meta>
        </property>

		<property
			name="updateUser"
			column="UPDATE_USER"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">更新用户名</meta>
            <meta attribute="ColumnComment">更新用户名</meta>
        </property>

		<many-to-one
			name="acceptStatus"
			column="ACCEPT_STATUS_ID"
			class="NetAcceptStatus"
			not-null="false"
		>
		</many-to-one>

        <many-to-one
                name="apply"
                column="APPLY_ID"
                class="NetApply"
                not-null="false"
                >
        </many-to-one>

	</class>	
    <query name="netmonitor_all_data_query">
        <![CDATA[FROM NetMonitor]]>
    </query>
</hibernate-mapping>