<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
	<class
		name="WebProjectInfo"
		table="WEB_PROJECT_INFO"
	>
		
		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">false</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">项目信息</meta>
        <meta attribute="tableComment">项目信息</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">web</meta>

		<id
			name="id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
		</id>

		<property
			name="bjbh"
			column="BJBH"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">报建编号</meta>
            <meta attribute="ColumnComment">报建编号</meta>
        </property>

		<property
			name="project＿name"
			column="PROJECT＿NAME"
			type="string"
			not-null="false"
			length="500"
        >
            <meta attribute="ColumnName">项目名称</meta>
            <meta attribute="ColumnComment">项目名称</meta>
        </property>

		<property
			name="bjDate"
			column="BJ_DATE"
			type="date"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">报建日期</meta>
            <meta attribute="ColumnComment">报建日期</meta>
        </property>

		<property
			name="projectCategory"
			column="PROJECT_CATEGORY"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">项目分类</meta>
            <meta attribute="ColumnComment">项目分类</meta>
        </property>

		<property
			name="sgPermit"
			column="SG_PERMIT"
			type="string"
			not-null="false"
			length="500"
        >
            <meta attribute="ColumnName">施工许可</meta>
            <meta attribute="ColumnComment">施工许可</meta>
        </property>

		<property
			name="sjUnit"
			column="SJ_UNIT"
			type="string"
			not-null="false"
			length="500"
        >
            <meta attribute="ColumnName">建设单位</meta>
            <meta attribute="ColumnComment">建设单位</meta>
        </property>

		<property
			name="sjAddress"
			column="SJ_ADDRESS"
			type="string"
			not-null="false"
			length="1000"
        >
            <meta attribute="ColumnName">建设地点</meta>
            <meta attribute="ColumnComment">建设地点</meta>
        </property>

		<property
			name="totalMoney"
			column="TOTAL_MONEY"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">总投资</meta>
            <meta attribute="ColumnComment">总投资</meta>
        </property>

		<property
			name="totalArea"
			column="TOTAL_AREA"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">建设规模（面积）</meta>
            <meta attribute="ColumnComment">建设规模（面积）</meta>
        </property>

	

		<set name="webSgPermits" inverse="true">
			<key column="PROJECT_INFO_ID"/>
			<one-to-many class="WebSgPermit"/>
		</set>

		<set name="webContractBacks" inverse="true">
			<key column="PROJECT_INFO_ID"/>
			<one-to-many class="WebContractBack"/>
		</set>

	</class>	
    <query name="webprojectinfo_all_data_query">
        <![CDATA[FROM WebProjectInfo]]>
    </query>
</hibernate-mapping>