<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
	<class
		name="ShareAccidentSwry"
		table="SHARE_ACCIDENT_SWRY"
	>
		
		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">false</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">事故伤亡人员</meta>
        <meta attribute="tableComment">事故伤亡人员</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">share</meta>

		<id
			name="id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
		</id>

		<property
			name="accidentId"
			column="ACCIDENT_ID"
			type="java.lang.Long"
			not-null="false"
			length="10"
        >
            <meta attribute="ColumnName">事故标识</meta>
            <meta attribute="ColumnComment">事故标识</meta>
        </property>

		<property
			name="swryName"
			column="SWRY_NAME"
			type="string"
			not-null="false"
			length="30"
        >
            <meta attribute="ColumnName">姓名</meta>
            <meta attribute="ColumnComment">姓名</meta>
        </property>

		<property
			name="swryCard"
			column="SWRY_CARD"
			type="string"
			not-null="false"
			length="30"
        >
            <meta attribute="ColumnName">身份证</meta>
            <meta attribute="ColumnComment">身份证</meta>
        </property>

		<property
			name="swrySex"
			column="SWRY_SEX"
			type="string"
			not-null="false"
			length="1"
        >
            <meta attribute="ColumnName">性别</meta>
            <meta attribute="ColumnComment">性别</meta>
        </property>

		<property
			name="swrySexDesc"
			column="SWRY_SEX_DESC"
			type="string"
			not-null="false"
			length="10"
        >
            <meta attribute="ColumnName">性别描述</meta>
            <meta attribute="ColumnComment">性别描述</meta>
        </property>

		<property
			name="swryAgeStage"
			column="SWRY_AGE_STAGE"
			type="java.lang.Long"
			not-null="false"
			length="10"
        >
            <meta attribute="ColumnName">年龄阶段</meta>
            <meta attribute="ColumnComment">年龄阶段</meta>
        </property>

		<property
			name="swryAgeStageDesc"
			column="SWRY_AGE_STAGE_DESC"
			type="string"
			not-null="false"
			length="20"
        >
            <meta attribute="ColumnName">年龄阶段描述</meta>
            <meta attribute="ColumnComment">年龄阶段描述</meta>
        </property>

		<property
			name="swryWorkClass"
			column="SWRY_WORK_CLASS"
			type="java.lang.Long"
			not-null="false"
			length="10"
        >
            <meta attribute="ColumnName">工种</meta>
            <meta attribute="ColumnComment">工种</meta>
        </property>

		<property
			name="swryWorkClassDesc"
			column="SWRY_WORK_CLASS_DESC"
			type="string"
			not-null="false"
			length="20"
        >
            <meta attribute="ColumnName">工种描述</meta>
            <meta attribute="ColumnComment">工种描述</meta>
        </property>

		<property
			name="swrySwqk"
			column="SWRY_SWQK"
			type="string"
			not-null="false"
			length="1"
        >
            <meta attribute="ColumnName">伤亡情况（死亡、伤亡）</meta>
            <meta attribute="ColumnComment">伤亡情况（死亡、伤亡）</meta>
        </property>

		<property
			name="swrySwqkDesc"
			column="SWRY_SWQK_DESC"
			type="string"
			not-null="false"
			length="20"
        >
            <meta attribute="ColumnName">伤亡情况描述</meta>
            <meta attribute="ColumnComment">伤亡情况描述</meta>
        </property>


	</class>	
    <query name="shareaccidentswry_all_data_query">
        <![CDATA[FROM ShareAccidentSwry]]>
    </query>
</hibernate-mapping>