<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
	<class
		name="JdStopOrder"
		table="JD_STOP_ORDER"
	>
		
		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">false</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">安全质量监督--整改/暂缓施工/停工单</meta>
        <meta attribute="tableComment">安全质量监督--整改/暂缓施工/停工单</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">jd</meta>

		<id
			name="id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
		</id>

		<property
			name="formCode"
			column="FORM_CODE"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">表单类别(整改/暂缓/停工)</meta>
            <meta attribute="ColumnComment">表单类别(整改/暂缓/停工)</meta>
        </property>

		<property
			name="jdTypeCode"
			column="JD_TYPE_CODE"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">监督类别编码</meta>
            <meta attribute="ColumnComment">监督类别编码</meta>
        </property>

        <property
                name="jdAttributeCode"
                column="JD_ATTRIBUTE_CODE"
                type="string"
                not-null="false"
                length="50"
                >
            <meta attribute="ColumnName">监督属性类别编码（质量/安全）</meta>
            <meta attribute="ColumnComment">监督属性类别编码（质量/安全）</meta>
        </property>

		<property
			name="recordId"
			column="RECORD_ID"
			type="java.lang.Long"
			not-null="false"
			length="10"
        >
            <meta attribute="ColumnName">监督id(首次/安全/质量/竣工)</meta>
            <meta attribute="ColumnComment">监督id(首次/安全/质量/竣工)</meta>
        </property>

        <property
                name="bjbh"
                column="BJBH"
                type="string"
                not-null="false"
                length="50"
                >
            <meta attribute="ColumnName">报建编号</meta>
            <meta attribute="ColumnComment">报建编号</meta>
        </property>

		<property
			name="year"
			column="YEAR"
			type="integer"
			not-null="false"
			length="4"
        >
            <meta attribute="ColumnName">年份</meta>
            <meta attribute="ColumnComment">年份</meta>
        </property>

		<property
			name="orderNo"
			column="ORDER_NO"
			type="string"
			not-null="false"
			length="4"
        >
            <meta attribute="ColumnName">序号</meta>
            <meta attribute="ColumnComment">序号</meta>
        </property>

		<property
			name="dutyDept"
			column="DUTY_DEPT"
			type="string"
			not-null="false"
			length="200"
        >
            <meta attribute="ColumnName">责任主体</meta>
            <meta attribute="ColumnComment">责任主体</meta>
        </property>

		<property
			name="projectName"
			column="PROJECT_NAME"
			type="string"
			not-null="false"
			length="200"
        >
            <meta attribute="ColumnName">工程名称(冗余)</meta>
            <meta attribute="ColumnComment">工程名称(冗余)</meta>
        </property>

		<property
			name="recheckDept"
			column="RECHECK_DEPT"
			type="string"
			not-null="false"
			length="200"
        >
            <meta attribute="ColumnName">复查单位</meta>
            <meta attribute="ColumnComment">复查单位</meta>
        </property>

		<property
			name="recheckOther"
			column="RECHECK_OTHER"
			type="string"
			not-null="false"
			length="200"
        >
            <meta attribute="ColumnName">复查单位其他</meta>
            <meta attribute="ColumnComment">复查单位其他</meta>
        </property>

		<property
			name="reformArea"
			column="REFORM_AREA"
			type="string"
			not-null="false"
			length="200"
        >
            <meta attribute="ColumnName">整改范围</meta>
            <meta attribute="ColumnComment">整改范围</meta>
        </property>

		<property
			name="reformBeginDate"
			column="REFORM_BEGIN_DATE"
			type="timestamp"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">整改起始时间</meta>
            <meta attribute="ColumnComment">整改起始时间</meta>
        </property>

		<property
			name="reformLimit"
			column="REFORM_LIMIT"
			type="date"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">整改期限</meta>
            <meta attribute="ColumnComment">整改期限</meta>
        </property>

		<property
			name="signDept"
			column="SIGN_DEPT"
			type="string"
			not-null="false"
			length="200"
        >
            <meta attribute="ColumnName">签发单位</meta>
            <meta attribute="ColumnComment">签发单位</meta>
        </property>

		<property
			name="signPerson"
			column="SIGN_PERSON"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">签发人</meta>
            <meta attribute="ColumnComment">签发人</meta>
        </property>

		<property
			name="signDate"
			column="SIGN_DATE"
			type="date"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">签发日期</meta>
            <meta attribute="ColumnComment">签发日期</meta>
        </property>

		<property
			name="description"
			column="DESCRIPTION"
			type="string"
			not-null="false"
        >
            <meta attribute="ColumnName">备注</meta>
            <meta attribute="ColumnComment">备注</meta>
        </property>

		<property
			name="status"
			column="STATUS"
			type="integer"
			not-null="false"
			length="2"
        >
            <meta attribute="ColumnName">状态</meta>
            <meta attribute="ColumnComment">状态</meta>
        </property>

		<property
			name="createTime"
			column="CREATE_TIME"
			type="timestamp"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">创建时间</meta>
            <meta attribute="ColumnComment">创建时间</meta>
        </property>

		<property
			name="createUser"
			column="CREATE_USER"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">创建用户名</meta>
            <meta attribute="ColumnComment">创建用户名</meta>
        </property>

		<property
			name="updateTime"
			column="UPDATE_TIME"
			type="timestamp"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">更新时间</meta>
            <meta attribute="ColumnComment">更新时间</meta>
        </property>

		<property
			name="updateUser"
			column="UPDATE_USER"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">更新用户名</meta>
            <meta attribute="ColumnComment">更新用户名</meta>
        </property>

        <many-to-one
                name="document"
                column="DOCUMENT_ID"
                class="DocDocument"
                not-null="false"
                >
        </many-to-one>

	

		<set name="jdRecoverForms" inverse="true">
			<key column="STOP_FORM_ID"/>
			<one-to-many class="JdRecoverForm"/>
		</set>

        <set name="jdReformReplies" inverse="true">
            <key column="STOP_FORM_ID"/>
            <one-to-many class="JdReformReply"/>
        </set>

		<set name="jdStopOrderQuestions" inverse="true">
			<key column="REFORM_ORDER_ID"/>
			<one-to-many class="JdStopOrderQuestion"/>
		</set>

	</class>	
    <query name="jdstoporder_all_data_query">
        <![CDATA[FROM JdStopOrder]]>
    </query>
</hibernate-mapping>