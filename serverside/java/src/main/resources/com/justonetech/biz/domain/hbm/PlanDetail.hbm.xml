<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
	<class
		name="PlanDetail"
		table="PLAN_DETAIL"
	>
		
		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">false</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">计划明细</meta>
        <meta attribute="tableComment">计划明细</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">plan</meta>

		<id
			name="id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
		</id>

		<property
			name="jdTaskCode"
			column="JD_TASK_CODE"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">监督任务书编号</meta>
            <meta attribute="ColumnComment">监督任务书编号</meta>
        </property>

		<property
			name="bjbh"
			column="BJBH"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">报建编号</meta>
            <meta attribute="ColumnComment">报建编号</meta>
        </property>

		<property
			name="bdh"
			column="BDH"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">标段号</meta>
            <meta attribute="ColumnComment">标段号</meta>
        </property>

		<property
			name="buildUnitName"
			column="BUILD_UNIT_NAME"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">建设单位</meta>
            <meta attribute="ColumnComment">建设单位</meta>
        </property>

		<property
			name="sgUnitName"
			column="SG_UNIT_NAME"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">施工单位</meta>
            <meta attribute="ColumnComment">施工单位</meta>
        </property>

		<property
			name="gdName"
			column="GD_NAME"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">工地名称</meta>
            <meta attribute="ColumnComment">工地名称</meta>
        </property>

		<property
			name="planEndTime"
			column="PLAN_END_TIME"
			type="timestamp"
			not-null="false"
			length="16"
        >
            <meta attribute="ColumnName">计划完成日期</meta>
            <meta attribute="ColumnComment">计划完成日期</meta>
        </property>

		<property
			name="planActTime"
			column="PLAN_ACT_TIME"
			type="timestamp"
			not-null="false"
			length="16"
        >
            <meta attribute="ColumnName">实际完成日期</meta>
            <meta attribute="ColumnComment">实际完成日期</meta>
        </property>

        <property
                name="selectCause"
                column="SELECT_CAUSE"
                type="string"
                not-null="false"
                length="100"
                >
            <meta attribute="ColumnName">选择原因</meta>
            <meta attribute="ColumnComment">选择原因</meta>
        </property>
        <property
                name="selectCauseOther"
                column="SELECT_CAUSE_OTHER"
                type="string"
                not-null="false"
                length="100"
                >
            <meta attribute="ColumnName">选择原因_其他</meta>
            <meta attribute="ColumnComment">选择原因_其他</meta>
        </property>

		<property
			name="createTime"
			column="CREATE_TIME"
			type="timestamp"
			not-null="false"
			length="16"
        >
            <meta attribute="ColumnName">创建时间</meta>
            <meta attribute="ColumnComment">创建时间</meta>
        </property>

		<property
			name="updateTime"
			column="UPDATE_TIME"
			type="timestamp"
			not-null="false"
			length="16"
        >
            <meta attribute="ColumnName">更新时间</meta>
            <meta attribute="ColumnComment">更新时间</meta>
        </property>

		<property
			name="createUser"
			column="CREATE_USER"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">创建人</meta>
            <meta attribute="ColumnComment">创建人</meta>
        </property>

		<property
			name="updateUser"
			column="UPDATE_USER"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">更新人</meta>
            <meta attribute="ColumnComment">更新人</meta>
        </property>

		<many-to-one
			name="group"
			column="GROUP_ID"
			class="PollingWorkGroup"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="plan"
			column="PLAN_ID"
			class="PlanMainInfo"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="status"
			column="STATUS"
			class="com.justonetech.system.domain.SysCodeDetail"
			not-null="false"
		>
		</many-to-one>
        <set name="planDetailUsers" inverse="true" >
            <key column="PLAN_DETAIL_ID"/>
            <one-to-many class="PlanDetailUser"/>
        </set>
	</class>	
    <query name="plandetail_all_data_query">
        <![CDATA[FROM PlanDetail]]>
    </query>
</hibernate-mapping>