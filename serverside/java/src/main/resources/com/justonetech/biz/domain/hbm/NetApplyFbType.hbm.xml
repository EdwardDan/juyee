<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
	<class
		name="NetApplyFbType"
		table="NET_APPLY_FB_TYPE"
	>
		
		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">false</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">项目报建_发包类型</meta>
        <meta attribute="tableComment">项目报建_发包类型</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">net</meta>

		<id
			name="id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
		</id>

		<property
			name="fbTypeDesc"
			column="FB_TYPE_DESC"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">发包类型中文描述</meta>
            <meta attribute="ColumnComment">发包类型中文描述</meta>
        </property>

		<property
			name="isBid"
			column="IS_BID"
			type="java.lang.Boolean"
			not-null="false"
			length="1"
        >
            <meta attribute="ColumnName">是否招标</meta>
            <meta attribute="ColumnComment">是否招标</meta>
        </property>

		<many-to-one
			name="apply"
			column="APPLY_ID"
			class="NetApply"
			not-null="false"
		>
		</many-to-one>

        <many-to-one
                name="fbTypeId"
                column="FB_TYPE_ID"
                class="com.justonetech.system.domain.SysCodeDetail"
                not-null="false"
                >
        </many-to-one>

	</class>	
    <query name="netapplyfbtype_all_data_query">
        <![CDATA[FROM NetApplyFbType]]>
    </query>
</hibernate-mapping>