<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
	<class
		name="ProjectMajor"
		table="PROJECT_MAJOR"
	>
		
		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">false</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">项目重大工程</meta>
        <meta attribute="tableComment">项目重大工程</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">project</meta>

		<id
			name="id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
		</id>

		<property
			name="bjbh"
			column="BJBH"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">报建编号</meta>
            <meta attribute="ColumnComment">报建编号</meta>
        </property>

		<property
			name="projectName"
			column="PROJECT_NAME"
			type="string"
			not-null="false"
			length="200"
        >
            <meta attribute="ColumnName">项目名称</meta>
            <meta attribute="ColumnComment">项目名称</meta>
        </property>

		<property
			name="projectAdd"
			column="PROJECT_ADD"
			type="string"
			not-null="false"
			length="200"
        >
            <meta attribute="ColumnName">项目地址</meta>
            <meta attribute="ColumnComment">项目地址</meta>
        </property>

		<property
			name="jsUnitName"
			column="JS_UNIT_NAME"
			type="string"
			not-null="false"
			length="200"
        >
            <meta attribute="ColumnName">建设单位</meta>
            <meta attribute="ColumnComment">建设单位</meta>
        </property>

		<property
			name="projectTypeDesc"
			column="PROJECT_TYPE_DESC"
			type="string"
			not-null="false"
			length="200"
        >
            <meta attribute="ColumnName">项目类型中文描述</meta>
            <meta attribute="ColumnComment">项目类型中文描述</meta>
        </property>

		<property
			name="scheduleDesc"
			column="SCHEDULE_DESC"
			type="string"
			not-null="false"
			length="200"
        >
            <meta attribute="ColumnName">当前进度中文描述</meta>
            <meta attribute="ColumnComment">当前进度中文描述</meta>
        </property>

        <property
                name="posX"
                column="POS_X"
                type="java.lang.Double"
                not-null="false"
                length="15"
                >
            <meta attribute="ColumnName">坐标X</meta>
            <meta attribute="ColumnComment">坐标X</meta>
        </property>

        <property
                name="posY"
                column="POS_Y"
                type="java.lang.Double"
                not-null="false"
                length="15"
                >
            <meta attribute="ColumnName">坐标Y</meta>
            <meta attribute="ColumnComment">坐标Y</meta>
        </property>

		<property
			name="createTime"
			column="CREATE_TIME"
			type="timestamp"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">创建时间</meta>
            <meta attribute="ColumnComment">创建时间</meta>
        </property>

		<property
			name="createUser"
			column="CREATE_USER"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">创建用户名</meta>
            <meta attribute="ColumnComment">创建用户名</meta>
        </property>

		<property
			name="updateTime"
			column="UPDATE_TIME"
			type="timestamp"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">更新时间</meta>
            <meta attribute="ColumnComment">更新时间</meta>
        </property>

		<property
			name="updateUser"
			column="UPDATE_USER"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">更新用户名</meta>
            <meta attribute="ColumnComment">更新用户名</meta>
        </property>

		<many-to-one
			name="schedule"
			column="SCHEDULE_ID"
			class="com.justonetech.system.domain.SysCodeDetail"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="projectType"
			column="PROJECT_TYPE_ID"
			class="com.justonetech.system.domain.SysCodeDetail"
			not-null="false"
		>
		</many-to-one>

	</class>	
    <query name="projectmajor_all_data_query">
        <![CDATA[FROM ProjectMajor]]>
    </query>
</hibernate-mapping>