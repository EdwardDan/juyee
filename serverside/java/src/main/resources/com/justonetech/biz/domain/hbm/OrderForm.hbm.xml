<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.biz.domain">
	<class
		name="OrderForm"
		table="ORDER_FORM"
	>
		
		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">false</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">监督预约</meta>
        <meta attribute="tableComment">监督预约</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">order</meta>

		<id
			name="id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
		</id>

		<property
			name="orderCode"
			column="ORDER_CODE"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">预约单号</meta>
            <meta attribute="ColumnComment">预约单号</meta>
        </property>

		<property
			name="jdTaskCode"
			column="JD_TASK_CODE"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">监督任务书编号</meta>
            <meta attribute="ColumnComment">监督任务书编号</meta>
        </property>

		<property
			name="buildUnitName"
			column="BUILD_UNIT_NAME"
			type="string"
			not-null="false"
			length="200"
        >
            <meta attribute="ColumnName">建设单位</meta>
            <meta attribute="ColumnComment">建设单位</meta>
        </property>

		<property
			name="sgUnitName"
			column="SG_UNIT_NAME"
			type="string"
			not-null="false"
			length="200"
        >
            <meta attribute="ColumnName">施工单位</meta>
            <meta attribute="ColumnComment">施工单位</meta>
        </property>

		<property
			name="projectName"
			column="PROJECT_NAME"
			type="string"
			not-null="false"
			length="200"
        >
            <meta attribute="ColumnName">工程名称</meta>
            <meta attribute="ColumnComment">工程名称</meta>
        </property>

		<property
			name="buildAddress"
			column="BUILD_ADDRESS"
			type="string"
			not-null="false"
			length="200"
        >
            <meta attribute="ColumnName">工程地点</meta>
            <meta attribute="ColumnComment">工程地点</meta>
        </property>

		<property
			name="buildArea"
			column="BUILD_AREA"
			type="java.lang.Double"
			not-null="false"
			length="15"
        >
            <meta attribute="ColumnName">建筑面积</meta>
            <meta attribute="ColumnComment">建筑面积</meta>
        </property>

		<property
			name="structure"
			column="STRUCTURE"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">结构层次</meta>
            <meta attribute="ColumnComment">结构层次</meta>
        </property>

		<property
			name="sgPosition"
			column="SG_POSITION"
			type="string"
			not-null="false"
			length="200"
        >
            <meta attribute="ColumnName">隐蔽工程部位</meta>
            <meta attribute="ColumnComment">隐蔽工程部位</meta>
        </property>

		<property
			name="buildLegalPerson"
			column="BUILD_LEGAL_PERSON"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">建设单位法人代表</meta>
            <meta attribute="ColumnComment">建设单位法人代表</meta>
        </property>

		<property
			name="buildLegalPhone"
			column="BUILD_LEGAL_PHONE"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">联系电话</meta>
            <meta attribute="ColumnComment">联系电话</meta>
        </property>

		<property
			name="projectManager"
			column="PROJECT_MANAGER"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">项目经理</meta>
            <meta attribute="ColumnComment">项目经理</meta>
        </property>

		<property
			name="planSgDatatime"
			column="PLAN_SG_DATATIME"
			type="timestamp"
			not-null="false"
			length="16"
        >
            <meta attribute="ColumnName">计划隐蔽日期</meta>
            <meta attribute="ColumnComment">计划隐蔽日期</meta>
        </property>

		<property
			name="orderDatetime"
			column="ORDER_DATETIME"
			type="timestamp"
			not-null="false"
			length="16"
        >
            <meta attribute="ColumnName">预约日期</meta>
            <meta attribute="ColumnComment">预约日期</meta>
        </property>

		<property
			name="inputTime"
			column="INPUT_TIME"
			type="timestamp"
			not-null="false"
			length="16"
        >
            <meta attribute="ColumnName">填表日期</meta>
            <meta attribute="ColumnComment">填表日期</meta>
        </property>

		<property
			name="linkMan"
			column="LINK_MAN"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">联系人</meta>
            <meta attribute="ColumnComment">联系人</meta>
        </property>

        <property
                name="linkManPhone"
                column="LINK_MAN_PHONE"
                type="string"
                not-null="false"
                length="50"
                >
            <meta attribute="ColumnName">联系电话</meta>
            <meta attribute="ColumnComment">联系电话</meta>
        </property>

		<property
			name="createTime"
			column="CREATE_TIME"
			type="timestamp"
			not-null="false"
			length="16"
        >
            <meta attribute="ColumnName">创建时间</meta>
            <meta attribute="ColumnComment">创建时间</meta>
        </property>

		<property
			name="updateTime"
			column="UPDATE_TIME"
			type="timestamp"
			not-null="false"
			length="16"
        >
            <meta attribute="ColumnName">更新时间</meta>
            <meta attribute="ColumnComment">更新时间</meta>
        </property>

		<property
			name="updateUser"
			column="UPDATE_USER"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">更新人</meta>
            <meta attribute="ColumnComment">更新人</meta>
        </property>

		<property
			name="createUser"
			column="CREATE_USER"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">创建人</meta>
            <meta attribute="ColumnComment">创建人</meta>
        </property>

		<many-to-one
			name="orderStatus"
			column="ORDER_STATUS"
			class="com.justonetech.system.domain.SysCodeDetail"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="formDoc"
			column="FORM_DOC_ID"
			class="DocDocument"
			not-null="false"
		>
		</many-to-one>
	

		<set name="orderFormDeploies" inverse="true">
			<key column="ORDER_FORM_ID"/>
			<one-to-many class="OrderFormDeploy"/>
		</set>

	</class>	
    <query name="orderform_all_data_query">
        <![CDATA[FROM OrderForm]]>
    </query>
</hibernate-mapping>